	<S id="S001">
		<AUDIO start="0.6" end="2.77"/>
		<FORM kindOf="phono">kɯɕɯŋgɯ kɯ-wxti ra kɯ pjɯ-fɕɤt-nɯ tɕe</FORM>
	</S>
	<S id="S002">
		<AUDIO start="3.34" end="8.11"/>
		<FORM kindOf="phono">nɤki pɯwɯ nɯnɯ ɯʑo mbɤr ri, ɯ-sɯm wuma ʑo ŋɤn tu-ti-nɯ ɲɯ-ŋu</FORM>
	</S>
	<S id="S003">
		<AUDIO start="8.67" end="14.09"/>
		<FORM kindOf="phono">ɯ-sɯm ɲɯ-ŋɤn ma, nɤkinɯ, βɣɤtu tu-ɣɯɕkat tɕe, nɯnɯ wuma ʑo rʑi ri,</FORM>
	</S>
	<S id="S004">
		<AUDIO start="14.51" end="15.63"/>
		<FORM kindOf="phono">ɯ-lɯm ɲɯ-xtɕi tɕe,</FORM>
	</S>
	<S id="S005">
		<AUDIO start="16.07" end="18.57"/>
		<FORM kindOf="phono">tɕe ɯ-ʁjɯβ lu-ru tɕe, tɕendɤre nɤki</FORM>
	</S>
	<S id="S006">
		<AUDIO start="18.88" end="21.79"/>
		<FORM kindOf="phono">"a ki a-fkɯr ɯ-tɯ-xtɕi ki a-ɕkat ɯ-tɯ-xtɕi ki</FORM>
	</S>
	<S id="S007">
		<AUDIO start="22.18" end="23.83"/>
		<FORM kindOf="phono">nɯnɯ ɲɯ-mbat ɕti ɲɯ-sɯsɤm tɕe,</FORM>
	</S>
	<S id="S008">
		<AUDIO start="23.83" end="25.71"/>
		<FORM kindOf="phono">stɤrɟɯɣ kɯ ju-ɕe ɲɯ-cha</FORM>
	</S>
	<S id="S009">
		<AUDIO start="26.06" end="27.43"/>
		<FORM kindOf="phono">tɕe ʑaʑa mɯ́j-nɯna</FORM>
	</S>
	<S id="S010">
		<AUDIO start="27.43" end="31.31"/>
		<FORM kindOf="phono">χtsɯmpapɯ, χtsɯm kɯfse tu-ɣɯɕkat tɕe</FORM>
	</S>
	<S id="S011">
		<AUDIO start="31.68" end="34.24"/>
		<FORM kindOf="phono">nɯnɯ kɯ-ʑɯ-ʑo ɲɯ-ɕti ri, ɯ-lɯm ɲɯ-wxti tɕe,</FORM>
	</S>
	<S id="S012">
		<AUDIO start="34.81" end="39.7"/>
		<FORM kindOf="phono">ɯ-ʁjɯβ lu-ru tɕe, tɕendɤre aʑo kɯ-xtɕɯ-xtɕi kɯsthɯci kɯ-wxti (kɯrʑi) tu-ɣɯɕkat-a nɤ,</FORM>
	</S>
	<S id="S013">
		<AUDIO start="40.4" end="43.88"/>
		<FORM kindOf="phono">ɲɯ-sɯsɤm, tɕe nɯ "mɯ́j-cha-a" ɲɯ-sɯsɤm tɕe</FORM>
	</S>
	<S id="S014">
		<AUDIO start="44.27" end="49.35"/>
		<FORM kindOf="phono">kɤ-ɕɯŋke mɯ-tu-khɯ tɕe lu-nɯ-rŋgɯ ŋu ra tu-ti-nɯ pɯ-ŋgrɤl</FORM>
	</S>
