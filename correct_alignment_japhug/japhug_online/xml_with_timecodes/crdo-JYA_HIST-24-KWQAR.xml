<TEXT id="crdo-JYA_HIST-24-KWQAR" xml:lang="jya">
	<HEADER>
		<TITLE>crdo-JYA_HIST-24-KWQAR</TITLE>
		<SOUNDFILE href="hist-24-kWqar.wav"/>
	</HEADER>
	<S id="S001">
		<AUDIO start="0.09" end="1.82"/>
		<FORM kindOf="phono">kɯqar kɤti ci tu tɕe</FORM>
	</S>
	<S id="S002">
		<AUDIO start="1.82" end="4.46"/>
		<FORM kindOf="phono">kɯqar nɯnɯ tu-kɯ-ɕɯngo ɲɯŋu, ko?</FORM>
	</S>
	<S id="S003">
		<AUDIO start="5.07" end="9.09"/>
		<FORM kindOf="phono">tɕe tɯci kɯqar ɣɤʑu, si kɯqar ɣɤʑu, sɤtɕha kɯqar ɣɤʑu</FORM>
	</S>
	<S id="S004">
		<AUDIO start="9.74" end="12.64"/>
		<FORM kindOf="phono">tɕe sɤtɕha kɯqar nɯnɯ tɕhi tu-fse ŋu mɤxsi ma</FORM>
	</S>
	<S id="S005">
		<AUDIO start="13.1" end="17.42"/>
		<FORM kindOf="phono">tɯci kɯqar nɯ tɕe, tɯjaʁ pjɯ́wɣnɯχtɕi kɯfse nɯra tɕe, tɯjaʁ tuɕɯmŋɤm</FORM>
	</S>
	<S id="S006">
		<AUDIO start="17.91" end="23.33"/>
		<FORM kindOf="phono">nɯra tɯku ɯtaʁ tɯci túwɣlɤt tɕe tɯku tuɕɯmŋɤm ɲɯŋgrɤl</FORM>
	</S>
	<S id="S007">
		<AUDIO start="23.33" end="26.26"/>
		<FORM kindOf="phono">si kɯqar ɣɤʑu tɕe si nɯ́wɣphɯt tɕe tɕe</FORM>
	</S>
	<S id="S008">
		<AUDIO start="26.26" end="28.21"/>
		<FORM kindOf="phono">tɕe lúwɣphɯt tɕe tɕe li nɯnɯ </FORM>
	</S>
	<S id="S009">
		<AUDIO start="28.21" end="32.44"/>
		<FORM kindOf="phono">ɯsɤphɯt nɯ ɯsŋi nɯ amɤpɯpe tɕe tɕe tu-kɯ-ɕɯ-ngo ɲɯ-ŋgrɤl</FORM>
	</S>
	<S id="S010">
		<AUDIO start="32.99" end="35.8"/>
		<FORM kindOf="phono">ɯkɯphɯt nɯ ɣɯ mɯ-pɯ-nɯ-ʁdɯɣ kɯnɤ</FORM>
	</S>
	<S id="S011">
		<AUDIO start="36.26" end="41.19"/>
		<FORM kindOf="phono">pɯkɯɣɯsɯphɯt nɯ tɕe kɯmaʁ ci kɯ "wo, tɤnɯna je" atɤti </FORM>
	</S>
	<S id="S012">
		<AUDIO start="41.85" end="45.51"/>
		<FORM kindOf="phono">tɕe nɯnɯ li nɤki jɤ-kɯ-ɤkhu nɯ tu-ɕɯngɤm ɲɯŋgrɤl</FORM>
	</S>
	<S id="S013">
		<AUDIO start="46.02" end="47.67"/>
		<FORM kindOf="phono">tɕe nɯnɯ kɯqar tukɯti ŋu</FORM>
	</S>
	<S id="S014">
		<AUDIO start="48.31" end="49.89"/>
		<FORM kindOf="phono">tɕeri nɯra ŋu maʁ mɤxsi</FORM>
	</S>
</TEXT>