	<S id="S070">
		<AUDIO start="219.81" end="222.28"/>
		<FORM kindOf="phono">qajɯ ɲɯ-βze qhe, chɯ-wxti ɯ-tɯ-mbat ɲɯ-sɤre ʑo </FORM>
