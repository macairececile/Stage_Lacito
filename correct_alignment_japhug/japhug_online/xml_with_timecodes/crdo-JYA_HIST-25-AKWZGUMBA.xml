	<S id="S023">
		<AUDIO start="71.65" end="72.92"/>
		<FORM kindOf="phono">qandʐe ɯ-pɯ-fɕɤt-tɕi?</FORM>
	</S>
	<NOTE message="========" xmlns:lang="fr"/>
