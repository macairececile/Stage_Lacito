	</HEADER>
	<S id="S001">
		<AUDIO start="0.39" end="4.58"/>
		<FORM kindOf="phono">﻿thoŋraʁ nɯ kɯnɤ nɯnɯ qhe ɯ-taʁ</FORM>
	</S>
	<S id="S002">
		<AUDIO start="4.92" end="6.38"/>
		<FORM kindOf="phono">nɯ ɯ-ro ra naχtɕɯɣ ɕti</FORM>
	</S>
	<S id="S003">
		<AUDIO start="6.77" end="8.41"/>
		<FORM kindOf="phono">ɯ-spa cho ra naχtɕɯɣ ɕti ɕti,</FORM>
	</S>
	<S id="S004">
		<AUDIO start="8.87" end="11.11"/>
		<FORM kindOf="phono">tɕeri araʁ kɤ-tɕɤt tɤ-mda tɕe nɯnɯ,</FORM>
	</S>
	<S id="S005">
		<AUDIO start="11.47" end="14.05"/>
		<FORM kindOf="phono">chɤzwa nɯ lu-ta-nɯ, nɯ ɯ-taʁ tɯthɯ ɯ-ŋgɯ,</FORM>
	</S>
	<S id="S006">
		<AUDIO start="14.36" end="16.01"/>
		<FORM kindOf="phono">chɤzwa tu-rku-nɯ, nɯ ɯ-taʁ ri,</FORM>
	</S>
	<S id="S007">
		<AUDIO start="16.38" end="19.88"/>
		<FORM kindOf="phono">tongtong kɯ-fse ci, ɕom tongtong kɯ-tu ɣɤʑu,</FORM>
	</S>
	<S id="S008">
		<AUDIO start="19.88" end="22.14"/>
		<FORM kindOf="phono">si tongtong, zɯm kɯ-fse ɲɯ-ŋu wo,</FORM>
	</S>
	<S id="S009">
		<AUDIO start="22.36" end="24.01"/>
		<FORM kindOf="phono">tɕe nɯnɯ pjɯ-βʁum-nɯ ɲɯ-ŋu,</FORM>
	</S>
	<S id="S010">
		<AUDIO start="24.42" end="26.06"/>
		<FORM kindOf="phono">tɕe ɯ-pa ri qhajŋgɯ ɣɤʑu.</FORM>
	</S>
	<S id="S011">
		<AUDIO start="26.86" end="29.4"/>
		<FORM kindOf="phono">tɕe nɯ kɯ-fse ɕom qhajŋgɯ kɯ-fse lu-ta-nɯ tɕe tɕe,</FORM>
	</S>
	<S id="S012">
		<AUDIO start="29.69" end="30.93"/>
		<FORM kindOf="phono">sqhi ɯ-taʁ ri pjɯ-ɣi tɕe,</FORM>
	</S>
	<S id="S013">
		<AUDIO start="31.22" end="32.77"/>
		<FORM kindOf="phono">tɕe sqhi ɯ-phaʁ ntsi ri,</FORM>
	</S>
	<S id="S014">
		<AUDIO start="33.2" end="36.33"/>
		<FORM kindOf="phono">nɤki, cha ɣɯ ɯ-fkɯm nɯ ɲɯ-ɣɤntaβ-nɯ tɕe, nɯ ɯ-ŋgɯ ri pjɯ-lɤt-nɯ ɲɯ-ŋu.</FORM>
	</S>
	<S id="S015">
		<AUDIO start="37.27" end="38.9"/>
		<FORM kindOf="phono">tɕe nɯnɯ thoŋraʁ tu-kɯ-ti ɲɯ-ŋu</FORM>
	</S>
	<S id="S016">
		<AUDIO start="39.75" end="42.61"/>
		<FORM kindOf="phono">nɯ ɯ-spa ra ɲɯ-naχtɕɯɣ ɕti,</FORM>
	</S>
	<S id="S017">
		<AUDIO start="43.15" end="45.26"/>
		<FORM kindOf="phono">tɕe thoŋraʁ nɯ kɯ kɯ-mɯm tu-sɯpa-nɯ ŋu,</FORM>
	</S>
	<S id="S018">
		<AUDIO start="46.02" end="48.28"/>
		<FORM kindOf="phono">tɕeri tsuku tɕe, nɤki, </FORM>
	</S>
	<S id="S019">
		<AUDIO start="48.77" end="50.93"/>
		<FORM kindOf="phono">tɯŋguraʁ kɯ kɯ-rga ɣɤʑu-nɯ, </FORM>
	</S>
	<S id="S020">
		<AUDIO start="50.93" end="52.79"/>
		<FORM kindOf="phono">tsuku tɕe thoŋraʁ kɯ kɯ-rga ɣɤʑu-nɯ</FORM>
	</S>
	<S id="S021">
		<AUDIO start="53.14" end="54.16"/>
		<FORM kindOf="phono">ŋotɕu mɯm mɤxsi </FORM>
	</S>
	<S id="S022">
		<AUDIO start="55.18" end="57.52"/>
		<FORM kindOf="phono">cha ɯ-di kɯ-mnɤm ndɤ ɲɯ-naχtɕɯɣ.</FORM>
	</S>
</TEXT>