	<S id="S001">
		<AUDIO start="0.41" end="3.34"/>
		<FORM kindOf="phono">awa pɯxtɕi tɕe</FORM>
	</S>
	<S id="S002">
		<AUDIO start="3.34" end="6.43"/>
		<FORM kindOf="phono">tshɤt cho qaʑo kɤlɤt wuma ʑo pjɤrga</FORM>
	</S>
	<S id="S003">
		<AUDIO start="6.89" end="10.07"/>
		<FORM kindOf="phono">tɕeri ɯʑo nɯ li tɤpɤtso kɯqhi ci pjɤŋu tɕe,</FORM>
	</S>
	<S id="S004">
		<AUDIO start="10.54" end="15.39"/>
		<FORM kindOf="phono">tɕe tshɤt qaʑo kɯ-sɤqhɯqha aʁɤndɯndɤt ʑo kɯnɤru kɯɕe</FORM>
	</S>
	<S id="S005">
		<AUDIO start="15.76" end="20.99"/>
		<FORM kindOf="phono">kɯsɤmtshi kɯfse, kɯsɤmtshi tɕe iɕqha, ɯzda ra nɯjukɯtsɯm nɯ tɕe </FORM>
	</S>
	<S id="S006">
		<AUDIO start="20.99" end="24.54"/>
		<FORM kindOf="phono">kɯsɤmtshi tukɯti, jukɯmɤku nɯ </FORM>
	</S>
	<S id="S007">
		<AUDIO start="24.99" end="29.01"/>
		<FORM kindOf="phono">tɕe nɯnɯra kɯfse, tɕe nɯnɯ tshɤt qaʑo kɯnɤru kɯfse </FORM>
	</S>
	<S id="S008">
		<AUDIO start="29.01" end="33.1"/>
		<FORM kindOf="phono">nɯnɯ ra ɣɯ nɯ-ʁrɯ ɯtaʁ nɯ tɕe cɯpɤspoʁspoʁ pjɯβze</FORM>
	</S>
	<S id="S009">
		<AUDIO start="33.1" end="35.61"/>
		<FORM kindOf="phono">tɕe nɯ cɯpɤspoʁspoʁ nɯ ɣɯ ɯri ɲɯβze tɕe</FORM>
	</S>
	<S id="S010">
		<AUDIO start="36.0" end="37.58"/>
		<FORM kindOf="phono">ɯʁrɯ, ɲɯ-ɣɯri tɕe, </FORM>
	</S>
	<S id="S011">
		<AUDIO start="37.96" end="39.81"/>
		<FORM kindOf="phono">nɯra ɯʁrɯ ɯtaʁ ku-βraʁ</FORM>
	</S>
	<S id="S012">
		<AUDIO start="40.62" end="43.72"/>
		<FORM kindOf="phono">tɕendɤre nɯ ku-βraʁ tɕendɤre, tshɤt qaʑo nɯra </FORM>
	</S>
	<S id="S013">
		<AUDIO start="43.96" end="45.1"/>
		<FORM kindOf="phono">tɤ-nɤrɟɯrɟɯɣnɯ tɕe</FORM>
	</S>
	<S id="S014">
		<AUDIO start="45.1" end="49.61"/>
		<FORM kindOf="phono">tɕe nɯ-ʁrɯ nɯ ɲɯ-nɤrkhɯrkhɯβ tɕe tu-ɕɯmŋɤm tɕe, tɕe kúwɣzrɤʑi-nɯ pjɤŋu</FORM>
	</S>
	<S id="S015">
		<AUDIO start="50.11" end="52.84"/>
		<FORM kindOf="phono">tɕe nɯ ku-nɯmge tɕe nɯ tu-ste pjɤŋu</FORM>
