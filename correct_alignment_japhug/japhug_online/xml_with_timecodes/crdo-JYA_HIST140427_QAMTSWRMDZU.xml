<?xml version="1.0" ?>
<!DOCTYPE TEXT SYSTEM "https://cocoon.huma-num.fr/schemas/Archive.dtd">
<TEXT id="crdo-JYA_HIST140427_QAMTSWRMDZU" xml:lang="jya">
	<HEADER>
		<TITLE>crdo-JYA_HIST140427_QAMTSWRMDZU</TITLE>
		<SOUNDFILE href="hist140427_qamtsWrmdzu.wav"/>
	</HEADER>
	<S id="S001">
		<AUDIO start="1.3" end="4.31"/>
		<FORM kindOf="phono">qamtsɯrmdzu kɤ-ti ci tu tɕe, nɯnɯ tɤ-mdzu ŋu tɕe,</FORM>
	</S>
	<S id="S002">
		<AUDIO start="4.86" end="9.24"/>
		<FORM kindOf="phono">nɯnɯ si nɯ sɤre ma, ɯ-mdzu ɲɯ-kɯ-ɬoʁ nɯ, </FORM>
	</S>
	<S id="S003">
		<AUDIO start="10.04" end="12.05"/>
		<FORM kindOf="phono">nɤkinɯ jamar, sanlimi jamar rɲɟi.</FORM>
	</S>
	<S id="S004">
		<AUDIO start="12.33" end="16.82"/>
		<FORM kindOf="phono">tɕe nɯnɯ ɯ-jwaʁ nɯnɯ nɯ ɯ-mdzu ɯ-taʁ ri chɯ-ɤʑɯrja ŋu</FORM>
	</S>
	<S id="S005">
		<AUDIO start="17.68" end="20.78"/>
		<FORM kindOf="phono">tɕe nɯnɯ nɤkinɯ, ɯ-rqhu nɯra wuma ʑo ngɯt.</FORM>
	</S>
	<S id="S006">
		<AUDIO start="21.48" end="26.34"/>
		<FORM kindOf="phono">tɕeri nɯnɯ ɯ-jwaʁ ɯ-sɤɣ-ɬoʁ nɯ tɤ-mdzu nɯ ɣɯ ɯ-ku nɯ ʁo lɯski tɕe amtɕoʁ </FORM>
	</S>
	<S id="S007">
		<AUDIO start="26.91" end="28.63"/>
		<FORM kindOf="phono">ma koŋla ʑo tɤ-mdzu ŋu</FORM>
	</S>
	<S id="S008">
		<AUDIO start="29.42" end="31.4"/>
		<FORM kindOf="phono">tɕe ɯ-ru nɯra wumaʑo ldʑɯz tɕe, </FORM>
	</S>
	<S id="S009">
		<AUDIO start="31.98" end="33.53"/>
		<FORM kindOf="phono">ɯʑosti tu-ɬoʁ mɤ-ŋgrɤl.</FORM>
	</S>
	<S id="S010">
		<AUDIO start="33.7" end="36.63"/>
		<FORM kindOf="phono">ɕkrɤz ɯ-ŋgɯ tɕe aʁɤndɯndɤt tu-ɬoʁ ŋgrɤl.</FORM>
	</S>
	<S id="S011">
		<AUDIO start="37.25" end="38.99"/>
		<FORM kindOf="phono">tɕe nɯ qamtsɯrmdzu tu-kɯ-ti ŋu.</FORM>
	</S>
	<S id="S012">
		<AUDIO start="38.99" end="42.71"/>
		<FORM kindOf="phono">tɕe nɯ ma tɤ-me tɕe ɲɯ-phɯt-nɯ tɕe, /tɤ/ nɤkinɯ,</FORM>
	</S>
	<S id="S013">
		<AUDIO start="42.71" end="44.67"/>
		<FORM kindOf="phono">tɯji ɯ-rkɯ ra tɤɣur pjɯ-ta-nɯ tɕe,</FORM>
	</S>
	<S id="S014">
		<AUDIO start="45.02" end="47.96"/>
		<FORM kindOf="phono">fsapaʁ ɯ-mɤ-sɤ-xɕe tu-βzu-nɯ ŋgrɤl. (causative, preventive)</FORM>
	</S>
	<S id="S015">
		<AUDIO start="47.96" end="49.5"/>
		<FORM kindOf="phono">ri mɯ-mɤ-pɯ-dɤn tsa nɤ, </FORM>
	</S>
	<S id="S016">
		<AUDIO start="49.9" end="51.82"/>
		<FORM kindOf="phono">fsapaʁ ra kɯ rɤtɕaʁ-nɯ ɕti ma</FORM>
	</S>
	<S id="S017">
		<AUDIO start="52.32" end="54.67"/>
		<FORM kindOf="phono">ɲɯ-jpum tɕe tu-mbro mɤ-cha.</FORM>
	</S>
	<S id="S018">
		<AUDIO start="55.91" end="58.08"/>
		<FORM kindOf="phono">tɕe nɯ qamtsɯrmdzu ŋu.</FORM>
	</S>
	<S id="S019">
		<AUDIO start="58.4" end="65.46"/>
		<FORM kindOf="phono">tɕe qamtsɯrmdzu nɯnɯ, nɤkinɯ, ɯ-mdoʁ ra, ɯ-jwaʁ me, ɯ-mdzu me, kɯ-pɣi ci ŋu.</FORM>
	</S>
	<S id="S020">
		<AUDIO start="66.06" end="70.76"/>
		<FORM kindOf="phono">tɕe nɯnɯ tɤ-ɬoʁ tɕe, nɤkinɯ, ɯ-mat nɯnɯ staχpɯcɤβ kɯ-fse chɯ-βze ŋu.</FORM>
	</S>
	<S id="S021">
		<AUDIO start="71.52" end="75.1"/>
		<FORM kindOf="phono">tɕe staχpɯcɤβ, iɕqha wuŋgru ɣɯ ɯ-mat kɯ-fsɯ-fse ʑo ŋu.</FORM>
	</S>
	<S id="S022">
		<AUDIO start="75.45" end="77.07"/>
		<FORM kindOf="phono">tɕe nɯnɯ kɯ-fse chɯ-βze ŋu tɕe</FORM>
	</S>
	<S id="S023">
		<AUDIO start="77.26" end="81.58"/>
		<FORM kindOf="phono">tɕe ɯ-mdzu nɯ ɯ-taʁ ri, ɯ-jwaʁ nɯ kɯ-ɤrtɯm tsa lu-nɯgrɤl ŋu.</FORM>
	</S>
	<S id="S024">
		<AUDIO start="82.29" end="84.56"/>
		<FORM kindOf="phono">staχpɯ rɟɤskhi ɣɯ ɯ-jwaʁ wuma ʑo fse.</FORM>
	</S>
	<S id="S025">
		<AUDIO start="85.43" end="90.29"/>
		<FORM kindOf="phono">ma nɯnɯ tɕe, ɯ-mdzu nɯnɯ ɯ-ku nɯ tɤ-mdzu ŋu,</FORM>
	</S>
	<S id="S026">
		<AUDIO start="90.29" end="93.02"/>
		<FORM kindOf="phono">ɯ-ru nɯnɯra, ɯ-jwaʁ ɯ-sɤɣ-ndzoʁ ŋu.</FORM>
	</S>
	<S id="S027">
		<AUDIO start="93.92" end="95.6"/>
		<FORM kindOf="phono">tɕe nɯnɯ qamtsɯrmdzu rmi.</FORM>
	</S>
	<S id="S028">
		<AUDIO start="96.14" end="97.97"/>
		<FORM kindOf="phono">tɕe nɯ ɣɯ ɯ-rqhu nɯ chɯ́-wɣ-qaʁ tɕe,</FORM>
	</S>
	<S id="S029">
		<AUDIO start="98.28" end="100.9"/>
		<FORM kindOf="phono">tasa thɯ-kɤ-qaʁ ʑo fse tɕe, ɯ-rqhu nɯ ngɯt.</FORM>
	</S>
	<S id="S030">
		<AUDIO start="101.74" end="104.44"/>
		<FORM kindOf="phono">tɕe tshitsuku ɯ-sɤ-xtɕɤr ra tu-sɯpa-nɯ ŋgrɤl.</FORM>
	</S>
	<S id="S031">
		<AUDIO start="105.17" end="107.66"/>
		<FORM kindOf="phono">tɕe nɯnɯ qamtsɯrmdzu ŋu.</FORM>
	</S>
	<S id="S032">
		<AUDIO start="109.71" end="113.03"/>
		<FORM kindOf="phono">tshɤmdzu ɯ-pɯ-fɕat-a?</FORM>
	</S>
	<S id="S033">
		<AUDIO start="113.86" end="117.72"/>
		<FORM kindOf="phono">pɯ-fɕat-a low, ɯ-qa nɯ kɯ-qarŋe ŋu tɕe,</FORM>
	</S>
	<S id="S034">
		<AUDIO start="117.84" end="121.43"/>
		<FORM kindOf="phono">tshwi pjɯ́-wɣ-lɤt sna.</FORM>
	</S>
</TEXT>