	<S id="S001">
		<AUDIO start="1.04" end="3.57"/>
		<FORM kindOf="phono">ɴɢolo nɯ si ŋu</FORM>
	</S>
	<S id="S002">
		<AUDIO start="4.41" end="9.01"/>
		<FORM kindOf="phono">si ŋu tɕe nɯnɯ ɯ-mdzu rcanɯ wuma ʑo mtɕoʁ chondɤre χɕu,</FORM>
	</S>
	<S id="S003">
		<AUDIO start="9.84" end="13.14"/>
		<FORM kindOf="phono">tɕe ɯ-mdzu nɯ, ɯ-rtsɤɣ roŋri nɯ tɕu tɕe,</FORM>
	</S>
	<S id="S004">
		<AUDIO start="13.61" end="15.1"/>
		<FORM kindOf="phono">ɯ-mdzu (ɣɤʑu) tu tɕe,</FORM>
	</S>
	<S id="S005">
		<AUDIO start="15.4" end="17.41"/>
		<FORM kindOf="phono">tɯ-ldʑa nɯ ɯ-taʁ χsɯm ntsɯ ki kɯ-fse tu.</FORM>
	</S>
	<S id="S006">
		<AUDIO start="18.21" end="19.55"/>
		<FORM kindOf="phono">tɕe χsɯm tu tɕe,</FORM>
	</S>
	<S id="S007">
		<AUDIO start="20.06" end="22.98"/>
		<FORM kindOf="phono">nɯnɯ, nɤkinɯ, aʁɤndɯndɤt ju-sɤtse ŋu,</FORM>
	</S>
	<S id="S008">
		<AUDIO start="23.39" end="26.91"/>
		<FORM kindOf="phono">ɯ-ru ɯ-taʁ nɯra ɯ-mdzu kɯ-ndɯ-ndɯβ ʑo tu-oʑɯrja ŋu,</FORM>
	</S>
	<S id="S009">
		<AUDIO start="27.58" end="30.04"/>
		<FORM kindOf="phono">tɕe nɯnɯ ɴɢolo ŋu tɕe, ɯ-tɯ-mbro nɯ,</FORM>
	</S>
	<S id="S010">
		<AUDIO start="30.83" end="34.65"/>
		<FORM kindOf="phono">nɯ tɯrme tɯ-fsu jamar, nɯ roro jamar tu-mbro cha.</FORM>
	</S>
	<S id="S011">
		<AUDIO start="35.41" end="38.22"/>
		<FORM kindOf="phono">ma ɯ-jwaʁ nɯ babɯ ɯ-jwaʁ cho wuma ʑo naχtɕɯɣ.</FORM>
	</S>
	<S id="S012">
		<AUDIO start="39.22" end="43.81"/>
		<FORM kindOf="phono">ma nɯnɯ ɴɢolo ŋu tɕe, ɴɢolo nɯ, nɤkinɯ, </FORM>
	</S>
	<S id="S013">
		<AUDIO start="43.81" end="46.83"/>
		<FORM kindOf="phono">ɯ-mat nɯ pɯ-kɯ-xtɕi tɕe tú-wɣ-ndza tɕe ɯ-tɯ-tɕur saχaʁ ʑo,</FORM>
	</S>
	<S id="S014">
		<AUDIO start="47.52" end="50.59"/>
		<FORM kindOf="phono">ɯ-mɯntoʁ nɯ kɯ-mpɕɯ-mpɕɤr ʑo ɲɯ-lɤt tɕe, nɤki,</FORM>
	</S>
	<S id="S015">
		<AUDIO start="50.59" end="52.84"/>
		<FORM kindOf="phono">ɯ-rkɯ tɤ-kɯ-mbɣaʁ kɯ-fse ɲɯ-lɤt tɕe </FORM>
	</S>
	<S id="S016">
		<AUDIO start="53.28" end="55.96"/>
		<FORM kindOf="phono">ɯ-mɯntoʁ nɯ kɯ-ndɯ-ndɯβ ʑo ŋu ri,</FORM>
	</S>
	<S id="S017">
		<AUDIO start="56.31" end="59.01"/>
		<FORM kindOf="phono">li nɯnɯ ɯ-mɯntoʁ roŋri nɯnɯ ɯ-mat ɲɯ-βze ŋu.</FORM>
	</S>
	<S id="S018">
		<AUDIO start="59.6" end="61.64"/>
		<FORM kindOf="phono">tɕe ɯ-mat nɯ ɯ-tɯ-tɕur saχaʁ ʑo.</FORM>
	</S>
	<S id="S019">
		<AUDIO start="62.32" end="65.43"/>
		<FORM kindOf="phono">pɤjkhu kɯnɤ, pjɯ-sɯso-a tɕe, ɲɯ-tɕur kɯ-fse ma, </FORM>
