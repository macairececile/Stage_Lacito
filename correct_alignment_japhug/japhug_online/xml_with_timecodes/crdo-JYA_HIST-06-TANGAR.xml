<TEXT id="crdo-JYA_HIST-06-TANGAR" xml:lang="jya">
	<HEADER>
		<TITLE>crdo-JYA_HIST-06-TANGAR</TITLE>
		<SOUNDFILE href="hist-06-tANgAr.wav"/>
	</HEADER>
	<S id="S001">
		<AUDIO start="0.14" end="4.01"/>
		<FORM kindOf="phono">paʁ ɣɯ ɯ-ŋgɤr tu, ɯ-rtsi tu, ɯ-tʂɤm tu</FORM>
	</S>
	<S id="S002">
		<AUDIO start="4.56" end="8.11"/>
		<FORM kindOf="phono">tɕeri ɯ-rtsi nɯ... maʁ, ɯ-tʂɤm... ɯ-ŋgɤr nɯnɯ</FORM>
	</S>
	<S id="S003">
		<AUDIO start="8.47" end="10.75"/>
		<FORM kindOf="phono">ɯ-ndʐi ɯ-ŋgɯ ɕɯmɯma nɯ tɕu ku-rɤʑi tɕe</FORM>
	</S>
	<S id="S004">
		<AUDIO start="11.1" end="15.2"/>
		<FORM kindOf="phono">zgoku chondɤre, nɯnɯ, ɯ-ŋgɤr tu-kɯ-ti ŋu </FORM>
	</S>
	<S id="S005">
		<AUDIO start="15.97" end="19.32"/>
		<FORM kindOf="phono">ɯ-xtu ɯ-taʁ nɯnɯ tɕe ʑo</FORM>
	</S>
	<S id="S006">
		<AUDIO start="19.32" end="21.09"/>
		<FORM kindOf="phono">nɤki tu-kɯ-luj kɯ-fse nɯnɯ </FORM>
	</S>
	<S id="S007">
		<AUDIO start="21.5" end="26.37"/>
		<FORM kindOf="phono">nɯnɯ ɯ-kri kɯ-tu nɯnɯ tɕe tɤ-rtsi tu-kɯ-ti ŋu</FORM>
	</S>
	<S id="S008">
		<AUDIO start="26.37" end="30.23"/>
		<FORM kindOf="phono">iɕqha ɯ-tʂɤm, ɯ-pu ɯ-taʁ aʁɤndɯndɤt ku-kɯ-ndzoʁ </FORM>
	</S>
	<S id="S009">
		<AUDIO start="30.23" end="35.03"/>
		<FORM kindOf="phono">ɯ-pu ɯ-taʁ qhe ɯ-mtshi ɯ-taʁ ɯ-rtshɤz ɯ-taʁ nɯra kɯ-kɯ-ndzoʁ ɣɯ </FORM>
	</S>
	<S id="S010">
		<AUDIO start="35.03" end="38.49"/>
		<FORM kindOf="phono">ɯkri kɯtu nɯnɯ, ɯɕa nɯnɯ, tɤ-tʂɤm tu-kɯ-ti ŋu</FORM>
	</S>
	<S id="S011">
		<AUDIO start="39.03" end="42.67"/>
		<FORM kindOf="phono">tɕe ɯ-βri nɯtɕu ɯ-kri kɯ-tu χsɯ-tɯ-phu tu ri, </FORM>
	</S>
	<S id="S012">
		<AUDIO start="42.67" end="44.74"/>
		<FORM kindOf="phono">nɯ ʑakastaka nɯ-rmi mɤ-naχtɕɯɣ</FORM>
	</S>
</TEXT>