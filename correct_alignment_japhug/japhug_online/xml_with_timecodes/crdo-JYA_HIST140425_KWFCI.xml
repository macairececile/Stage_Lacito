	<S id="S001">
		<AUDIO start="0.53" end="4.23"/>
		<FORM kindOf="phono">kɯɕɯŋgɯ tɕe, kɯfɕi nɯnɯra wuma ʑo, nɤkinɯ,</FORM>
	</S>
	<S id="S002">
		<AUDIO start="4.23" end="7.0"/>
		<FORM kindOf="phono">nɯ kɯ-fse mɤ-kɯ-taŋ tu-nɤma-nɯ pjɤ-ŋgrɤl tɕe,</FORM>
	</S>
	<S id="S003">
		<AUDIO start="7.38" end="12.5"/>
		<FORM kindOf="phono">tɕendɤre iɕqha nɯ, ri nɯ-mtɕhi nɯ wuma ʑo pjɯ-pe ra tu-ti-nɯ.</FORM>
	</S>
	<S id="S004">
		<AUDIO start="13.39" end="17.62"/>
		<FORM kindOf="phono">tɕe nɯ, fanzheng shi nɯnɯ, mimi nɯra kɤ-nɤtsɯ pjɯ-mkhɤz-nɯ, </FORM>
	</S>
	<S id="S005">
		<AUDIO start="17.62" end="19.09"/>
		<FORM kindOf="phono">nɯ-tsɯ pjɯ-rnaʁ ra kɤ-ti ɲɯ-ŋu. </FORM>
	</S>
	<S id="S006">
		<AUDIO start="19.64" end="25.4"/>
		<FORM kindOf="phono">tɕe núndʐa nɯ, kɯfɕi ɣɯ ɯ-mtɕhi nɯnɯ khɯzgɯr kɯngɯt pɯ-kɤ-lɤt pjɯ-fse ra kɤ-ti ɲɯ-ŋu</FORM>
	</S>
	<S id="S007">
		<AUDIO start="25.98" end="28.52"/>
		<FORM kindOf="phono">tu-ti-nɯ pjɤ-ŋgrɤl</FORM>
	</S>
