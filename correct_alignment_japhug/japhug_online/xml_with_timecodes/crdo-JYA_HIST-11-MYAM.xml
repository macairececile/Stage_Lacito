	</HEADER>
	<S id="S001">
		<AUDIO start="0.12" end="3.06"/>
		<FORM kindOf="phono">mɲɤm nɯnɯ si kɯ-nɤkɤro kɯ-wxti ci ŋu.</FORM>
	</S>
	<S id="S002">
		<AUDIO start="3.87" end="7.27"/>
		<FORM kindOf="phono">tɕe mi cho naχtɕɯɣ-ndʑi jamar ɲɯ-ŋu.</FORM>
	</S>
	<S id="S003">
		<AUDIO start="7.88" end="10.16"/>
		<FORM kindOf="phono">mi cho ndʑi-jwaʁ ra wuma ʑo ɲɯ-naχtɕɯɣ.</FORM>
	</S>
	<S id="S004">
		<AUDIO start="10.84" end="14.37"/>
		<FORM kindOf="phono">tɕeri mɲɤm nɯnɯ ɯ-jwaʁ tɯ-lɤt la-ʑa tɕe, ɲɯ-ɤɣɯrnɯɕɯr.</FORM>
	</S>
	<S id="S005">
		<AUDIO start="14.81" end="18.04"/>
		<FORM kindOf="phono">kɯ-ɤrqhɯ-rqhi ju-kɯ-ru tɕe, ŋotɕu mɲɤm kɯ-tu nɯ ɲɯ-saχsɤl </FORM>
	</S>
	<S id="S006">
		<AUDIO start="19.02" end="22.99"/>
		<FORM kindOf="phono">nɤkinɯ, ɯ-ru nɯnɯ li mi cho ɲɯ-naχtɕɯɣ.</FORM>
	</S>
	<S id="S007">
		<AUDIO start="23.43" end="25.63"/>
		<FORM kindOf="phono">ɯ-rqhu kɯ-tu nɯra cho ɲɯ-naχtɕɯɣ-ndʑi ʑo.</FORM>
	</S>
	<S id="S008">
		<AUDIO start="26.17" end="28.03"/>
		<FORM kindOf="phono">mbɤr-nɯ tɕe xtɕi-nɯ tɕu tɕe </FORM>
	</S>
	<S id="S009">
		<AUDIO start="28.03" end="30.88"/>
		<FORM kindOf="phono">mi cho mɲɤm ni wuma ɲɯ-naχtɕɯɣ-ndʑi</FORM>
	</S>
	<S id="S010">
		<AUDIO start="31.66" end="33.73"/>
		<FORM kindOf="phono">tɕeri tɤ-wxti-ndʑi tɕe tɕendɤre </FORM>
	</S>
	<S id="S011">
		<AUDIO start="33.73" end="35.84"/>
		<FORM kindOf="phono">mi kɯ... tu-mbro cho ɲɯ-jpum ɲɯ-cha ma,</FORM>
	</S>
	<S id="S012">
		<AUDIO start="35.84" end="40.41"/>
		<FORM kindOf="phono">mɲɤm nɯnɯ wuma wuma ʑo tu-jpum cho ɲɯ... tu-mbro mɯ́j-cha.</FORM>
	</S>
	<S id="S013">
		<AUDIO start="41.42" end="44.95"/>
		<FORM kindOf="phono">tɕe nɤki, romɲa kɯ-pe jamar nɯ wuma ʑo ɲɯ-dɤn.</FORM>
	</S>
	<S id="S014">
		<AUDIO start="45.63" end="48.08"/>
		<FORM kindOf="phono">tɕe nɯnɯ tɤ-ɬoʁ tɕe wuma ʑo ɲɯ-ʁjɤr</FORM>
	</S>
	<S id="S015">
		<AUDIO start="48.97" end="51.09"/>
		<FORM kindOf="phono">ɲɯ-ʁjɤr tɕe tɕe mɤʑɯ ɲɯ-jpum mɯ́j-cha.</FORM>
	</S>
	<S id="S016">
		<AUDIO start="51.66" end="53.62"/>
		<FORM kindOf="phono">ɯ-sɤɣɬoʁ nɯnɯ zrɯ tsa ɲɯ-ŋu.</FORM>
	</S>
	<S id="S017">
		<AUDIO start="54.14" end="55.48"/>
		<FORM kindOf="phono">ɴqiaβ nɯ sthɯci maŋe,</FORM>
	</S>
	<S id="S018">
		<AUDIO start="55.48" end="56.81"/>
		<FORM kindOf="phono">ɴqiaβ kɯ-ɬoʁ ɣɤʑu ri,</FORM>
	</S>
	<S id="S019">
		<AUDIO start="57.24" end="58.85"/>
		<FORM kindOf="phono">zrɯ kɯmɯxte tu-ɬoʁ ɲɯ-ŋu.</FORM>
	</S>
	<S id="S020">
		<AUDIO start="60.32" end="66.53"/>
		<FORM kindOf="phono">tɕe nɯnɯ mɲɤm nɯnɯ, nɤki, ɯ-si, ɯʑo ɯ-si nɯnɯ wuma ʑo ɲɯ-mpɯ mɯ́j-ngɯt.</FORM>
	</S>
	<S id="S021">
		<AUDIO start="67.32" end="70.42"/>
		<FORM kindOf="phono">tɕe kɯɕɯŋgɯ tɕe tɤre sɤ-tɕɯ-tɕɤt taχpi tu-βzu-nɯ tɕe, </FORM>
	</S>
	<S id="S022">
		<AUDIO start="70.93" end="75.73"/>
		<FORM kindOf="phono">qajdo ɯ-rʑaβ ɯ-ɕki ɯ-sɤz-nɯrmɤʑu nɯ mɲɤm kɤ-phaʁ pjɤ-ŋu ɲɯ-ŋgrɤl ma </FORM>
	</S>
	<S id="S023">
		<AUDIO start="75.73" end="79.66"/>
		<FORM kindOf="phono">tɕe nɯnɯ, nɤkinɯ, nɯnɯ mɲɤm ŋu.</FORM>
	</S>
	<S id="S024">
		<AUDIO start="80.06" end="82.64"/>
		<FORM kindOf="phono">tɕe ɯ-si mɯ́j-ngɯt ɲɯ-mpɯ tɕe tɕe nɯ </FORM>
	</S>
	<S id="S025">
		<AUDIO start="82.89" end="85.29"/>
		<FORM kindOf="phono">qajdo kɯ nɯ pjɤ-ko tɕe nɯ tu-ti-nɯ ɲɯ-ŋu ma</FORM>
	</S>
	<S id="S026">
		<AUDIO start="85.73" end="91.63"/>
		<FORM kindOf="phono">tɕe mɲɤm nɯnɯ, nɤkinɯ, a-tɤ-nɤkhɯ tɕe wuma ʑo tu-ngɯt ɲɯ-ŋgrɤl tɕe,</FORM>
	</S>
	<S id="S027">
		<AUDIO start="91.69" end="95.72"/>
		<FORM kindOf="phono">tɕe khɤjmu ɣɯ ɯ-taʁ romɲa nɯnɯ mɲɤm chɯ-lɤt-nɯ pjɤ-ŋgrɤl.</FORM>
	</S>
	<S id="S028">
		<AUDIO start="96.36" end="100.23"/>
		<FORM kindOf="phono">tɕe tɕe nɯ tɤ-nɤkhɯ ɯ-jɯja nɯ tɯ-tu-rko tɕe tɯ-tu-ngɯt ɲɯ-ŋgrɤl. </FORM>
	</S>
	<S id="S029">
		<AUDIO start="100.92" end="104.19"/>
		<FORM kindOf="phono">tɕe nɯ mɲɤm ɲɯ-ŋu tɕe ɯ-jwaʁ nɯra mi cho naχtɕɯɣ,</FORM>
	</S>
	<S id="S030">
		<AUDIO start="104.49" end="105.81"/>
		<FORM kindOf="phono">pjɯ-nɯɕɯrɲɟo ɕti.</FORM>
	</S>
	<S id="S031">
		<AUDIO start="106.37" end="113.13"/>
		<FORM kindOf="phono">tɕe pjɯ-nɯɕɯrɲɟo tɕe nɤki, stonka tɕe tɕe ɲɯ-qarŋe, χɯŋχɯŋ zo ɲɯ-qarŋe tɕe ɯ-tɯ-qarŋe mpɕɤr.</FORM>
	</S>
	<S id="S032">
		<AUDIO start="113.96" end="117.89"/>
		<FORM kindOf="phono">tɕe nɯnɯ mɤʑɯ, tɯɣur pa-ta tɕe, tɕe ɯ-jwaʁ pjɯ-ŋgra qhe,</FORM>
	</S>
	<S id="S033">
		<AUDIO start="117.98" end="121.15"/>
		<FORM kindOf="phono">qartsɯ tɕe ɯ-jwaʁ tɯ-mpɕar ci nɤ ɯ-ku kɯ-ndzoʁ me. </FORM>
	</S>
	<S id="S034">
		<AUDIO start="121.98" end="124.59"/>
		<FORM kindOf="phono">ftɕar kɤ-ndzoʁ qhe tɕe li, nɤkinɯ,</FORM>
	</S>
	<S id="S035">
		<AUDIO start="124.59" end="128.73"/>
		<FORM kindOf="phono">ɲɯ-ɣɤtɕɯɣ qhe, ɯ-jwaʁ lu-lɤt tɕe nɯ kóʁmɯz ɯ-rtaʁ tu-zri ɲɯ-ŋu.</FORM>
	</S>
	<S id="S036">
		<AUDIO start="129.74" end="132.92"/>
		<FORM kindOf="phono">tɕe nɯnɯ mɲɤm ŋu.</FORM>
	</S>
	<NOTE message="========================" xmlns:lang="fr"/>
	<S id="S037">
		<AUDIO start="133.87" end="141.23"/>
		<FORM kindOf="phono">tɤndɤku nɯ ɯ-jwaʁ nɯ khɯjŋga ɯ-jwaʁ tsa fse ri,</FORM>
	</S>
	<S id="S038">
		<AUDIO start="141.23" end="143.31"/>
		<FORM kindOf="phono">nɯ sɤznɤ ɲɯ-tɕɤr cho ɲɯ-rɲɟi.</FORM>
	</S>
	<S id="S039">
		<AUDIO start="144.3" end="145.55"/>
		<FORM kindOf="phono">tɕe nɤkinɯ,</FORM>
	</S>
	<S id="S040">
		<AUDIO start="145.55" end="149.78"/>
		<FORM kindOf="phono">tɤndɤku nɯ rcanɯ, li mpɕɤr, ɯ-di ɯ-tɯ-sɤjloʁ saχaʁ ʑo.</FORM>
	</S>
	<S id="S041">
		<AUDIO start="150.62" end="153.09"/>
		<FORM kindOf="phono">ma ɯ-ru nɯra wuma ʑo ɲɯ-ldʑɯz. </FORM>
	</S>
	<S id="S042">
		<AUDIO start="153.45" end="154.68"/>
		<FORM kindOf="phono">tɕe kɤ-prɤt ɴqa.</FORM>
	</S>
	<S id="S043">
		<AUDIO start="155.33" end="158.28"/>
		<FORM kindOf="phono">ma nɯnɯ si ŋu ri, kɯ-mbɯ-mbɤr ci ɕti.</FORM>
	</S>
	<S id="S044">
		<AUDIO start="158.94" end="161.03"/>
		<FORM kindOf="phono">tɯ-mthɤɣ ɯ-fsu jamar ma tu-zɣɯt mɤ-cha.</FORM>
	</S>
	<S id="S045">
		<AUDIO start="161.65" end="164.13"/>
		<FORM kindOf="phono">tɕe ɯ-mɯntoʁ nɯnɯ kɯ-ndɯ-ndɯβ ʑo qhe, </FORM>
	</S>
	<S id="S046">
		<AUDIO start="164.51" end="167.04"/>
		<FORM kindOf="phono">iɕqha stonka mɯntoʁ... maʁ kɯ iɕqha,</FORM>
	</S>
	<S id="S047">
		<AUDIO start="167.85" end="171.37"/>
		<FORM kindOf="phono">ɕɯxtɯrɟɤxtsa, wo nɯnɯ ɣɯ ɯ-mɯntoʁ wuma ʑo fse.</FORM>
	</S>
	<S id="S048">
		<AUDIO start="172.25" end="174.34"/>
		<FORM kindOf="phono">ma nɯnɯ tɤndɤku ŋu tɕe,</FORM>
	</S>
	<S id="S049">
		<AUDIO start="174.62" end="177.3"/>
		<FORM kindOf="phono">ɯ-jwaʁ nɯnɯ tɯxpa lɤskɤr ʑo anɯrŋi </FORM>
	</S>
	<S id="S050">
		<AUDIO start="178.19" end="179.74"/>
		<FORM kindOf="phono">tɕe wannianqing tsa fse.</FORM>
	</S>
	<S id="S051">
		<AUDIO start="180.53" end="182.27"/>
		<FORM kindOf="phono">ma nɯnɯ nɤkinɯ,</FORM>
	</S>
	<S id="S052">
		<AUDIO start="182.67" end="185.22"/>
		<FORM kindOf="phono">qartsɯmɤftɕar tɕe ɯ-jwaʁ nɯ anɯrŋi ɕti.</FORM>
	</S>
	<S id="S053">
		<AUDIO start="185.85" end="188.24"/>
		<FORM kindOf="phono">tɕe ftɕar tɕe tɕe ɯ-mɯntoʁ ɲɯ-lɤt ŋu laʁma,</FORM>
	</S>
	<S id="S054">
		<AUDIO start="188.69" end="191.0"/>
		<FORM kindOf="phono">ftɕar tɕe ʑaʑa ʑo ɯ-mɯntoʁ ɲɯ-lɤt ŋu.</FORM>
	</S>
	<S id="S055">
		<AUDIO start="191.88" end="195.36"/>
		<FORM kindOf="phono">tɕe sansiyuefen jamar ri tɕe ɯ-mɯntoʁ ɲɯ-lɤt ɕti.</FORM>
	</S>
	<S id="S056">
		<AUDIO start="196.18" end="197.47"/>
		<FORM kindOf="phono">ma nɯ kɯ-fse ɲɯ-ŋu tɕe,</FORM>
	</S>
	<S id="S057">
		<AUDIO start="197.91" end="200.35"/>
		<FORM kindOf="phono">nɯnɯ li ɯ-zrɤm kɯ ɲɯ-mphɯl ɲɯ-ɕti ma </FORM>
	</S>
	<S id="S058">
		<AUDIO start="200.35" end="202.73"/>
		<FORM kindOf="phono">ɯ-rɣi kɯ-tu maŋe ma si ɲɯ-ɕti.</FORM>
	</S>
	<S id="S059">
		<AUDIO start="203.18" end="205.0"/>
		<FORM kindOf="phono">tɕe nɯnɯ tɤndɤku ŋu.</FORM>
	</S>
	<S id="S060">
		<AUDIO start="205.67" end="209.11"/>
		<FORM kindOf="phono">mɤʑɯ tɕhi pɯ-ŋu?</FORM>
	</S>
	<NOTE message="=====================" xmlns:lang="fr"/>
	<S id="S061">
		<AUDIO start="209.11" end="210.13"/>
		<FORM kindOf="phono">tsantʂa tu.</FORM>
	</S>
	<S id="S062">
		<AUDIO start="210.63" end="214.28"/>
		<FORM kindOf="phono">tsantʂa nɯ si ŋu tɕe, ɯ-jwaʁ nɯra tɯrgi wuma ʑo fse.</FORM>
	</S>
	<S id="S063">
		<AUDIO start="215.05" end="219.1"/>
		<FORM kindOf="phono">tɕeri tɯrgi ɣɯ nɯ ɯ-jwaʁ nɯnɯ, nɤki nɯnɯ,</FORM>
	</S>
	<S id="S064">
		<AUDIO start="219.5" end="221.96"/>
		<FORM kindOf="phono">ɯ-βri ku-ofskɤr ʑo kɯ-fse lu-ɬoʁ ŋu tɕe,</FORM>
	</S>
	<S id="S065">
		<AUDIO start="222.36" end="225.78"/>
		<FORM kindOf="phono">ɯ-jwaʁ ɯ-tshɯɣa nɯnɯ kɤrɯtaqaβ kɯ-fse naχtɕɯɣ ri,</FORM>
	</S>
	<S id="S066">
		<AUDIO start="226.26" end="230.33"/>
		<FORM kindOf="phono">tsantʂa ɣɯ ɯ-jwaʁ nɯ lu-oʑɯrja rbɤrbɤt ʑo tɕe, nɤki</FORM>
	</S>
	<S id="S067">
		<AUDIO start="230.33" end="232.21"/>
		<FORM kindOf="phono">kumpɣa ɯ-muj kɯ-fse nɯnɯ</FORM>
	</S>
	<S id="S068">
		<AUDIO start="232.47" end="235.49"/>
		<FORM kindOf="phono">pɣa ɣɯ ɯ-muj nɯ tsa ɯ-tshɯɣa tu-fse ŋu ma</FORM>
	</S>
	<S id="S069">
		<AUDIO start="235.49" end="239.08"/>
		<FORM kindOf="phono">tɯrgi ɣɯ nɯ kɯ-fse, ku-fskɤr kɯ-fse, ɲɯ-loʁ ɲɯ-maʁ.</FORM>
	</S>
	<S id="S070">
		<AUDIO start="240.01" end="242.81"/>
		<FORM kindOf="phono">tɕe nɯnɯ tsantʂa nɯnɯ wuma ʑo smɤn ɲɯ-ŋu khi</FORM>
	</S>
	<S id="S071">
		<AUDIO start="244.0" end="247.3"/>
		<FORM kindOf="phono">tɕe wuma ʑo ɲɯ-ɤsɯ-ɕar-nɯ ma nɯnɯ smɤn kɯ-pe ɲɯ-ŋu khi ma</FORM>
	</S>
	<S id="S072">
		<AUDIO start="247.83" end="251.73"/>
		<FORM kindOf="phono">tsantʂa nɯnɯ zgoku kɯ-mbro me,</FORM>
	</S>
	<S id="S073">
		<AUDIO start="251.73" end="254.23"/>
		<FORM kindOf="phono">cɤmi, tɯci kɯ-wxti ɣɯ ɯ-rkɯ nɯnɯra tu tɕe,</FORM>
	</S>
	<S id="S074">
		<AUDIO start="254.61" end="257.98"/>
		<FORM kindOf="phono">nɯ kɯnɤ sɤtɕha tsantʂa kɯ-tu ɣɤʑu, kɯ-me ɣɤʑu.</FORM>
	</S>
	<S id="S075">
		<AUDIO start="258.74" end="260.34"/>
		<FORM kindOf="phono">tɕe nɯnɯ li tu-mbro tɕe,</FORM>
	</S>
	<S id="S076">
		<AUDIO start="260.34" end="263.56"/>
		<FORM kindOf="phono">tɯrgi sthɯci tɕi tu-mbro mɯ́j-cha ma </FORM>
	</S>
	<S id="S077">
		<AUDIO start="264.08" end="267.08"/>
		<FORM kindOf="phono">li, nɤkinɯ, ʁnɯ-rtsɤɣ χsɯ-rtsɤɣ jamar tu-mbro ɲɯ-cha.</FORM>
	</S>
	<S id="S078">
		<AUDIO start="267.97" end="269.25"/>
		<FORM kindOf="phono">tɕe nɯnɯ tsantʂa ŋu.</FORM>
	</S>
	<S id="S079">
		<AUDIO start="270.89" end="271.83"/>
		<FORM kindOf="phono">tsantʂa nɯnɯ li nɤki,</FORM>
	</S>
	<S id="S080">
		<AUDIO start="271.83" end="276.06"/>
		<FORM kindOf="phono">tɤtho, tɯrgi nɯnɯra chondɤre</FORM>
	</S>
	<S id="S081">
		<AUDIO start="276.06" end="278.32"/>
		<FORM kindOf="phono">kɯ-naχtɕɯɣ tsa ɲɯ-ŋu ri,  </FORM>
	</S>
	<S id="S082">
		<AUDIO start="278.32" end="280.84"/>
		<FORM kindOf="phono">nɯnɯ ɯ-ŋgɯ zɯ li ɯ-tɯphu ɲɯ-ɤntɕhɯ tɕe,</FORM>
	</S>
	<S id="S083">
		<AUDIO start="281.21" end="282.98"/>
		<FORM kindOf="phono">nɯnɯ tsantʂa nɯ tɯ-tɯphu ɲɯ-ŋu tɕe,</FORM>
	</S>
	<S id="S084">
		<AUDIO start="282.98" end="286.17"/>
		<FORM kindOf="phono">nɯ pa tsa tu-ɬoʁ ɲɯ-cha ma kɯ-mbro,</FORM>
	</S>
	<S id="S085">
		<AUDIO start="286.17" end="288.01"/>
		<FORM kindOf="phono">sɤtɕha tɤ-mbro tɕe tu-ɬoʁ mɯ́j-cha. </FORM>
	</S>
	<S id="S086">
		<AUDIO start="288.83" end="290.13"/>
		<FORM kindOf="phono">tɕe nɯnɯ tsantʂa ŋu.</FORM>
	</S>
	<S id="S087">
		<AUDIO start="290.95" end="291.98"/>
		<FORM kindOf="phono">tɕe mɤʑɯ tɕhi pɯ-ŋu?</FORM>
	</S>
</TEXT>