	<S id="S001">
		<AUDIO start="0.04" end="1.55"/>
		<FORM kindOf="phono">gongxun jɤxtshi lɤ-ɣe tɕe,</FORM>
	</S>
	<S id="S002">
		<AUDIO start="2.05" end="5.88"/>
		<FORM kindOf="phono">a-ɣe ɣɯ ɯ-pɤro ci qiche kɯ-xtɕi ci to-χtɯ. </FORM>
	</S>
	<S id="S003">
		<AUDIO start="6.64" end="7.68"/>
		<FORM kindOf="phono">tɕe andi ra ata.</FORM>
	</S>
	<S id="S004">
		<AUDIO start="8.19" end="11.78"/>
		<FORM kindOf="phono">tɕetha a-ɣe thɯ-ɣe tɕe nɯrge ma andi ra ata.</FORM>
	</S>
	<S id="S005">
		<AUDIO start="13.68" end="16.63"/>
		<FORM kindOf="phono">tɕe dɯxpa ma nɯ ɯ-pɤro ra jo-ɣɯt. </FORM>
	</S>
	<S id="S006">
		<AUDIO start="18.25" end="20.72"/>
		<FORM kindOf="phono">tɕe i-kha ci tɤ-rɯndzɤtshi qhe tɕe jɤ-anɯri.</FORM>
	</S>
	<S id="S007">
		<AUDIO start="21.24" end="22.9"/>
		<FORM kindOf="phono">wo tɤ-rɯndzɤtshi tʂhɯɣ me ma</FORM>
	</S>
	<S id="S008">
		<AUDIO start="23.32" end="25.11"/>
		<FORM kindOf="phono">to-rɯndzɤtshi tɕe jɤ-ɣe ɕti tɕe tɕe</FORM>
	</S>
	<S id="S009">
		<AUDIO start="25.11" end="27.11"/>
		<FORM kindOf="phono">aʑo taʁ tɤ-ari-a ri tɕe tɕe </FORM>
	</S>
	<S id="S010">
		<AUDIO start="27.53" end="29.04"/>
		<FORM kindOf="phono">pjɯ-ɣi-a ɕɯŋgɯ tɕe jo-nɯɕe</FORM>
	</S>
	<S id="S011">
		<AUDIO start="29.83" end="31.09"/>
		<FORM kindOf="phono">tɕe nɯ ɯ-qhu mɯ-nɯ-atɯɣ-tɕi.</FORM>
	</S>
	<S id="S012">
		<AUDIO start="32.57" end="35.88"/>
		<FORM kindOf="phono">tɕe Tsheyag laoshi ɯ-ɕki ɕe-a ra ɲɯ-ti tɕe nɯ jɤ-ari ŋu ma</FORM>
	</S>
	<S id="S013">
		<AUDIO start="36.68" end="37.63"/>
		<FORM kindOf="phono">tɕe  mɯ-nɯ-atɯɣ-tɕi.</FORM>
	</S>
</TEXT>