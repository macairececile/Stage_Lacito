	<S id="S001">
		<AUDIO start="0.04" end="2.24"/>
		<FORM kindOf="phono">﻿tɕamu kɤ-ti ci tu.</FORM>
	</S>
	<S id="S002">
		<AUDIO start="3.16" end="4.44"/>
		<FORM kindOf="phono">tɕamu kɤ-ti ci tu tɕe,</FORM>
	</S>
	<S id="S003">
		<AUDIO start="4.44" end="8.2"/>
		<FORM kindOf="phono">nɯnɯ tɕe iɕqha, tʂha ɯ-ŋgɯ lu-nɯ-ta-nɯ ŋgrɤl tɕe núndʐa ŋu,</FORM>
	</S>
	<S id="S004">
		<AUDIO start="8.78" end="9.6"/>
		<FORM kindOf="phono">ɲɯ́-wɣ-sɯɣrom ŋu tɕe, </FORM>
	</S>
	<S id="S005">
		<AUDIO start="10.23" end="12.88"/>
		<FORM kindOf="phono">tɕe nɯnɯ ɣɯ ɯ-jwaʁ nɯ rʁɤβrʁɤβ kɯ-pa ci ŋu tɕe,</FORM>
	</S>
	<S id="S006">
		<AUDIO start="13.23" end="15.73"/>
		<FORM kindOf="phono">tɕetu rɯŋgu nɯra sɯjno tɤrca tu,</FORM>
	</S>
	<S id="S007">
		<AUDIO start="16.32" end="21.91"/>
		<FORM kindOf="phono">tɕe ɯʑo nɯnɯ, nɤkinɯ, tɯɣro ɯ-rca ɲɯ́-wɣ-nɯ-phɯt tɕe li nɯŋa jla ra nɯ-ndza kɯnɤ sna.</FORM>
	</S>
	<S id="S008">
		<AUDIO start="22.8" end="26.62"/>
		<FORM kindOf="phono">ma nɯnɯ ɯʑo ʁɟa tɯ-mdʑu ɯ-taʁ kú-wɣ-sthaβ tɕe mɤrtsaβ, </FORM>
	</S>
	<S id="S009">
		<AUDIO start="27.57" end="31.27"/>
		<FORM kindOf="phono">tɕe nɯnɯ tɕamu nɯnɯ sɯjno kɯmbɯ... kɯ-mbɯ-mbɤr ci ɕti,</FORM>
	</S>
	<S id="S010">
		<AUDIO start="31.73" end="34.33"/>
		<FORM kindOf="phono">ɯ-jwaʁ ra nɤkɤro wxti tɕe ɲɯ-sqhiar kɯ-fse ŋu.</FORM>
	</S>
	<S id="S011">
		<AUDIO start="35.04" end="39.63"/>
		<FORM kindOf="phono">tɕe nɯnɯ, nɤki, li tɤ-ɬoʁ tɕe li ftɕar tɕe tu-ɬoʁ, qartsɯ tɕe pjɯ-rom ɕti ma,</FORM>
	</S>
	<S id="S012">
		<AUDIO start="40.1" end="45.04"/>
		<FORM kindOf="phono">nɯnɯ, nɤki, tɤ-ɬoʁ tɕe tɕe, ɯ-jwaʁ nɯ ɲɯ-sqhiar kɯ-fse ŋu tɕe,</FORM>
	</S>
	<S id="S013">
		<AUDIO start="45.39" end="46.93"/>
		<FORM kindOf="phono">ɯ-ru nɯ tɕhi nɯ-jpum </FORM>
	</S>
	<S id="S014">
		<AUDIO start="46.93" end="48.76"/>
		<FORM kindOf="phono">tɯ-jaʁndzu kɯnɤ ɲɯ-βze mɤ-cha.</FORM>
	</S>
	<S id="S015">
		<AUDIO start="49.81" end="54.35"/>
		<FORM kindOf="phono">tɕe nɯnɯ, nɤki tɕamu nɯnɯ tu-ɬoʁ tɕe, nɤkinɯ,</FORM>
	</S>
	<S id="S016">
		<AUDIO start="54.81" end="56.75"/>
		<FORM kindOf="phono">ɯ-jwaʁ nɯ ɲɯ-sqhiar kɯ-fse tɕe tɕe,</FORM>
	</S>
	<S id="S017">
		<AUDIO start="57.1" end="58.95"/>
		<FORM kindOf="phono">ɯ-χcɤl ri li ɯ-spjɯŋ tu-ɣi tɕe,</FORM>
	</S>
	<S id="S018">
		<AUDIO start="58.95" end="60.69"/>
		<FORM kindOf="phono">nɯ ɯ-taʁ ri ɲɯ-rɯmɯntoʁ ŋu tɕe,</FORM>
	</S>
	<S id="S019">
		<AUDIO start="60.99" end="63.12"/>
		<FORM kindOf="phono">ɯ-mɯntoʁ ɯ-taʁ kɯnɤ ɯ-jwaʁ ɲɯ-βze ŋu ma</FORM>
	</S>
	<S id="S020">
		<AUDIO start="63.12" end="65.13"/>
		<FORM kindOf="phono">nɯnɯ ɯ-mɯntoʁ ɯ-ru nɯ kɯ-zɯ-zri ŋu.</FORM>
	</S>
	<S id="S021">
		<AUDIO start="65.52" end="67.33"/>
		<FORM kindOf="phono">ɯ-mɯntoʁ nɯ kɯ-wɣrum ɲɯ-lɤt ŋu.</FORM>
	</S>
	<S id="S022">
		<AUDIO start="67.84" end="70.91"/>
		<FORM kindOf="phono">ɯ-χcɤl nɯ kɯ-qarŋe, ɯ-jwaʁ kɯ-fse nɯ ra kɯ-wɣrum,</FORM>
	</S>
	<S id="S023">
		<AUDIO start="71.52" end="77.69"/>
		<FORM kindOf="phono">tɕe kɯ-wɣrum, ɯ-mɯntoʁ, maʁ, ɯ-jwaʁ ɣɯ ɯ-qhuchu nɯra kɯ-ɤɣɯrnɯɕɯr ŋu.</FORM>
	</S>
	<S id="S024">
		<AUDIO start="78.17" end="80.25"/>
