<TEXT id="crdo-JYA_HIST-05-TAMAR" xml:lang="jya">
	<HEADER>
		<TITLE>crdo-JYA_HIST-05-TAMAR</TITLE>
		<SOUNDFILE href="hist-05-tamar.wav"/>
	</HEADER>
	<S id="S001">
		<AUDIO start="0.4" end="3.09"/>
		<FORM kindOf="phono">qra ɣɯ ɯ-mar wuma ʑo kɯ-ʑru ɲɯ-ŋu tɕe,</FORM>
	</S>
	<S id="S002">
		<AUDIO start="3.57" end="6.21"/>
		<FORM kindOf="phono">qra ɣɯ ɯ-lu pjɯ́-wɣ-tɕɤt tɕe, li nɯnɯ ta-mar</FORM>
	</S>
	<S id="S003">
		<AUDIO start="6.62" end="8.7"/>
		<FORM kindOf="phono">pjɯ́-wɣ-ɣɟaβ tɕe ta-mar tú-wɣ-tɕɤt tɕe nɯnɯ</FORM>
	</S>
	<S id="S004">
		<AUDIO start="9.22" end="11.61"/>
		<FORM kindOf="phono">li tɯ-mtshi cho tɯ-qhoχpa wuma ʑo kɯ-phɤn</FORM>
	</S>
	<S id="S005">
		<AUDIO start="12.35" end="14.3"/>
		<FORM kindOf="phono">tɕe qra kɯnɤ wuma ʑo kɯ-pe ɕti</FORM>
	</S>
	<S id="S006">
		<AUDIO start="14.91" end="16.7"/>
		<FORM kindOf="phono">qra ɣɯ ɯ-ɣli nɯ kɯnɤ li </FORM>
	</S>
	<S id="S007">
		<AUDIO start="17.06" end="18.59"/>
		<FORM kindOf="phono">alo mbroχpa ra tɕe tɕe</FORM>
	</S>
	<S id="S008">
		<AUDIO start="19.1" end="21.81"/>
		<FORM kindOf="phono">nɤki qra cho qambrɯ ra ɣɯ nɯɣli nɯnɯ</FORM>
	</S>
	<S id="S009">
		<AUDIO start="21.93" end="24.94"/>
		<FORM kindOf="phono">tɕe nɯ tu-wum-nɯ, tu-sɯɣrom-nɯ</FORM>
	</S>
	<S id="S010">
		<AUDIO start="25.75" end="29.0"/>
		<FORM kindOf="phono">mbroχpa sɤtɕha tɕe stomku ʁɟa ɲɯɕti ma si maŋe tɕe</FORM>
	</S>
	<S id="S011">
		<AUDIO start="29.55" end="31.79"/>
		<FORM kindOf="phono">tɕe nɯnɯtɕu tɕe, nɯ-si ɯ-tshɤt ɲɯ-ŋu </FORM>
	</S>
	<S id="S012">
		<AUDIO start="32.4" end="33.74"/>
		<FORM kindOf="phono">tɕe chɯ-nɯβlɯ-nɯ ɲɯ-ŋu</FORM>
	</S>
	<S id="S013">
		<AUDIO start="34.53" end="37.09"/>
		<FORM kindOf="phono">iʑora tɕe si ɲɯ́-wɣ-phɯt tɕe, si chɯ́-wɣ-nɯ-βlɯ ŋu ma</FORM>
	</S>
	<S id="S014">
		<AUDIO start="37.64" end="39.54"/>
		<FORM kindOf="phono">mbroχpa ra iɕqha nɯ</FORM>
	</S>
	<S id="S015">
		<AUDIO start="39.92" end="42.97"/>
		<FORM kindOf="phono">qra cho qambrɯ ra ɣɯ nɯqe nɯ tusɯɣromnɯ tɕe </FORM>
	</S>
	<S id="S016">
		<AUDIO start="43.33" end="46.91"/>
		<FORM kindOf="phono">tɕheme ra wuma ɲɯ-zdɯɣ-nɯ ma nɯ-stomku ɯ-taʁ chɯ-mar-nɯ</FORM>
	</S>
	<S id="S017">
		<AUDIO start="47.61" end="52.21"/>
		<FORM kindOf="phono">chɯmarnɯ tɕe nɯnɯ tɤrom tɕe, tɕe turtsɯɣnɯ tɕe pjɯɕɯfkaβnɯ tɕe</FORM>
	</S>
	<S id="S018">
		<AUDIO start="52.68" end="54.4"/>
		<FORM kindOf="phono">tɕe qartsɯ tɕe nɯ chɯnɯβlɯnɯ</FORM>
	</S>
	<S id="S019">
		<AUDIO start="54.98" end="62.53"/>
		<FORM kindOf="phono">tɕe sɤznɯmbjɯm ndzɤtshi sɤβzu  lonba nɯnɯ tɯqe nɯ ɲɯŋu</FORM>
	</S>
</TEXT>