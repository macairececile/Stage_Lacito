	<S id="S001">
		<AUDIO start="0.56" end="3.65"/>
		<FORM kindOf="phono">﻿qachɣa mɯntoʁ kɤ-ti ci tu tɕe, nɯ sɯjno ŋu.</FORM>
	</S>
	<S id="S002">
		<AUDIO start="4.69" end="9.89"/>
		<FORM kindOf="phono">sɯjno ŋu tɕe, ɯʑo mbɤr, ʁnɯ-tɣa jamar ma tu-βze mɤ-cha.</FORM>
	</S>
	<S id="S003">
		<AUDIO start="10.46" end="12.56"/>
		<FORM kindOf="phono">tɕeri ftɕar tɕe li tu-ɬoʁ ŋu.</FORM>
	</S>
	<S id="S004">
		<AUDIO start="12.74" end="15.74"/>
		<FORM kindOf="phono">tɕe nɯ tɤ-ɬoʁ ɕɯmɯma nɯ ɯ-jwaʁ ma me,</FORM>
	</S>
	<S id="S005">
		<AUDIO start="16.41" end="19.79"/>
		<FORM kindOf="phono">ɯ-ru nɯnɯ ɲaʁ tsa, aɣɯnɯɕɯr kɯ-fse ma,</FORM>
	</S>
	<S id="S006">
		<AUDIO start="20.23" end="22.09"/>
		<FORM kindOf="phono">ɲaʁ tsa, tɕe nɯ ɯ-βri,</FORM>
	</S>
	<S id="S007">
		<AUDIO start="22.09" end="24.17"/>
		<FORM kindOf="phono">ɯ-ru nɯre ɯ-jwaʁ tu-nɯgrɤl ŋu tɕe, </FORM>
	</S>
	<S id="S008">
		<AUDIO start="24.17" end="26.88"/>
		<FORM kindOf="phono">tɕe taʁ tsa ri tɕe tɕe, ɯ-mɯntoʁ ɲɯ-lɤt ŋu.</FORM>
	</S>
	<S id="S009">
		<AUDIO start="27.51" end="31.16"/>
		<FORM kindOf="phono">tɕe ɯ-mɯntoʁ nɯ mpɕɤr, ɯ-mɯntoʁ ɣɯ ɯ-jwaʁ nɯ kɯ-dɯ-dɤn ʑo ɲɯ-ŋu.</FORM>
	</S>
	<S id="S010">
		<AUDIO start="31.72" end="34.09"/>
		<FORM kindOf="phono">kɯ-tɕɤr qhe, kɯ-ndɯβ qhe kɯ-dɯ-dɤn ʑo ɲɯ-ŋu.</FORM>
	</S>
	<S id="S011">
		<AUDIO start="34.6" end="39.74"/>
		<FORM kindOf="phono">tɕe ɯ-χcɤl nɯnɯ tɕe, iɕqha, xiangrikui tsa ɲɯ-fse, ɯ-mɯntoʁ ɯ-tshɯɣa nɯ. </FORM>
	</S>
	<S id="S012">
		<AUDIO start="40.21" end="42.93"/>
		<FORM kindOf="phono">ri ɲɯ-xtɕi, ki kɯ-xtɕɯ-xtɕi ma me.</FORM>
	</S>
	<S id="S013">
		<AUDIO start="44.06" end="45.99"/>
		<FORM kindOf="phono">tɕeri li nɤkɤro, nɤki, </FORM>
	</S>
	<S id="S014">
		<AUDIO start="45.99" end="50.69"/>
		<FORM kindOf="phono">χɕɤlphoŋ ɯ-fkaβ jamar ɲɯ-wxti cha.</FORM>
	</S>
	<S id="S015">
		<AUDIO start="51.74" end="53.95"/>
		<FORM kindOf="phono">ma nɯnɯ ɯ-mŋu jamar ɲɯ-wxti cha.</FORM>
	</S>
	<S id="S016">
		<AUDIO start="54.69" end="57.75"/>
		<FORM kindOf="phono">ma nɯnɯ qachɣa mɯntoʁ ŋu tɕe, ŋu low?</FORM>
	</S>
	<S id="S017">
		<AUDIO start="58.45" end="60.68"/>
		<FORM kindOf="phono">tɕe nɯ qachɣa mɯntoʁ tu-kɯ-ti ŋu tɕe,</FORM>
	</S>
	<S id="S018">
		<AUDIO start="61.1" end="62.7"/>
		<FORM kindOf="phono">ɯ-χcɤl nɯ kɯ-qarŋe ɲɯ-ŋu,</FORM>
	</S>
	<S id="S019">
		<AUDIO start="63.25" end="68.58"/>
		<FORM kindOf="phono">ɯ-rkɯ nɯre, ɯ-mɯntoʁ kɣɯ ɯ-jwaʁ ɯ-fse nɯra kɯ-ɤrŋi ɲɯ-ŋu. tɕe ɲɯ-mpɕɤr.</FORM>
	</S>
	<S id="S020">
		<AUDIO start="69.66" end="71.76"/>
		<FORM kindOf="phono">tɕe nɯ qachɣa mɯntoʁ ŋu. </FORM>
	</S>
	<S id="S021">
		<AUDIO start="71.76" end="73.53"/>
		<FORM kindOf="phono">nɯ rɯŋgu aʁɤndɯndɤt ʑo tu.</FORM>
	</S>
	<S id="S022">
		<AUDIO start="74.19" end="77.27"/>
		<FORM kindOf="phono">tɯji ɯ-rkɯ ra tu, sɯŋgɯ ra tɕi tu.</FORM>
	</S>
	<S id="S023">
		<AUDIO start="77.92" end="81.02"/>
		<FORM kindOf="phono">ri stɤmku, stu ɯ-sɤ-dɤn nɯ stɤmku nɯra ŋu-nɯ.</FORM>
	</S>
	<S id="S024">
		<AUDIO start="81.95" end="85.72"/>
		<FORM kindOf="phono">tɕe nɯra nɯ-ŋgɯ tɕe tu-ɬoʁ ŋu tɕe, mpɕɤr.</FORM>
	</S>
	<S id="S025">
		<AUDIO start="86.97" end="88.42"/>
		<FORM kindOf="phono">tɕe nɯ qachɣa mɯntoʁ ŋu.</FORM>
	</S>
	<S id="S026">
		<AUDIO start="88.78" end="91.5"/>
		<FORM kindOf="phono">tɕe stonka jɤ-ɣe tɕe, li tɤ-rca pjɯ-nɯɣur qhe, </FORM>
	</S>
	<S id="S027">
		<AUDIO start="92.06" end="93.12"/>
		<FORM kindOf="phono">tɕe ɲɯ-me ɕti.</FORM>
	</S>
	<S id="S028">
		<AUDIO start="93.69" end="96.47"/>
		<FORM kindOf="phono">tɕe ɯ-fsaqhe, ftɕar kɤ-ndzoʁ qhe li tu-ɬoʁ.</FORM>
	</S>
	<S id="S029">
		<AUDIO start="97.6" end="99.35"/>
		<FORM kindOf="phono">tɕe nɯ qachɣa mɯntoʁ ŋu.</FORM>
	</S>
