	<S id="S001">
		<AUDIO start="0.46" end="4.88"/>
		<FORM kindOf="phono">tɕɣom kɤ-ti ci tu tɕe,</FORM>
	</S>
	<S id="S002">
		<AUDIO start="4.88" end="8.26"/>
		<FORM kindOf="phono">tɕɣom nɯnɯ, kɯmaʁ nɯra mɤ-fse kɯ</FORM>
	</S>
	<S id="S003">
		<AUDIO start="8.26" end="10.1"/>
		<FORM kindOf="phono">ɯ-βri nɯra ɯ-mdzu tu tɕe,</FORM>
	</S>
	<S id="S004">
		<AUDIO start="10.1" end="13.32"/>
		<FORM kindOf="phono">nɯnɯ li χɕitka tɕe ɲɯ-ɣɤtɕɯɣ</FORM>
	</S>
	<S id="S005">
		<AUDIO start="13.88" end="15.57"/>
		<FORM kindOf="phono">tɕe nɯ ɯ-jwaʁ ɲɯ-βze, </FORM>
	</S>
	<S id="S006">
		<AUDIO start="15.86" end="20.26"/>
		<FORM kindOf="phono">tɕendɤre mɤʑɯ iɕqha nɯ, ɯ-mat nɯnɯ na-lɤt ɕɯmɯma nɤ</FORM>
	</S>
	<S id="S007">
		<AUDIO start="20.26" end="22.8"/>
		<FORM kindOf="phono">kɯ-ndɯ-ndɯβ ʑo ma me, karɣi ʑo kɯ-fse ma me </FORM>
	</S>
	<S id="S008">
		<AUDIO start="23.01" end="24.79"/>
		<FORM kindOf="phono">tɕe ʑɯrɯʑɤri ɲɯ-wxti tɕe,</FORM>
	</S>
	<S id="S009">
		<AUDIO start="25.16" end="28.88"/>
		<FORM kindOf="phono">tɯ-sla ʁnɯ-sla jamar tɤ-tsu tɕe tɕendɤre chɯ-ɣɯrni ŋu</FORM>
	</S>
	<S id="S010">
		<AUDIO start="29.45" end="32.34"/>
		<FORM kindOf="phono">tɕe thɯ-ɣɯrni tɕe tɕe nɯ ɲɯ́-wɣ-phɯt ra ma</FORM>
	</S>
	<S id="S011">
		<AUDIO start="32.34" end="35.11"/>
		<FORM kindOf="phono">nɤki, nɯ maʁ nɤ tɕe kɯmaqhu tɕe nɤkinɯ </FORM>
	</S>
	<S id="S012">
		<AUDIO start="35.11" end="38.69"/>
		<FORM kindOf="phono">ɯ-rqhu nɯ ɲɯ-ɴɢaʁ tɕe, ɯ-ŋgɯ ɯ-rɣi pjɯ-nɯɬoʁ tɕe tɕe  mɤ-nɯɣɯphɯt</FORM>
	</S>
	<S id="S013">
		<AUDIO start="39.14" end="40.31"/>
		<FORM kindOf="phono">tɕe pjɯ-ŋgra ɕti</FORM>
	</S>
	<S id="S014">
		<AUDIO start="40.9" end="43.43"/>
		<FORM kindOf="phono">tɕe nɯ kɤ-sɤjtɯm ɯ-thoʁ ntsɯ pjɯ-nɯɕe ɕti ma </FORM>
	</S>
	<S id="S015">
		<AUDIO start="43.43" end="45.45"/>
		<FORM kindOf="phono">kɤ-nɯrdoʁ ri mɤ-ɤɲaj ma kɯ-ndɯ-ndɯβ ɕti</FORM>
	</S>
	<S id="S016">
		<AUDIO start="46.2" end="47.43"/>
		<FORM kindOf="phono">tɕe nɯnɯ tɕɣom ŋu tɕe</FORM>
	</S>
	<S id="S017">
		<AUDIO start="47.43" end="48.48"/>
		<FORM kindOf="phono">tɕɣom ɣɯ ɯ-ru nɯnɯ</FORM>
	</S>
	<S id="S018">
		<AUDIO start="48.48" end="51.95"/>
		<FORM kindOf="phono">nɯ aɣɯrtɯrtaʁ ŋu ma wuma ʑo tu-mbro mɤ-cha</FORM>
	</S>
	<S id="S019">
		<AUDIO start="52.48" end="54.69"/>
		<FORM kindOf="phono">tɯ-rtsɤɣ ɕaŋtaʁ tu-wxti mɤ-cha ma</FORM>
	</S>
	<S id="S020">
		<AUDIO start="55.17" end="58.59"/>
		<FORM kindOf="phono">ɯnɯnɯ nɤkinɯ, ɲɯ-ɤɣɯrtɯrtaʁ tɕe tɕendɤre</FORM>
	</S>
	<S id="S021">
		<AUDIO start="58.59" end="61.11"/>
		<FORM kindOf="phono">nɯ ɯ-jwaʁ ɲɯ-lɤt tɕendɤre ɯ-mat ɲɯ-βze  </FORM>
	</S>
	<S id="S022">
		<AUDIO start="61.44" end="63.49"/>
		<FORM kindOf="phono">tɕe ɯ-mat nɯ ɲɯ́-wɣ-nɯ-phɯt tɕe tɕendɤre</FORM>
	</S>
	<S id="S023">
		<AUDIO start="63.49" end="67.15"/>
		<FORM kindOf="phono">stonka jɤ-ɣe tɕe tɕe li ɯ-jwaʁ nɯ ɲɯ-ɣɯrni kɯ-fse</FORM>
	</S>
	<S id="S024">
		<AUDIO start="67.15" end="69.1"/>
		<FORM kindOf="phono">tsuku ɲɯ-qarŋe kɯ-fse tɕe tɕendɤre</FORM>
	</S>
	<S id="S025">
		<AUDIO start="69.7" end="71.72"/>
		<FORM kindOf="phono">qartsɯ jɤɣe tɕe tɯɣur pɯ-ata tɕe, </FORM>
	</S>
	<S id="S026">
		<AUDIO start="71.72" end="73.48"/>
		<FORM kindOf="phono">tɕe ɯ-jwaʁ nɯra pjɯ-ŋgra ɕti</FORM>
	</S>
	<S id="S027">
		<AUDIO start="74.07" end="78.61"/>
		<FORM kindOf="phono">tɕe nɯ ɯβri nɯ, nɤkinɯ, ɯ-ru kɯjpum ɯtaʁ tɕe ɯ-mdzu tu,</FORM>
	</S>
	<S id="S028">
		<AUDIO start="78.98" end="80.42"/>
		<FORM kindOf="phono">ɯ-rtaʁ tɕe ɯ-mdzu tu</FORM>
	</S>
	<S id="S029">
		<AUDIO start="80.82" end="84.4"/>
		<FORM kindOf="phono">tɕe kɤ-phɯt, ɯ-mat kɤ-nɯ-phɯt tɤ-mda tɕe wuma ʑo sɤɣdɯɣ ma </FORM>
	</S>
	<S id="S030">
		<AUDIO start="84.81" end="86.42"/>
		<FORM kindOf="phono">tɯ-jaʁfkɯm mɯ-mɤ-thɯ́-wɣ-ŋga nɤ </FORM>
	</S>
	<S id="S031">
		<AUDIO start="87.0" end="90.91"/>
		<FORM kindOf="phono">maka ɯ-mdzu tu tɕe, tɯjaʁ rcanɯ ki, tɯjaʁ ɯku nɯra rcanɯ</FORM>
	</S>
	<S id="S032">
		<AUDIO start="91.53" end="95.0"/>
		<FORM kindOf="phono">kɯndɯ-ndɯβ ʑo, maka, tshaʁ ɯ-mɲaʁ ʑo ɲɯ-ste ɕti wo </FORM>
	</S>
	<S id="S033">
		<AUDIO start="96.68" end="98.27"/>
		<FORM kindOf="phono">ɯpɯ tɕe kusɯspoʁ ŋu ma</FORM>
	</S>
	<S id="S034">
		<AUDIO start="98.37" end="100.1"/>
		<FORM kindOf="phono">tɕɣom nɯnɯ kɯfse ŋu tɕeri</FORM>
	</S>
	<S id="S035">
		<AUDIO start="100.1" end="102.65"/>
		<FORM kindOf="phono">tɕɣom nɯ ɯŋgɯ nɯ ɯrɣi nɯnɯ</FORM>
	</S>
	<S id="S036">
		<AUDIO start="102.96" end="104.67"/>
		<FORM kindOf="phono">tɕɣom ɣɯ ɯqe tukɯti ŋu tɕe</FORM>
	</S>
	<S id="S037">
		<AUDIO start="105.0" end="106.58"/>
		<FORM kindOf="phono">nɯnɯ ɯqe nɯ pjɯ́wɣtɕɤt ra ma </FORM>
	</S>
	<S id="S038">
		<AUDIO start="106.58" end="108.66"/>
		<FORM kindOf="phono">nɯnɯ kɤrtɯm rlaʁrlaʁ ŋu tɕe, nɯnɯ </FORM>
	</S>
	<S id="S039">
		<AUDIO start="109.0" end="112.23"/>
		<FORM kindOf="phono">ɯrɣi ŋu ma kɤ-ndza maka (mɤ-khɯ) mɤ-sna</FORM>
	</S>
	<S id="S040">
		<AUDIO start="112.71" end="115.1"/>
		<FORM kindOf="phono">tɕe tsuku kɯ tɕe nɯ pjɯ-ɣndʑɯr-nɯ tɕe tɕe</FORM>
	</S>
	<S id="S041">
		<AUDIO start="115.42" end="118.91"/>
		<FORM kindOf="phono">nɯ ɯ-kri nɯnɯ li, nɤki, ndzɤtshi ɯ-cu pjɯ-lɤt ɲɯ-ŋu</FORM>
	</S>
	<S id="S042">
		<AUDIO start="119.64" end="123.48"/>
		<FORM kindOf="phono">tɕe nɯnɯ tɕɣom ŋu tɕe ri, nɯ ɯ-mdzu tu tɕe, wuma ʑo kɤ-phɯt sɤɣdɯɣ</FORM>
	</S>
	<S id="S043">
		<AUDIO start="124.27" end="127.18"/>
		<FORM kindOf="phono">tɕɣom nɯ ɯ-ru nɯ kɯ-ɤpɤpɣi kɯfse ŋu ma </FORM>
	</S>
	<S id="S044">
		<AUDIO start="127.18" end="129.62"/>
		<FORM kindOf="phono">kɯmaʁ nɯra kɯfse, ɯ-rqhu kɯjaʁ kɯfse me </FORM>
	</S>
	<S id="S045">
		<AUDIO start="130.52" end="131.6"/>
		<FORM kindOf="phono">tɕe nɯnɯ tɕɣom ŋu</FORM>
