	<S id="S001">
		<AUDIO start="0.04" end="5.33"/>
		<FORM kindOf="phono">﻿kɯɕɯŋgɯ tɕe iʑo ra ɣɯ ji-tɕhɤjlɯz nɯ, iɕqha nɤki,</FORM>
	</S>
	<S id="S002">
		<AUDIO start="5.33" end="8.37"/>
		<FORM kindOf="phono">kɤndʑɯsqhaj ɯ-rɟit nɯnɯ tɕe  kɤ-ndʑɯmɤtsa ŋu.</FORM>
	</S>
	<S id="S003">
		<AUDIO start="8.71" end="13.63"/>
		<FORM kindOf="phono">tɕe nɯnɯ kɤ-ndʑɯmɤtsa nɯni tɕe ci ku-pa-ndʑi kɯ-khɯ ɲɯ-ŋu, kɯ-ŋgrɤl ɲɯ-ŋu, mɤ-kɯ-ʁdɯɣ ɲɯ-ŋu.</FORM>
	</S>
	<S id="S004">
		<AUDIO start="14.61" end="18.19"/>
		<FORM kindOf="phono">a-pɯ-ŋu tɕe, kɤndʑɯwɤmɯsnom ɯ-rɟit nɯ a-pɯ-ŋu nɤ,</FORM>
	</S>
	<S id="S005">
		<AUDIO start="18.19" end="19.82"/>
		<FORM kindOf="phono">tɤ-wɤmɯ ɣɯ ɯ-rɟit nɯ tɕe,</FORM>
	</S>
	<S id="S006">
		<AUDIO start="20.36" end="23.0"/>
		<FORM kindOf="phono">nɯ maʁ nɤ, tɤ-ɬaʁ, nɯ maʁ nɤ, tɤ-rpɯ ɲɯ-ŋu tɕe,</FORM>
	</S>
	<S id="S007">
		<AUDIO start="23.4" end="25.64"/>
		<FORM kindOf="phono">ɯnɯni li ci ku-pa-ndʑi kɯ-khɯ ɲɯ-ŋu.</FORM>
	</S>
	<S id="S008">
		<AUDIO start="26.18" end="29.03"/>
		<FORM kindOf="phono">tɕeri nɯnɯ ɲɯ-rkɯn,</FORM>
	</S>
	<S id="S009">
		<AUDIO start="29.03" end="32.44"/>
		<FORM kindOf="phono">nɯ kɯ-fse ɲɯ-ŋu. nɤkinɯ,</FORM>
	</S>
	<S id="S010">
		<AUDIO start="32.63" end="36.5"/>
		<FORM kindOf="phono">kɤndʑɯxtɤɣ ɯ-rɟit nɯ tɤ-ŋu tɕe tɕe, nɤkinɯ,</FORM>
	</S>
	<S id="S011">
		<AUDIO start="36.63" end="38.39"/>
		<FORM kindOf="phono">ci kú-wɣ-pa maka mɤ-kɯ-khɯ ɲɯ-ŋu.</FORM>
	</S>
	<S id="S012">
		<AUDIO start="38.39" end="39.46"/>
		<FORM kindOf="phono">mɤʑɯ iɕqha nɤki</FORM>
	</S>
	<S id="S013">
		<AUDIO start="39.46" end="44.47"/>
		<FORM kindOf="phono">tɤ-βɣo cho tɤ-mdɯ ni ci kú-wɣ-pa mɤ-kɯ-khɯ ɲɯ-ŋu.</FORM>
	</S>
	<S id="S014">
		<AUDIO start="45.32" end="48.57"/>
		<FORM kindOf="phono">tɤ-rpɯ cho tɤ-ftsa ni ci kú-wɣ-pa mɤ-kɯ-khɯ ɲɯ-ŋu.</FORM>
	</S>
	<S id="S015">
		<AUDIO start="49.19" end="54.19"/>
		<FORM kindOf="phono">tɤ-ɬaʁ cho tɤ-ftsa ni li ci kú-wɣ-pa mɤ-kɯ-khɯ ɲɯ-ŋu.</FORM>
	</S>
	<S id="S016">
		<AUDIO start="54.74" end="59.69"/>
		<FORM kindOf="phono">tɤ-ɲi chondɤre li, nɯ ɣɯ, nɤki, ɯ-ʁi nɯ ci kú-wɣ-pamɤ-kɯ-khɯ ɲɯ-ŋu.</FORM>
	</S>
	<S id="S017">
		<AUDIO start="60.55" end="61.41"/>
		<FORM kindOf="phono">nɯra  kɯ-fse ɲɯ-ŋu.</FORM>
	</S>
	<S id="S018">
		<AUDIO start="61.85" end="66.93"/>
		<FORM kindOf="phono">tɤ-rpɯ cho ɯ-ftsa ni li ci kú-wɣ-pa mɤ-kɯ-khɯ ɲɯ-ŋu.</FORM>
	</S>
	<S id="S019">
		<AUDIO start="68.4" end="71.47"/>
		<FORM kindOf="phono">mɤ-kɯ-ŋgrɤl ɲɯ-ŋu</FORM>
