	<S id="S001">
		<AUDIO start="1.28" end="3.06"/>
		<FORM kindOf="phono">﻿mi nɯ ʁnɯ-tɯphu ɣɤʑu</FORM>
	</S>
	<S id="S002">
		<AUDIO start="4.0" end="5.63"/>
		<FORM kindOf="phono">mi nɯ ʁnɯ-tɯphu ɣɤʑu tɕe,</FORM>
	</S>
	<S id="S003">
		<AUDIO start="5.63" end="8.79"/>
		<FORM kindOf="phono">tɯ-tɯphu nɯ ɣɯ ɯ-mɯntoʁ ɣɤʑu, nɤki</FORM>
	</S>
	<S id="S004">
		<AUDIO start="8.79" end="10.2"/>
		<FORM kindOf="phono">ɯ-mɯntoʁ pɯ-ŋgra tɕe,</FORM>
	</S>
	<S id="S005">
		<AUDIO start="10.71" end="12.05"/>
		<FORM kindOf="phono">nɯnɯ ɯ-mɯntoʁ nɯ iɕqha,</FORM>
	</S>
	<S id="S006">
		<AUDIO start="12.05" end="14.34"/>
		<FORM kindOf="phono">ʑɴɢoloʁ kɯ-fse, ɯ-jndɯz kɯ-fse nɯ ɲɯ-ŋu tɕe,</FORM>
	</S>
	<S id="S007">
		<AUDIO start="14.88" end="18.42"/>
		<FORM kindOf="phono">ɯnɯnɯ, nɤkinɯ, ɯnɯnɯ nɯ-me, pɯ-ŋgra tɕe tɕe, </FORM>
	</S>
	<S id="S008">
		<AUDIO start="18.42" end="21.99"/>
		<FORM kindOf="phono">nɯ ɯ-sta ri, nɤkinɯ, ɯ-srɯn kɯ-fse ci ɲɯ-βze ɲɯ-ŋu tɕe,</FORM>
	</S>
	<S id="S009">
		<AUDIO start="22.47" end="24.57"/>
		<FORM kindOf="phono">iɕqha, ɯ-srɯn nɯ kɯ-fse ɲɯ-βze ɲɯ-ŋu tɕe,</FORM>
	</S>
	<S id="S010">
		<AUDIO start="25.07" end="27.76"/>
		<FORM kindOf="phono">nɯnɯ tɕe, ɯ-rɣi nɯ pjɯ-ŋgra ɯ-mɤ-ŋu ma tɕe </FORM>
	</S>
	<S id="S011">
		<AUDIO start="28.13" end="31.82"/>
		<FORM kindOf="phono">qale kɯ srɯn kɯ-fse nɯ aʁɤndɯndɤt ju-nɤtsɯmtsɯm ɲɯ-ŋu tɕe ɲɯ-dɤn. </FORM>
	</S>
	<S id="S012">
		<AUDIO start="31.82" end="34.92"/>
		<FORM kindOf="phono">tɯ-tɯphu nɯ tɕe nɯ kɯ-fse ɯ-mɯntoʁ ri maŋe,</FORM>
	</S>
	<S id="S013">
		<AUDIO start="35.25" end="36.77"/>
		<FORM kindOf="phono">ɯ-srɯn kɯ-fse nɯ ri maŋe.</FORM>
	</S>
	<S id="S014">
		<AUDIO start="37.44" end="40.17"/>
		<FORM kindOf="phono">tɕe mi nɯ nɯ ma mɤ-kɯ-naχtɕɯɣ,</FORM>
	</S>
	<S id="S015">
		<AUDIO start="40.17" end="43.81"/>
		<FORM kindOf="phono">ɯ-ru cho ɯ-jwaʁ nɯra kú-wɣ-rtoʁ ɲɯ-naχtɕɯ-χtɕɯɣ ʑo ɕti ri,</FORM>
	</S>
	<S id="S016">
		<AUDIO start="44.18" end="46.51"/>
		<FORM kindOf="phono">nɯnɯ ci nɯ ɣɯ ɯ-mɯntoʁ ɣɤʑu,</FORM>
	</S>
	<S id="S017">
		<AUDIO start="46.51" end="49.64"/>
		<FORM kindOf="phono">ɯ-jndɯz kɯ-fse nɯ ɣɤʑu, ɯ-srɯn kɯ-fse nɯ ɣɤʑu,</FORM>
	</S>
	<S id="S018">
		<AUDIO start="50.02" end="51.56"/>
		<FORM kindOf="phono">ci nɯ ɣɯ maka kɯ-tu maŋe.</FORM>
	</S>
	<S id="S019">
		<AUDIO start="52.22" end="54.68"/>
		<FORM kindOf="phono">tɕe nɯ, mi nɯ kɯnɤ ʁnɯ-tɯphu ɣɤʑu tɕe,</FORM>
	</S>
	<S id="S020">
		<AUDIO start="55.09" end="55.93"/>
		<FORM kindOf="phono">nɯ mɯ́j-naχtɕɯɣ.</FORM>
	</S>
	<S id="S021">
		<AUDIO start="56.75" end="59.59"/>
		<FORM kindOf="phono">nɯ ma ra ɲɯ-naχtɕɯɣ ɕti.</FORM>
	</S>
