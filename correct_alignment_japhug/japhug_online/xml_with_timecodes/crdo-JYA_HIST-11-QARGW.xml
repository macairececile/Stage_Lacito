	<S id="S001">
		<AUDIO start="0.16" end="3.11"/>
		<FORM kindOf="phono">﻿qarɣɯ nɯ sɯku ci ŋu tɕe, nɤkinɯ,</FORM>
	</S>
	<S id="S002">
		<AUDIO start="3.19" end="4.19"/>
		<FORM kindOf="phono">rɟaspa mbro.</FORM>
	</S>
	<S id="S003">
		<AUDIO start="4.74" end="8.53"/>
		<FORM kindOf="phono">tɕeri, ɯʑosti tu-nɯ-ɬoʁ tɕe nɯ sthɯci tu-mbro mɤ-cha</FORM>
	</S>
	<S id="S004">
		<AUDIO start="9.49" end="14.91"/>
		<FORM kindOf="phono">tɕeri, nɤki, kɯmaʁ si kɯ-mbro ra nɯ-taʁ ʑo tɕe tu-nɯrʁɯrʁa tɕe tɕe taʁ tsa tu-ɕe cha.</FORM>
	</S>
	<S id="S005">
		<AUDIO start="15.79" end="18.99"/>
		<FORM kindOf="phono">qarɣɯ nɯ rcanɯ ɯ-mdzu ɯ-tɯ-sɤndza saχaʁ.</FORM>
	</S>
	<S id="S006">
		<AUDIO start="19.82" end="22.07"/>
		<FORM kindOf="phono">tɕe ɯ-mdzu ɣɯ ɯ-ku ɯ-kɯ-ɤmtɕoʁ nɯreri,</FORM>
	</S>
	<S id="S007">
		<AUDIO start="22.07" end="23.39"/>
		<FORM kindOf="phono">ki kɯ-fse lu-ŋgɤɣ ŋu.</FORM>
	</S>
	<S id="S008">
		<AUDIO start="24.01" end="26.64"/>
		<FORM kindOf="phono">ki tu-fse lu-ŋgɤɣ ŋu qhe, tɕe nɯnɯ tɯ-taʁ kɤ-atsa qhe,</FORM>
	</S>
	<S id="S009">
		<AUDIO start="26.64" end="29.01"/>
		<FORM kindOf="phono">ki chɯ-ste qhe, chɯ-rɤɕi ʑo cha.</FORM>
	</S>
	<S id="S010">
		<AUDIO start="29.77" end="33.12"/>
		<FORM kindOf="phono">tɕe rca, tɯ-ɕa rca, kɯ-rɲɟi qhe, kɯ-rnaʁ ʑo chɯ-pri ɲɯ-ŋu. </FORM>
	</S>
	<S id="S011">
		<AUDIO start="34.07" end="37.7"/>
		<FORM kindOf="phono">tɕe qarɣɯ ɣɯ ɯ-jwaʁ nɯnɯ lu-oʑɯrja kɯ-fse ŋu.</FORM>
	</S>
	<S id="S012">
		<AUDIO start="38.68" end="41.37"/>
		<FORM kindOf="phono">tɕe nɯ qarɣɯ nɯ ŋu tɕe,</FORM>
	</S>
	<S id="S013">
		<AUDIO start="41.44" end="47.29"/>
		<FORM kindOf="phono">qarɣɯ nɯ ɯ-ru ɯ-taʁ tɕi ɯ-mdzu tu, ɯ-jwaʁ ɯ-taʁ tɕi ɯ-mdzu tu,</FORM>
	</S>
	<S id="S014">
		<AUDIO start="47.49" end="48.84"/>
		<FORM kindOf="phono">ɯ-rtaʁ ɯ-taʁ tɕi ɯ-mdzu tu.</FORM>
	</S>
	<S id="S015">
		<AUDIO start="49.51" end="51.14"/>
		<FORM kindOf="phono">tɕeri ɯ-mɯntoʁ wuma ʑo mpɕɤr.</FORM>
	</S>
	<S id="S016">
		<AUDIO start="51.97" end="57.34"/>
		<FORM kindOf="phono">ɯ-mɯntoʁ kɯ-wɣrum tu, kɯ-ɣɯrni tu.</FORM>
	</S>
	<S id="S017">
		<AUDIO start="58.33" end="62.62"/>
		<FORM kindOf="phono">tɕe ɯ-mɯntoʁ wuma ʑo mpɕɤr.</FORM>
	</S>
	<S id="S018">
		<AUDIO start="63.34" end="67.09"/>
		<FORM kindOf="phono">tɕe nɯ mɤ-sna me tɕe ɯ-ru kɯ-jpum nɯra chɯ́-wɣ-nɯ-βlɯ sna.</FORM>
	</S>
	<S id="S019">
		<AUDIO start="67.36" end="69.92"/>
		<FORM kindOf="phono">kɯmaʁ nɯra ɯ-mdzu kɯ-tu nɯra tɤɣur sna.</FORM>
	</S>
	<S id="S020">
		<AUDIO start="69.92" end="72.36"/>
		<FORM kindOf="phono">nɯ ma kɯ-sna ci me.</FORM>
	</S>
	<S id="S021">
		<AUDIO start="73.36" end="74.69"/>
