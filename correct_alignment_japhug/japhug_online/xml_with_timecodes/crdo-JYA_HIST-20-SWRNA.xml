	<S id="S001">
		<AUDIO start="0.89" end="4.35"/>
		<FORM kindOf="phono">﻿sɯrna kɤ-ti ci tu tɕe, nɯnɯ </FORM>
	</S>
	<S id="S002">
		<AUDIO start="4.43" end="5.91"/>
		<FORM kindOf="phono">kupa kɯ muer tu-ti ɲɯ-ŋu.</FORM>
	</S>
	<S id="S003">
		<AUDIO start="5.91" end="10.79"/>
		<FORM kindOf="phono">nɯnɯ ɯ-tun nɯ ɲɯ-pe, ɲɯ-sɤstu.</FORM>
	</S>
	<S id="S004">
		<AUDIO start="11.39" end="12.81"/>
		<FORM kindOf="phono">ma nɯnɯ kɤ-ti ci tu tɕe,</FORM>
	</S>
	<S id="S005">
		<AUDIO start="12.81" end="15.84"/>
		<FORM kindOf="phono">nɯnɯ ndɤre si ɯ-phoŋbu ɯ-taʁ ʁɟa tu-ɬoʁ ɕti.</FORM>
	</S>
	<S id="S006">
		<AUDIO start="16.3" end="17.22"/>
		<FORM kindOf="phono">ɯ-ru ɯ-taʁ tu-ɬoʁ </FORM>
	</S>
	<S id="S007">
		<AUDIO start="17.72" end="19.72"/>
		<FORM kindOf="phono">iɕqha si thɯ-kɯ-ndʐaβ tɕe,</FORM>
	</S>
	<S id="S008">
		<AUDIO start="20.13" end="23.53"/>
		<FORM kindOf="phono">ʑatsa kɯ-tsɣi tɤ-kɯ-rɤŋgat kɯ-fse nɯra nɯ-taʁ ʁɟa tu-ɬoʁ ɲɯ-ŋu.</FORM>
	</S>
	<S id="S009">
		<AUDIO start="23.53" end="25.37"/>
		<FORM kindOf="phono">ma ɯ-thoʁ tu-ɬoʁ ɲɯ-maʁ.</FORM>
	</S>
	<S id="S010">
		<AUDIO start="25.99" end="28.8"/>
		<FORM kindOf="phono">grɯβgrɯβ nɯ tɕe, ɯ-thoʁ tu-ɬoʁ ɲɯ-ŋu.</FORM>
	</S>
	<S id="S011">
		<AUDIO start="28.8" end="30.77"/>
		<FORM kindOf="phono">sɤtɕha ɯ-taʁ tu-ɬoʁ ɲɯ-ŋu,</FORM>
	</S>
	<S id="S012">
		<AUDIO start="30.83" end="33.45"/>
		<FORM kindOf="phono">sɯrna nɯ tɕe, si ɯ-phoŋbu ɯ-taʁ tu-ɬoʁ ɲɯ-ŋu tɕe,</FORM>
	</S>
	<S id="S013">
		<AUDIO start="33.89" end="36.4"/>
		<FORM kindOf="phono">ɯ-tshɯɣa ra tsuku tɯ-rna  wuma ʑo ɲɯ-fse tɕe,</FORM>
	</S>
	<S id="S014">
		<AUDIO start="36.77" end="39.46"/>
		<FORM kindOf="phono">tɕe sɯrna tu-ti-nɯ ŋu ma nɯnɯ, nɤkinɯ</FORM>
	</S>
	<S id="S015">
		<AUDIO start="39.46" end="42.15"/>
		<FORM kindOf="phono">ɯʑo tɤ-kɯ-nɯ-ɬoʁ nɯnɯ tɕe,</FORM>
	</S>
	<S id="S016">
		<AUDIO start="42.58" end="44.4"/>
		<FORM kindOf="phono">wuma ʑo mɯm, tú-wɣ-ndza tɕe mpɯ.</FORM>
	</S>
	<S id="S017">
		<AUDIO start="45.01" end="50.95"/>
		<FORM kindOf="phono">tɕe nɯnɯ mɯm ma kɯkɯreri, ʑara kɯ pɯ-kɤ-nɯ-ji ci ɣɤʑu tɕe,</FORM>
	</S>
	<S id="S018">
		<AUDIO start="50.95" end="52.04"/>
		<FORM kindOf="phono">nɯ ndɤre mɯ́j-mɯm.</FORM>
	</S>
	<S id="S019">
		<AUDIO start="52.52" end="55.97"/>
		<FORM kindOf="phono">tɤ... ku-smi mɯ́j-cha  tú-wɣ-ndza ɕkrɯɣnɤɕkrɯɣ ntsɯ ɲɯ-nɯ-ti ɕti.</FORM>
	</S>
	<S id="S020">
		<AUDIO start="55.97" end="61.1"/>
		<FORM kindOf="phono">ma nɯnɯ sɯrna ŋu tɕe, thamtham iʑora nɯ kɯ-fse z-ɲɯ-ɕar-nɯ </FORM>
	</S>
	<S id="S021">
		<AUDIO start="61.47" end="64.16"/>
		<FORM kindOf="phono">li nɯnɯ kɯnɤ ɯ-kɯ-mto ɣɤʑu, ɯ-mɤ-kɯ-mto ɣɤʑu.</FORM>
	</S>
	<S id="S022">
		<AUDIO start="64.67" end="66.91"/>
		<FORM kindOf="phono">nɯnɯ li ci, ɯ-kɤ-nɯ-ɕɯftaʁ tɕe, </FORM>
	</S>
	<S id="S023">
		<AUDIO start="67.26" end="70.57"/>
		<FORM kindOf="phono">ɯ-sɤtɕha ɣɤʑu tɕe, nɯ tɕu ju-ɕe-nɯ ɲɯ-ŋu.</FORM>
	</S>
	<S id="S024">
		<AUDIO start="70.57" end="72.08"/>
		<FORM kindOf="phono">tɕe z-ɲɯ-nɯ-ɕar-nɯ ɲɯ-ŋu ma </FORM>
	</S>
	<S id="S025">
		<AUDIO start="72.58" end="75.53"/>
		<FORM kindOf="phono">iʑo a-ʁi waŋtɕin ɣɯ ɯ-rʑaβ nɯnɯ kɯ wuma ʑo mtɤm.</FORM>
	</S>
	<S id="S026">
		<AUDIO start="76.17" end="78.3"/>
		<FORM kindOf="phono">tɯ-xpa tɕe kɯ-dɯ-dɤn ʑo ɲɯ-sɤjti ɕti.</FORM>
	</S>
	<S id="S027">
		<AUDIO start="79.21" end="81.29"/>
		<FORM kindOf="phono">tɕe ma nɯnɯ ɲɯ́-wɣ-mbi-a ra ŋgrɤl.</FORM>
	</S>
	<S id="S028">
		<AUDIO start="82.02" end="84.56"/>
		<FORM kindOf="phono">a-kɤ-ndza ɲɯ́-wɣ-mbi-a ra ŋgrɤl.</FORM>
	</S>
	<S id="S029">
		<AUDIO start="84.56" end="87.1"/>
		<FORM kindOf="phono">"nɯ-ntsɣe ra" tu-ti tɕe, ɲɯ-ntsɣe-a ra ŋgrɤl. </FORM>
	</S>
	<S id="S030">
		<AUDIO start="87.67" end="89.16"/>
		<FORM kindOf="phono">ma ɣɯjpa pɤjkhu mɯ-tha-ɣɯt.</FORM>
	</S>
	<S id="S031">
		<AUDIO start="90.08" end="93.42"/>
		<FORM kindOf="phono">ma nɯnɯ sɯrna ŋu tɕe, kɯ-ɲaʁ ci ŋu.</FORM>
	</S>
	<S id="S032">
		<AUDIO start="93.42" end="96.39"/>
		<FORM kindOf="phono">kɯ-ɲaʁ ŋu, kɯmɯxte nɯ kɯ-ɲaʁ tsa kɯ-fse ŋu, </FORM>
	</S>
	<S id="S033">
		<AUDIO start="96.83" end="100.19"/>
		<FORM kindOf="phono">kɯ-qandʐi kɯ-fse, mɤʑɯ kɯ-xtɕɯ-xtɕi kɯ-wɣrum kɯ-fse.</FORM>
	</S>
	<S id="S034">
		<AUDIO start="100.6" end="101.08"/>
		<FORM kindOf="phono">nɯra tu.</FORM>
	</S>
	<S id="S035">
		<AUDIO start="101.08" end="104.49"/>
		<FORM kindOf="phono">ma nɯnɯ sɯrna ŋu.</FORM>
	</S>
	<S id="S036">
		<AUDIO start="104.99" end="108.12"/>
		<FORM kindOf="phono">tɕe nɯnɯ nɤkinɯ, ɯ-phu ɲɯ-wxti.</FORM>
	</S>
	<S id="S037">
		<AUDIO start="108.7" end="111.02"/>
		<FORM kindOf="phono">ɯ-kɯ-nɯ-rga ɲɯ-dɤn tɕe, "smɤn ŋu" ɲɯ-ti-nɯ tɕe</FORM>
	</S>
	<S id="S038">
		<AUDIO start="111.02" end="112.19"/>
		<FORM kindOf="phono">tɕhi smɤn ŋu mɤxsi.</FORM>
	</S>
	<S id="S039">
		<AUDIO start="113.03" end="115.29"/>
		<FORM kindOf="phono">ma nɯnɯ iʑora i-sɤtɕha dɤn qhe,</FORM>
	</S>
	<S id="S040">
		<AUDIO start="115.8" end="118.04"/>
		<FORM kindOf="phono">kɯ-rtaʁ ra kɯ ɲɯ́-wɣ-sɯɣrom qhe ɲɯ́-wɣ-nɯ-ntsɣe </FORM>
	</S>
	<S id="S041">
		<AUDIO start="118.04" end="121.38"/>
		<FORM kindOf="phono">mɤ-kɯ-rtaʁ nɯra pɯ́-wɣ-mto qhe tɕe tú-wɣ-nɯ-ndza ɕti qhe, </FORM>
	</S>
	<S id="S042">
		<AUDIO start="121.38" end="122.35"/>
		<FORM kindOf="phono">nɯ ma sɯz-a me.</FORM>
	</S>
	<S id="S043">
		<AUDIO start="123.31" end="124.53"/>
		<FORM kindOf="phono">tɕe nɯnɯ sɯrna ŋu.</FORM>
	</S>
	<NOTE message="===============" xmlns:lang="fr"/>
	<S id="S044">
		<AUDIO start="130.59" end="134.56"/>
		<FORM kindOf="phono">khɯrwum nɯnɯ ndɤre, nɤki, kɤ-ndzɤtshi nɯra,</FORM>
	</S>
	<S id="S045">
		<AUDIO start="135.08" end="140.08"/>
		<FORM kindOf="phono">kɯmɯxte iɕqha cai tɤ-mthɯm kɯ-fse nɯnɯra tɯ-mɲɤt na-ʑa tɕe,</FORM>
	</S>
	<S id="S046">
		<AUDIO start="140.08" end="141.89"/>
		<FORM kindOf="phono">ɯ-taʁ ri kɯ-wɣrum ci ku-te ŋu tɕe,</FORM>
	</S>
	<S id="S047">
		<AUDIO start="142.44" end="145.02"/>
		<FORM kindOf="phono">kɯ-wɣrum ci ku-te ŋu tɕe, nɯnɯ khɯrwum tu-kɯ-ti ŋu.</FORM>
	</S>
	<S id="S048">
		<AUDIO start="145.54" end="150.06"/>
		<FORM kindOf="phono">ɯnɯnɯ ka-ta qhe tɕe nɯra kɤ-ndzɤtshi nɯra, kɤ-ndza mɯ-ɲɯ-sna ɕti.</FORM>
	</S>
	<S id="S049">
		<AUDIO start="150.44" end="152.4"/>
		<FORM kindOf="phono">ma tɕe ɯ-di tu tɕe, chɯ-βde-nɯ ɕti. </FORM>
	</S>
	<S id="S050">
		<AUDIO start="153.06" end="154.89"/>
		<FORM kindOf="phono">ma tú-wɣ-ndza tɕe,</FORM>
	</S>
	<S id="S051">
		<AUDIO start="154.89" end="156.16"/>
		<FORM kindOf="phono">(??) ɲɯ-kɯ-z-nɯtɯfɕɤl ɕti</FORM>
	</S>
	<S id="S052">
		<AUDIO start="156.98" end="159.51"/>
		<FORM kindOf="phono">tɕe ɯnɯnɯ mɤ-pe tɕe nɯ khɯrwum ŋu tɕe,</FORM>
	</S>
	<S id="S053">
		<AUDIO start="159.94" end="162.84"/>
		<FORM kindOf="phono">nɯnɯ khɯrwum nɯnɯ aʁɤndɯndɤt ʑo tu ɕti.</FORM>
	</S>
	<S id="S054">
		<AUDIO start="163.53" end="168.93"/>
		<FORM kindOf="phono">nɯnɯ tɕe, a-pɯ-ŋu, tɤjko nɯ-mɲɤt qhe ɯ-taʁ ri li kɯ-wɣrum ku-te qhe nɯ khɯrwum ŋu.</FORM>
	</S>
	<S id="S055">
		<AUDIO start="169.42" end="172.52"/>
		<FORM kindOf="phono">tɤ-mthɯm kɯnɤ kɤ-kɤ-sqa nɯra ʑatsa ma-tɤ́-wɣ-nɯβdaʁ qhe, </FORM>
	</S>
	<S id="S056">
		<AUDIO start="172.83" end="178.07"/>
		<FORM kindOf="phono">ɯ-taʁ ri kɯ-wɣrum ku-te, ku-kɯ-ta ci tu qhe, nɯnɯ li khɯrwum ŋu</FORM>
	</S>
	<S id="S057">
		<AUDIO start="178.07" end="183.25"/>
		<FORM kindOf="phono">tɕe nɤkinɯ, si kɯnɤ rom tɤkha tɕe tɕe li ɯ-taʁ kɯ-wɣrum ku-te qhe,</FORM>
	</S>
	<S id="S058">
		<AUDIO start="183.63" end="184.93"/>
		<FORM kindOf="phono">nɯnɯ li khɯrwum ɕti.</FORM>
	</S>
	<S id="S059">
		<AUDIO start="185.78" end="188.51"/>
		<FORM kindOf="phono">qhe nɯnɯ li khɯrwum nɯnɯ, sɤz nɤkinɯ,</FORM>
	</S>
	<S id="S060">
		<AUDIO start="189.15" end="191.83"/>
		<FORM kindOf="phono">iɕqha kɤ-ndza me, laχtɕha me nɯnɯ,</FORM>
	</S>
	<S id="S061">
		<AUDIO start="192.27" end="194.31"/>
		<FORM kindOf="phono">nɯnɯ kɯ-mɲɤt tɤ-mda qhe,</FORM>
	</S>
	<S id="S062">
		<AUDIO start="194.79" end="197.0"/>
		<FORM kindOf="phono">kɯ-tsɣi tɤ-rɤŋgat qhe, nɯ kɯ-fse ku-te ŋu.</FORM>
	</S>
	<S id="S063">
		<AUDIO start="197.64" end="201.26"/>
		<FORM kindOf="phono">yangyu kɯnɤ, tsɣi tɤkha qhe ɯ-taʁ ri kɯ-wɣrum ku-te qhe,</FORM>
	</S>
	<S id="S064">
		<AUDIO start="201.55" end="203.0"/>
		<FORM kindOf="phono">tɕe nɯnɯ li khɯrwum ɕti,</FORM>
	</S>
	<S id="S065">
		<AUDIO start="203.89" end="204.98"/>
		<FORM kindOf="phono">tɕe nɯ khɯrwum ŋu.</FORM>
	</S>
