	<S id="S090">
		<AUDIO start="273.15" end="274.97"/>
		<FORM kindOf="phono">nɯnɯ ɯ-ŋgɯ ri ku-mphɯr-nɯ pɯ-ŋgrɤl.</FORM>
	</S>
