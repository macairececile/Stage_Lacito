	<S id="S001">
		<AUDIO start="0.38" end="5.22"/>
		<FORM kindOf="phono">﻿βɣɤrtshi chondɤre ʁmɤrɲɯɣ ni ʁnaʁna ndʑi-mɤlɤjaʁ ra kɯ-xtshɯ-xtshɯm ʑo qhe </FORM>
	</S>
	<S id="S002">
		<AUDIO start="5.22" end="6.91"/>
