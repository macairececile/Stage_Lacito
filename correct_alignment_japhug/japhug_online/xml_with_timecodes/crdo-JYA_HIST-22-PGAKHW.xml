<?xml version="1.0" ?>
<!DOCTYPE TEXT SYSTEM "https://cocoon.huma-num.fr/schemas/Archive.dtd">
<TEXT id="crdo-JYA_HIST-22-PGAKHW" xml:lang="jya">
	<HEADER>
		<TITLE>crdo-JYA_HIST-22-PGAKHW</TITLE>
		<SOUNDFILE href="hist-22-pGAkhW.wav"/>
	</HEADER>
	<S id="S001">
		<AUDIO start="0.96" end="2.51"/>
		<FORM kindOf="phono">﻿pɣɤkhɯ kɤ-ti ci tu.</FORM>
	</S>
	<S id="S002">
		<AUDIO start="3.04" end="6.54"/>
		<FORM kindOf="phono">tɕe pɣɤkhɯ nɯnɯ li pɣa ɲɯ-ŋu tɕeri nɤkinɯ,</FORM>
	</S>
	<S id="S003">
		<AUDIO start="7.01" end="10.14"/>
		<FORM kindOf="phono">pɣɤkhɯ nɯnɯ, nɤkinɯ, ɕɤr ntsɯ tu-mbri ŋu.</FORM>
	</S>
	<S id="S004">
		<AUDIO start="10.98" end="13.95"/>
		<FORM kindOf="phono">ɕɤr kɤ-armɤjɯβ ɯ-qhu jamar ri tɕe tɕe tu-mbri ŋu tɕe,</FORM>
	</S>
	<S id="S005">
		<AUDIO start="14.31" end="15.91"/>
		<FORM kindOf="phono">tɤpɤtso ra ɲɯ-ɕɯɣme ŋgrɤl</FORM>
	</S>
	<S id="S006">
		<AUDIO start="16.46" end="18.09"/>
		<FORM kindOf="phono">ma nɯnɯ pɣɤkhɯ ŋu tɕe,</FORM>
	</S>
	<S id="S007">
		<AUDIO start="18.16" end="21.88"/>
		<FORM kindOf="phono">pɣɤkhɯ nɯ ɯ-ku nɯnɯ lɯlu tsa ɲɯ-fse, ɯ-mtsioʁ ɣɤʑu ma </FORM>
	</S>
	<S id="S008">
		<AUDIO start="22.2" end="24.37"/>
		<FORM kindOf="phono">kɤ́rnɯrna lɯlu tu-fse ɲɯ-sɤre ʑo.</FORM>
	</S>
	<S id="S009">
		<AUDIO start="24.83" end="27.24"/>
		<FORM kindOf="phono">tɕe núndʐa kupa kɯ matouying tu-ti ŋu.</FORM>
	</S>
	<S id="S010">
		<AUDIO start="27.95" end="30.4"/>
		<FORM kindOf="phono">ma nɯnɯ kɯ kuwu ŋu tɕe kuwu nɯ li, </FORM>
	</S>
	<S id="S011">
		<AUDIO start="30.85" end="36.02"/>
		<FORM kindOf="phono">rɯdaʁ kɯ-fse kɯ-xtɕi nɯra tu-ndze ɲɯ-ɕti tɕe,</FORM>
	</S>
	<S id="S012">
		<AUDIO start="36.39" end="40.16"/>
		<FORM kindOf="phono">ɯ-ndzrɯ tu, ɯ-mtsioʁ kɯnɤ, nɤki, ɲɯ-ŋgɤɣ</FORM>
	</S>
	<S id="S013">
		<AUDIO start="40.81" end="44.74"/>
		<FORM kindOf="phono">ɯ-ku ri ɯ-jŋoʁ kɯ-fse ɣɤʑu ma, nɤkinɯ,</FORM>
	</S>
	<S id="S014">
		<AUDIO start="45.2" end="47.84"/>
		<FORM kindOf="phono">ɕɤr tɕe, nɤkinɯ, βʑɯ tu-ndze,</FORM>
	</S>
	<S id="S015">
		<AUDIO start="48.31" end="51.11"/>
		<FORM kindOf="phono">qaɲi nɯra ɕɤr thɯ-ɬoʁ tɕe nɯra tu-ndze.</FORM>
	</S>
	<S id="S016">
		<AUDIO start="51.59" end="56.86"/>
		<FORM kindOf="phono">mɤʑɯ iɕqha βɣɯz kɯ-fse nɯra ɕɤr ju-kɯ-ɬoʁ tu-kɯ-rɤma nɯra tu-ndze ɲɯ-ŋu tɕe, </FORM>
	</S>
	<S id="S017">
		<AUDIO start="56.86" end="60.2"/>
		<FORM kindOf="phono">tɕe nɯ ɯʑo kɯnɤ ɕɤr tɕe chɯ-ɬoʁ tɕe tu-rɤma ɲɯ-ŋu</FORM>
	</S>
	<S id="S018">
		<AUDIO start="60.88" end="62.38"/>
		<FORM kindOf="phono">sŋi tɕe kɤ-mto maka me.</FORM>
	</S>
	<S id="S019">
		<AUDIO start="63.01" end="65.53"/>
		<FORM kindOf="phono">ɕɤr tɕe tu ŋgrɤl, tu-mbri ŋgrɤl.</FORM>
	</S>
	<S id="S020">
		<AUDIO start="66.32" end="68.92"/>
		<FORM kindOf="phono">ma pɯ-kɤ-sat nɯ kɤ-mto nɯ pɯ-rɲo-t-a.</FORM>
	</S>
	<S id="S021">
		<AUDIO start="69.83" end="72.22"/>
		<FORM kindOf="phono">ma ɯ-mdoʁ nɯnɯ kafei ɯ-mdoʁ ɲɯ-ŋu</FORM>
	</S>
	<S id="S022">
		<AUDIO start="73.19" end="78.08"/>
		<FORM kindOf="phono">ma kɯ-pɣi qhe kɯ-ɤɲaʁndzɯm kɯ-fse nɯ kafei ɯ-mdoʁ wuma ʑo ɲɯ-ŋu</FORM>
	</S>
	<S id="S023">
		<AUDIO start="78.08" end="79.91"/>
		<FORM kindOf="phono">ma nɯnɯ kuwu ŋu.</FORM>
	</S>
	<S id="S024">
		<AUDIO start="80.5" end="84.29"/>
		<FORM kindOf="phono">tɕe ɯ-ndzrɯ nɯra li kɯmaʁ pɣa ra kɯ-kɯ-fse ʑo fse qhe,</FORM>
	</S>
	<S id="S025">
		<AUDIO start="84.29" end="86.16"/>
		<FORM kindOf="phono">ɯ-ʁar tu qhe ɲɯ-nɯqambɯmbjom ɕti.</FORM>
	</S>
	<S id="S026">
		<AUDIO start="86.74" end="90.86"/>
		<FORM kindOf="phono">tɕeri ɯ-ku nɯnɯ lɯlu ɯ-ku ɲɯ-fse tɕe, núndʐa maotouying tu-ti-nɯ ŋu.</FORM>
	</S>
	<S id="S027">
		<AUDIO start="91.84" end="93.88"/>
		<FORM kindOf="phono">ma iʑora nɯ pɣɤkhɯ ma ɯ-rmi me.</FORM>
	</S>
	<S id="S028">
		<AUDIO start="95.22" end="98.6"/>
		<FORM kindOf="phono">tɕe tsuku tɯrme ra kɤ-rɤma wuma ʑo kɯ-znɤʁamɟa tɕe</FORM>
	</S>
	<S id="S029">
		<AUDIO start="98.6" end="101.05"/>
		<FORM kindOf="phono">soz lu-fsoʁ ɕɯŋgɯ tu-rɤma-nɯ,</FORM>
	</S>
	<S id="S030">
		<AUDIO start="101.47" end="103.46"/>
		<FORM kindOf="phono">ɕɤr kɤ-qanɯ kɯnɤ ɲɯ-rɤma-nɯ tɕe,</FORM>
	</S>
	<S id="S031">
		<AUDIO start="103.46" end="105.52"/>
		<FORM kindOf="phono">"nɤʑo pɣɤkhɯ ʑo ɲɯ-tɯ-fse" tu-ti-nɯ ŋgrɤl.</FORM>
	</S>
	<S id="S032">
		<AUDIO start="107.09" end="110.95"/>
		<FORM kindOf="phono">tɕe nɯnɯ pɣɤkhɯ ŋu.</FORM>
	</S>
</TEXT>