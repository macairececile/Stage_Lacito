	<S id="S001">
		<AUDIO start="0.38" end="2.3"/>
		<FORM kindOf="phono">﻿ʁmɯrcɯftsa kɤ-ti ci tu tɕe </FORM>
	</S>
	<S id="S002">
		<AUDIO start="2.3" end="4.47"/>
		<FORM kindOf="phono">nɯnɯ ʁmɯrcɯftsa kɯ-xtɕɯ-xtɕi ʑo wxti.</FORM>
	</S>
	<S id="S003">
		<AUDIO start="5.24" end="7.93"/>
		<FORM kindOf="phono">tɕeri nɯnɯ ɯ-mtsioʁ nɯ ki tu-fse ɯ-qhu tɕethi, </FORM>
	</S>
	<S id="S004">
		<AUDIO start="7.93" end="9.08"/>
		<FORM kindOf="phono">ɯ-ŋgɯ ri lu-ŋgɤɣ ŋu</FORM>
	</S>
	<S id="S005">
		<AUDIO start="9.9" end="11.22"/>
		<FORM kindOf="phono">tɕe nɯnɯ rcanɯ</FORM>
	</S>
	<S id="S006">
		<AUDIO start="11.61" end="13.65"/>
		<FORM kindOf="phono">χpɤltɕin ɯ-mu kɯ wuma ʑo qhe ma</FORM>
	</S>
	<S id="S007">
		<AUDIO start="14.06" end="16.48"/>
		<FORM kindOf="phono">yangyu lɤ-kɤ-ji kɯnɤ tu-nɯ-tɕɤt ɕti ɲɯ-ti.</FORM>
	</S>
	<S id="S008">
		<AUDIO start="17.11" end="20.81"/>
		<FORM kindOf="phono">yangyu ra tu-nɯ-tɕɤt, jima pɯ-kɤ-ji nɯra kɯnɤ tu-nɯ-tɕɤt.</FORM>
	</S>
	<S id="S009">
		<AUDIO start="21.58" end="25.15"/>
		<FORM kindOf="phono">tɕeri tɤrɤku, tɯji ɯ-ŋgɯ wuma ʑo saʁnɤt tu-ti tɕe qhe ma</FORM>
	</S>
	<S id="S010">
		<AUDIO start="25.75" end="28.6"/>
		<FORM kindOf="phono">nɯnɯ nɤkinɯ, li kha ra kɯ-nɤru ju-ɣi. </FORM>
	</S>
	<S id="S011">
		<AUDIO start="29.03" end="31.18"/>
		<FORM kindOf="phono">tɯji ɯ-ŋgɯ ra ku-nɤru ɕti.</FORM>
	</S>
	<S id="S012">
		<AUDIO start="31.66" end="33.98"/>
		<FORM kindOf="phono">tɕ qajɯ tɕi ɲɯ-ndze, nɤki, </FORM>
	</S>
	<S id="S013">
		<AUDIO start="33.98" end="39.65"/>
		<FORM kindOf="phono">tɯjpu tɕi ɲɯ-ndze, tɕe ʁmɯrcɯftsa ŋu tɕe nɯnɯ nɤkinɯ, </FORM>
	</S>
	<S id="S014">
		<AUDIO start="39.65" end="42.46"/>
		<FORM kindOf="phono">ʁmɯrcɯ ɯ-rcɯrca ʑo tu ri,</FORM>
	</S>
	<S id="S015">
		<AUDIO start="42.76" end="46.22"/>
		<FORM kindOf="phono">ʁmɯrcɯ sthɯci mɤ-dɤn, ʁmɯrcɯ sɤznɤ rkɯn</FORM>
	</S>
	<S id="S016">
		<AUDIO start="46.73" end="48.64"/>
		<FORM kindOf="phono">nɯnɯ sɯŋgɯ kɯmɯxte ɕ-ku-rɤʑi ɲɯ-ŋu</FORM>
	</S>
	<S id="S017">
		<AUDIO start="49.31" end="50.8"/>
		<FORM kindOf="phono">nɯnɯ ɣɯ ɯ-ŋgɯm nɯnɯ </FORM>
	</S>
	<S id="S018">
		<AUDIO start="51.21" end="54.26"/>
		<FORM kindOf="phono">nɯ, kɯ-pɣi kɯ-fse qhe, ɯ-taʁ kɯ-ɲaʁ kɯ-ɤkhra kɯ-fse ɲɯ-ŋu.</FORM>
	</S>
	<S id="S019">
		<AUDIO start="55.03" end="56.71"/>
		<FORM kindOf="phono">nɯnɯ ɣɯ ɯ-ŋgɯm nɯ nɯ kɯ-fse ɲɯ-ŋu</FORM>
	</S>
	<S id="S020">
		<AUDIO start="56.71" end="58.04"/>
		<FORM kindOf="phono">ʁmɯrcɯ ɣɯ jamar ɲɯ-wxti.</FORM>
	</S>
	<S id="S021">
		<AUDIO start="58.79" end="60.84"/>
		<FORM kindOf="phono">ɯʑo nɯ ʁmɯrcɯ sɤz kɯ-xtɕɯ-xtɕi wxti.</FORM>
	</S>
	<S id="S022">
		<AUDIO start="62.43" end="64.21"/>
		<FORM kindOf="phono">tɕe ɯ-mtsioʁ nɯra kɯ-qarŋe ɲɯ-ŋu.</FORM>
	</S>
	<S id="S023">
		<AUDIO start="66.02" end="69.32"/>
		<FORM kindOf="phono">ɯ-mi nɯra kɯnɤ kɯ-qarŋe ɯ-mɤ-ŋu ma mɯ-kɤ-rtoʁ-a ri,</FORM>
	</S>
	<S id="S024">
		<AUDIO start="69.81" end="70.94"/>
		<FORM kindOf="phono">nɯnɯ ʁmɯrcɯftsa ŋu.</FORM>
	</S>
	<S id="S025">
		<AUDIO start="71.7" end="74.87"/>
		<FORM kindOf="phono">tɕe ndʑi-tezheng nɯra, nɤkinɯ,</FORM>
	</S>
	<S id="S026">
		<AUDIO start="75.0" end="77.57"/>
		<FORM kindOf="phono">ʁmɯrcɯ cho naχtɕɯɣ tsa ɕti, ndʑi-tshɯɣa.</FORM>
	</S>
	<NOTE message="=============================" xmlns:lang="fr"/>
	<S id="S027">
		<AUDIO start="93.0" end="96.7"/>
		<FORM kindOf="phono">qale kɯtshi kɤ-ti ci tu.</FORM>
	</S>
	<S id="S028">
		<AUDIO start="96.7" end="99.49"/>
		<FORM kindOf="phono">tɤrpat (ɯ-mdoʁ),  kafei ɯ-mdoʁ tsa ɯ-kɯ-ndo ci ŋu. </FORM>
	</S>
	<S id="S029">
		<AUDIO start="100.33" end="103.08"/>
		<FORM kindOf="phono">ma nɯ sɤz hanɯni pɣi kɯ-fse ma khro mɤ-wxti. </FORM>
	</S>
	<S id="S030">
		<AUDIO start="103.65" end="105.25"/>
		<FORM kindOf="phono">qajdo sɤz xtɕi.</FORM>
	</S>
	<S id="S031">
		<AUDIO start="106.19" end="108.3"/>
		<FORM kindOf="phono">tsaŋga sɤz tɕe wxti jamar ci ɲɯ-ŋu.</FORM>
	</S>
	<S id="S032">
		<AUDIO start="109.3" end="111.06"/>
		<FORM kindOf="phono">tɕe nɯnɯ qalekɯtshi ŋu tɕe, </FORM>
	</S>
	<S id="S033">
		<AUDIO start="111.55" end="116.17"/>
		<FORM kindOf="phono">qalekɯtshi nɯnɯ chondɤre βʑar nɯ ndʑi-tɯ-wxti naχtɕɯɣ.</FORM>
	</S>
	<S id="S034">
		<AUDIO start="117.7" end="120.67"/>
		<FORM kindOf="phono">tɕe nɯnɯ qalekɯtshi nɯnɯ, tu-kɯ-ru tɕe,</FORM>
	</S>
	<S id="S035">
		<AUDIO start="120.99" end="122.13"/>
		<FORM kindOf="phono">tɕhi tu-fse ku-nɯ-ŋu ma, </FORM>
	</S>
	<S id="S036">
		<AUDIO start="122.13" end="126.01"/>
		<FORM kindOf="phono">nɤmkha ri ɲɯ-nɯqambɯmbjom tɕe toʁde tɕe ɯ-stu ri ku-rɤʑi tɕe,</FORM>
	</S>
	<S id="S037">
		<AUDIO start="126.01" end="129.48"/>
		<FORM kindOf="phono">ɯ-jwaʁ, maʁkɯ, ɯ-ʁar tu-sɤlqɤlqɤt tu-sɤlqɤlqɤt tɕe </FORM>
	</S>
	<S id="S038">
		<AUDIO start="129.92" end="134.04"/>
		<FORM kindOf="phono">nɯnɯreri ku-rɤʑi tɕe tɕe toʁde mɯ́j-... kɤ-nɯqambɯmbjom mɯ́j-ɕe kɯ</FORM>
	</S>
	<S id="S039">
		<AUDIO start="134.42" end="139.7"/>
		<FORM kindOf="phono">nɯnɯre ɯ-stu ri ku-rɤʑi tɕe, ɯ-jwaʁ, maʁ, ɯ-ʁar nɯ tu-sɤlqɤlqɤt nɤ tu-sɤlqɤlqɤt ŋgrɤl </FORM>
	</S>
	<S id="S040">
		<AUDIO start="140.04" end="142.0"/>
		<FORM kindOf="phono">tɕe ɯ-jme nɯ li kɯ-ɤrtɯm kɯ-fse ɲɯ-ŋu.</FORM>
	</S>
	<S id="S041">
		<AUDIO start="142.61" end="144.82"/>
		<FORM kindOf="phono">ma nɯnɯ qalekɯtshi ŋu tɕe, </FORM>
	</S>
	<S id="S042">
		<AUDIO start="145.52" end="149.73"/>
		<FORM kindOf="phono">nɯnɯ qalekɯtshi nɯ, nɯnɯ cho βʑar ni wuma ʑo naχtɕɯɣ-ndʑi tɕe,</FORM>
	</S>
	<S id="S043">
		<AUDIO start="149.94" end="152.11"/>
		<FORM kindOf="phono">tɯ-tɯphu ŋu ɕi ʁnɯ-tɯphu ŋu mɤxsi.</FORM>
	</S>
	<S id="S044">
		<AUDIO start="153.39" end="159.69"/>
		<FORM kindOf="phono">ma qalekɯtshi nɯ kɯ nɯnɯ tɤrɤku tu-ndze kɯ-fse nɯra mtshɤm mɯ-pɯ-rɲo-t-a.</FORM>
	</S>
	<S id="S045">
		<AUDIO start="160.48" end="163.64"/>
		<FORM kindOf="phono">//nɯɣɯ// ku-nɯqambɯmbjom-nɯ ma nɯ ma ɯ-mto mɤ-rɲam-a.</FORM>
	</S>
	<S id="S046">
		<AUDIO start="164.64" end="166.27"/>
		<FORM kindOf="phono">nɯnɯ qalekɯtshi kɤ-ti ci tu.</FORM>
	</S>
	<S id="S047">
		<AUDIO start="166.98" end="172.89"/>
		<FORM kindOf="phono">ɯ-mdoʁ nɯra pɣi qhe, tɤrpat ɯ-mdoʁ tsa asɯndo qhe li ɯ-rca hanɯni kɯ-wɣrum ɣɤʑu qhe.</FORM>
	</S>
	<S id="S048">
		<AUDIO start="173.45" end="174.56"/>
		<FORM kindOf="phono">kɯ-xtɕɯ-xtɕi ɲɯ-ɤkhra.</FORM>
	</S>
	<S id="S049">
		<AUDIO start="176.05" end="179.37"/>
		<FORM kindOf="phono">ɯ-tɯ-ɤkhra khro mɯ́j-saχsɤl, mɯ́j-tʂot.</FORM>
	</S>
	<NOTE message="========" xmlns:lang="fr"/>
	<S id="S050">
		<AUDIO start="191.84" end="194.11"/>
		<FORM kindOf="phono">nɤmkha zɯ ɲɯ-nɯqambɯmbjom qhe tɕe, </FORM>
	</S>
	<S id="S051">
		<AUDIO start="194.58" end="197.52"/>
		<FORM kindOf="phono">ɲɯ-nɯqambɯmbjom, χtɯrca ɲɯ-nɯqambɯmbjom-nɯ ɲɯ-dɤn ri,</FORM>
	</S>
	<S id="S052">
		<AUDIO start="197.82" end="199.6"/>
		<FORM kindOf="phono">ci ci tɕe tɯ-khɤl ri ku-rɤʑi tɕe,</FORM>
	</S>
	<S id="S053">
		<AUDIO start="199.95" end="204.82"/>
		<FORM kindOf="phono">tɕe kɯki ɯ-ʁar ʁnɯz tu-sɤndzɯrndzɯr nɤ tu-sɤndzɯrndzɯr kɯ-fse ɲɯ-ŋu</FORM>
	</S>
	<S id="S054">
		<AUDIO start="206.13" end="208.36"/>
		<FORM kindOf="phono">tɕe nɯnɯ, nɯnɯ tɕe mɯ́j-naχtɕɯɣ.</FORM>
	</S>
	<S id="S055">
		<AUDIO start="208.77" end="211.62"/>
		<FORM kindOf="phono">tɕe kɯmaʁ ra nɯ kɯ-fse me ri,</FORM>
	</S>
	<S id="S056">
		<AUDIO start="212.3" end="215.35"/>
		<FORM kindOf="phono">qalekɯtshi nɯnɯ ɣɯ mɤ-kɯ-naχtɕɯɣ nɯ nɯ ɲɯ-ŋu.</FORM>
	</S>
	<NOTE message="=====================" xmlns:lang="fr"/>
	<S id="S057">
		<AUDIO start="222.24" end="225.26"/>
		<FORM kindOf="phono">qamtsɯr pɣɤtɕɯ kɤ-ti ci tu tɕe, nɯ wuma ʑo xtɕi</FORM>
	</S>
	<S id="S058">
		<AUDIO start="225.79" end="229.36"/>
		<FORM kindOf="phono">tɕe nɤkinɯ, qamtsɯr pɣɤtɕɯ nɯ kɯ-xtɕɯ-xtɕi ci ɕti.</FORM>
	</S>
	<S id="S059">
		<AUDIO start="229.96" end="231.81"/>
		<FORM kindOf="phono">te ʑɴɢoloʁ jamar ma maŋe wo.</FORM>
	</S>
	<S id="S060">
		<AUDIO start="232.33" end="235.68"/>
		<FORM kindOf="phono">woja, lɤ-zo qhe ʑɴɢoloʁ kɯ-fse ki kɯ-fse ma maŋe</FORM>
	</S>
	<S id="S061">
		<AUDIO start="236.2" end="239.4"/>
		<FORM kindOf="phono">tɕe nɯ qamtsɯr pɣɤtɕɯ, qamtsɯr pɣɤtɕɯ nɯ ʁo li,</FORM>
	</S>
	<S id="S062">
		<AUDIO start="239.99" end="242.83"/>
		<FORM kindOf="phono">nɤki tɤrɤku ra ri ɲɯ-ndze,  qajɯ ra ri ɲɯ-ndze ri</FORM>
	</S>
	<S id="S063">
		<AUDIO start="242.83" end="244.28"/>
		<FORM kindOf="phono">nɯ sɯŋgɯ ɕ-kurɤʑi ɕti.</FORM>
	</S>
	<S id="S064">
		<AUDIO start="244.71" end="246.41"/>
		<FORM kindOf="phono">kha ɯ-rkɯ kɯ-dɤn  tu mɤ-ŋgrɤl ri</FORM>
	</S>
	<S id="S065">
		<AUDIO start="246.41" end="248.08"/>
		<FORM kindOf="phono">"tɕrɯt nɤ tɕrɯt" tu-ti ŋu tɕe</FORM>
	</S>
	<S id="S066">
		<AUDIO start="248.6" end="250.98"/>
		<FORM kindOf="phono">ɯnɯnɯ, nɤkinɯ, ɯ-skɤt nɯ saχsɤl.</FORM>
	</S>
	<S id="S067">
		<AUDIO start="251.67" end="253.78"/>
		<FORM kindOf="phono">tɕe nɯnɯ qamtsɯr pɣɤtɕɯ tu-kɯ-ti ŋu tɕe,</FORM>
	</S>
	<S id="S068">
		<AUDIO start="253.78" end="254.91"/>
		<FORM kindOf="phono">nɯ kɯ-xtɕɯ-xtɕi ci ŋu.</FORM>
	</S>
	<S id="S069">
		<AUDIO start="256.1" end="258.98"/>
		<FORM kindOf="phono">nɯnɯ xtɕi cho mɤʑɯ jaʁmɤzdoʁzdoʁ kɤ-ti ci, </FORM>
	</S>
	<S id="S070">
		<AUDIO start="259.59" end="260.89"/>
		<FORM kindOf="phono">pɣɤtɕɯ ci tu tɕe</FORM>
	</S>
	<S id="S071">
		<AUDIO start="260.89" end="264.01"/>
		<FORM kindOf="phono">jaʁmɤzdoʁzdoʁ jaʁmu tɕe kɯki kɯ-fse ma me kɤ-ti ɲɯ-ŋu tɕe</FORM>
	</S>
	<S id="S072">
		<AUDIO start="264.38" end="266.34"/>
		<FORM kindOf="phono">nɯnɯ li nɯnɯ jamar ma mɤ-wxti.</FORM>
	</S>
	<S id="S073">
		<AUDIO start="267.05" end="270.09"/>
		<FORM kindOf="phono">ma nɯnɯ jaʁmɤzdoʁzdoʁ tu-ti ŋu tɕe,</FORM>
	</S>
	<S id="S074">
		<AUDIO start="270.31" end="272.03"/>
		<FORM kindOf="phono">qamtsɯr pɣɤtɕɯ nɯ ɯ-βri nɯra </FORM>
	</S>
	<S id="S075">
		<AUDIO start="272.39" end="276.62"/>
		<FORM kindOf="phono">kɯ-ɲaʁ ɯ-taʁ qhe kɯ-qarŋe kɯ-fse, kɯ-nɤmbju kɯ-fse tu qhe, nɯ kɯ mpɕɤr</FORM>
	</S>
	<S id="S076">
		<AUDIO start="277.21" end="279.41"/>
		<FORM kindOf="phono">tɕe nɯnɯ qamtsɯr pɣɤtɕɯ ŋu.</FORM>
	</S>
	<S id="S077">
		<AUDIO start="279.41" end="281.47"/>
		<FORM kindOf="phono">jaʁmɤzdoʁzdoʁ nɯ ʁo mɤʑɯ ʑo xtɕi.</FORM>
	</S>
	<S id="S078">
		<AUDIO start="282.09" end="284.33"/>
		<FORM kindOf="phono">ɯ-mtsioʁ rcanɯ kɯ-xtɕɯ-xtɕi ʑo ma maŋe.</FORM>
	</S>
	<S id="S079">
		<AUDIO start="284.65" end="289.68"/>
		<FORM kindOf="phono">ma nɯnɯ ʁnɯ-tɯphu nɯ (ɣɯ ndʑi-mtsioʁ) tɯtɯrca tsa tu-ŋgɤɣ-ndʑi ŋgrɤl </FORM>
	</S>
	<NOTE message="=========================" xmlns:lang="fr"/>
	<S id="S080">
		<AUDIO start="292.45" end="294.71"/>
		<FORM kindOf="phono">jaʁmɤzdoʁzdoʁ nɯ kɯnɤ pɣa ŋu, mɤ-arɤt</FORM>
	</S>
	<S id="S081">
		<AUDIO start="297.27" end="299.2"/>
		<FORM kindOf="phono">jaʁmɤzdoʁzdoʁ kɤ-ti ci tu tɕe,</FORM>
	</S>
	<S id="S082">
		<AUDIO start="299.2" end="301.46"/>
		<FORM kindOf="phono">nɯ qamtsɯr pɣɤtɕɯ cho ndʑi-tɯ-wxti naχtɕɯɣ.</FORM>
	</S>
	<S id="S083">
		<AUDIO start="302.14" end="303.8"/>
		<FORM kindOf="phono">kɯ-xtɕɯ-xtɕi ʑo zdoʁzdoʁ ma me. (ideoph)</FORM>
	</S>
	<S id="S084">
		<AUDIO start="305.34" end="308.57"/>
		<FORM kindOf="phono">//tɤ... tɤ-ŋgɯm// kɤ́rmɯrme tɤ-ŋgɯm jamar ma me.</FORM>
	</S>
	<S id="S085">
		<AUDIO start="311.06" end="313.77"/>
		<FORM kindOf="phono">kumpɣa ɯ-ŋgɯm, ma nɯ kɯ-fse ci ŋu.</FORM>
	</S>
	<S id="S086">
		<AUDIO start="314.67" end="316.5"/>
		<FORM kindOf="phono">tɕe nɯnɯ tɯ-tɯphu ŋu, mɤʑɯ</FORM>
	</S>
	<NOTE message="=======================" xmlns:lang="fr"/>
	<S id="S087">
		<AUDIO start="319.46" end="321.3"/>
		<FORM kindOf="phono">tɕe ɬɤndʐi pɣɤtɕɯ kɤ-ti ci tu.</FORM>
	</S>
	<S id="S088">
		<AUDIO start="321.79" end="323.35"/>
		<FORM kindOf="phono">ɬɤndʐi pɣɤtɕɯ nɯ wuma ʑo mpɕɤr.</FORM>
	</S>
	<S id="S089">
		<AUDIO start="323.35" end="328.51"/>
		<FORM kindOf="phono">nɤki ɯ-rme, ɯ-muj nɯra rcanɯ kɯ-mpɕɯ-mpɕɤr ʑo ŋu.</FORM>
	</S>
	<S id="S090">
		<AUDIO start="328.51" end="333.51"/>
		<FORM kindOf="phono">qhe maka kɯ-tʂɯ-tʂot ʑo qhe, kɯ-mpɕɯ-mpɕɤr ʑo ŋu qhe,</FORM>
	</S>
	<S id="S091">
		<AUDIO start="333.89" end="337.01"/>
		<FORM kindOf="phono">ɯ-rqɤpa nɯnɯra qarŋe qhe nɤmbju ʑo </FORM>
	</S>
	<S id="S092">
		<AUDIO start="337.41" end="340.32"/>
