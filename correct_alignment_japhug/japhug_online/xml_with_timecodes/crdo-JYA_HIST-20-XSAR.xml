<?xml version="1.0" ?>
<!DOCTYPE TEXT SYSTEM "https://cocoon.huma-num.fr/schemas/Archive.dtd">
<TEXT id="crdo-JYA_HIST-20-XSAR" xml:lang="jya">
	<HEADER>
		<TITLE>crdo-JYA_HIST-20-XSAR</TITLE>
		<SOUNDFILE href="hist-20-xsar.wav"/>
	</HEADER>
	<S id="S001">
		<AUDIO start="0.43" end="3.58"/>
		<FORM kindOf="phono">﻿tɕe rɯdaʁ nɯ ɯ-ŋgɯ ri, xsar kɤ-ti ci tu.</FORM>
	</S>
	<S id="S002">
		<AUDIO start="4.41" end="6.19"/>
		<FORM kindOf="phono">xsar nɯ iʑora ji-sɤtɕha tu.</FORM>
	</S>
	<S id="S003">
		<AUDIO start="6.36" end="9.16"/>
		<FORM kindOf="phono">tɕeri nɯnɯ, nɤkinɯ, </FORM>
	</S>
	<S id="S004">
		<AUDIO start="9.55" end="11.78"/>
		<FORM kindOf="phono">ɯ-tshɯɣa nɯra tshɤt ɲɯ-fse khi.</FORM>
	</S>
	<S id="S005">
		<AUDIO start="12.36" end="18.09"/>
		<FORM kindOf="phono">ɯ-ʁrɯ, ɯ-rna, ɯ-phoŋbu nɯra tshɤt wuma ʑo ɲɯ-fse khi.</FORM>
	</S>
	<S id="S006">
		<AUDIO start="18.59" end="20.23"/>
		<FORM kindOf="phono">tɕeri tshɤt sɤznɤ ɲɯ-wxti.</FORM>
	</S>
	<S id="S007">
		<AUDIO start="20.83" end="23.11"/>
		<FORM kindOf="phono">matɕi tshɤt nɯ khɤpa ku-ja-nɯ ɕti.</FORM>
	</S>
	<S id="S008">
		<AUDIO start="23.59" end="27.51"/>
		<FORM kindOf="phono">ci nɯnɯ, ɯʑɤsɯso aʁɤndɯndɤt yewai tu-nɤŋkɯŋke ɲɯ-ɕti. </FORM>
	</S>
	<S id="S009">
		<AUDIO start="28.08" end="28.84"/>
		<FORM kindOf="phono">sɯŋgɯ...</FORM>
	</S>
	<S id="S010">
		<AUDIO start="29.22" end="31.74"/>
		<FORM kindOf="phono">tɕeri ɯʑo stu sɤtɕha ɯ-kɤ-nɯrga nɯ, </FORM>
	</S>
	<S id="S011">
		<AUDIO start="32.05" end="35.36"/>
		<FORM kindOf="phono">tɕeki tɯ-ci kɯ-wxti ɣɯ ɯ-rkɯ tɕe, praʁ kɯ-tu ɯ-ŋgɯ nɯtɕu,</FORM>
	</S>
	<S id="S012">
		<AUDIO start="35.91" end="38.67"/>
		<FORM kindOf="phono">ɯ-kɤ-rga wuma ʑo ɲɯ-ŋu tɕe, ɲɯ-rkaŋ.</FORM>
	</S>
	<S id="S013">
		<AUDIO start="39.42" end="43.14"/>
		<FORM kindOf="phono">tɕe nɯ praʁ tɕhi kɯ-fse mɤ-kɯ-sɤɣa ra kɯnɤ ɯ-taʁ tu-ɕe,</FORM>
	</S>
	<S id="S014">
		<AUDIO start="43.14" end="46.43"/>
		<FORM kindOf="phono">ɯ-pa pjɯ-ɕe, pjɯ-mtsaʁ nɯra pjɯ-ndʐaβ mɯ́j-ŋgrɤl.</FORM>
	</S>
	<S id="S015">
		<AUDIO start="47.22" end="49.24"/>
		<FORM kindOf="phono">tɕe ɯ-qa ɣɯ nɯ kɯnɤ (ɯ-)ta-ʁrɯ nɯnɯ </FORM>
	</S>
	<S id="S016">
		<AUDIO start="49.51" end="52.2"/>
		<FORM kindOf="phono">tshɤt ɣɯ ɲɯ-fse ɕti rɯtɕi, nɯ mɯ́j-ŋgio rca matɕi,</FORM>
	</S>
	<S id="S017">
		<AUDIO start="52.62" end="53.47"/>
		<FORM kindOf="phono">nɯnɯ ɲɯ-cha.</FORM>
	</S>
	<S id="S018">
		<AUDIO start="54.34" end="55.7"/>
		<FORM kindOf="phono">tɕe nɯnɯ xsar ŋu tɕe,</FORM>
	</S>
	<S id="S019">
		<AUDIO start="56.04" end="59.53"/>
		<FORM kindOf="phono">ɯ-rme nɯnɯ, tshɤt ɯ-rme sɤznɤ ɲɯ-jndʐɤz,</FORM>
	</S>
	<S id="S020">
		<AUDIO start="60.5" end="62.99"/>
		<FORM kindOf="phono">ɲɯ-jndʐɤz qhe (ɲɯ-ɤpɣɤpɣi)  ɲɯ-ɤpɤpɣi kɯ-fse,</FORM>
	</S>
	<S id="S021">
		<AUDIO start="63.29" end="64.84"/>
		<FORM kindOf="phono">ɯ-xtɤpa nɯra ɲɯ-wɣrum.</FORM>
	</S>
	<S id="S022">
		<AUDIO start="65.31" end="67.94"/>
		<FORM kindOf="phono">ɯ-xtɤpa kɯ-fse chɯ-kɯ-ɕe nɯra lonba ɲɯ-wɣrum</FORM>
	</S>
	<S id="S023">
		<AUDIO start="68.55" end="72.13"/>
		<FORM kindOf="phono">tɕe ɯ-xtɤpa cho ɯ-mi, ɯ-jaʁ, ɯ-ŋgɯchu nɯra ɲɯ-wɣrum.</FORM>
	</S>
	<S id="S024">
		<AUDIO start="72.89" end="76.18"/>
		<FORM kindOf="phono">ɯ-taʁ nɯnɯ ɲɯ-pɣi,  ɲɯ-ɤdaʁlu kɯ-fse</FORM>
	</S>
	<S id="S025">
		<AUDIO start="77.01" end="78.82"/>
		<FORM kindOf="phono">ma nɯ kɯ-fse ɲɯ-ŋu tɕe,</FORM>
	</S>
	<S id="S026">
		<AUDIO start="79.26" end="81.09"/>
		<FORM kindOf="phono">nɯnɯ tshɤt sɤznɤ ɲɯ-wxti,</FORM>
	</S>
	<S id="S027">
		<AUDIO start="81.79" end="83.48"/>
		<FORM kindOf="phono">tshɤt nɯnɯ a-pɯ-ŋu,</FORM>
	</S>
	<S id="S028">
		<AUDIO start="83.94" end="85.89"/>
		<FORM kindOf="phono">qi ba shi jin jamar a-pɯ-zɣɯt tɕe,</FORM>
	</S>
	<S id="S029">
		<AUDIO start="85.99" end="89.21"/>
		<FORM kindOf="phono">xsar nɯ yibai yi ershi jamar ɲɯ-rʑi,</FORM>
	</S>
	<S id="S030">
		<AUDIO start="89.79" end="92.09"/>
		<FORM kindOf="phono">tú-wɣ-skɤr a-pɯ-ŋu tɕe, nɯ jamar ɲɯ-wxti. </FORM>
	</S>
	<S id="S031">
		<AUDIO start="93.32" end="96.65"/>
		<FORM kindOf="phono">tɕe nɤkinɯ, nɯnɯ,</FORM>
	</S>
	<S id="S032">
		<AUDIO start="96.65" end="99.66"/>
		<FORM kindOf="phono">nɯtɕu praʁ ɯ-ŋgɯ tu-nɤŋkɯŋke tɕe wuma ʑo ɲɯ-rkaŋ.</FORM>
	</S>
	<S id="S033">
		<AUDIO start="100.37" end="103.32"/>
		<FORM kindOf="phono">tɕe sɯjno tu-ndze, si ɯ-jwaʁ tu-ndze,</FORM>
	</S>
	<S id="S034">
		<AUDIO start="104.16" end="105.38"/>
		<FORM kindOf="phono">ma nɯ ma ɯ-kɤ-ndza maŋe,</FORM>
	</S>
	<S id="S035">
		<AUDIO start="105.38" end="107.27"/>
		<FORM kindOf="phono">tɤjmɤɣ nɯra tu-ndze ɲɯ-ŋgrɤl rca,</FORM>
	</S>
	<S id="S036">
		<AUDIO start="107.97" end="111.37"/>
		<FORM kindOf="phono">tɕe ɯ-sɤ-rma nɯnɯ, praʁ, praʁpa tɕe ɕ-ku-rma ɲɯ-ŋu </FORM>
	</S>
	<S id="S037">
		<AUDIO start="112.18" end="116.15"/>
		<FORM kindOf="phono">sɤz-nɤŋkɯŋke nɯ, praʁ ɯ-ŋgɯ cho nɯ, tɯ-ci ɯ-rkɯ nɯra tu-nɤŋkɯŋke ɲɯ-ŋu.</FORM>
	</S>
	<S id="S038">
		<AUDIO start="116.15" end="117.97"/>
		<FORM kindOf="phono">wuma ʑo ɲɯ-rkaŋ.</FORM>
	</S>
	<S id="S039">
		<AUDIO start="118.9" end="121.51"/>
		<FORM kindOf="phono">ɯ-tɯ-rkaŋ kɯ xsar ʑo ɲɯ-fse tu-kɯ-ti ŋgrɤl ma,</FORM>
	</S>
	<S id="S040">
		<AUDIO start="122.18" end="125.15"/>
		<FORM kindOf="phono">ɯnɯnɯ, nɤkinɯ, xsar ŋu tɕe,</FORM>
	</S>
	<S id="S041">
		<AUDIO start="125.66" end="130.56"/>
		<FORM kindOf="phono">kupa kɯ, iʑora tuhanzu ra kɯ, "qingyang" tu-ti-nɯ ŋu.</FORM>
	</S>
	<S id="S042">
		<AUDIO start="130.56" end="133.46"/>
		<FORM kindOf="phono">nɯnɯ shumian ɣɯ mingci nɯ tɕe, </FORM>
	</S>
	<S id="S043">
		<AUDIO start="134.09" end="134.89"/>
		<FORM kindOf="phono">aiyang, yanyang,</FORM>
	</S>
	<S id="S044">
		<AUDIO start="134.89" end="137.84"/>
		<FORM kindOf="phono">nɯnɯ kɯ-fse nɯ pɯ-pɯ-ŋu nɤ, </FORM>
	</S>
	<S id="S045">
		<AUDIO start="137.84" end="142.11"/>
		<FORM kindOf="phono">li aj ɲɯ-maʁ ɲɯ-sɯsam-a ma mɤʑɯ ldɯɣi kɤ-ti ci tu ko,</FORM>
	</S>
	<S id="S046">
		<AUDIO start="143.0" end="145.83"/>
		<FORM kindOf="phono">ldɯɣi nɯ pɯ-fɕat-a low,</FORM>
	</S>
	<S id="S047">
		<AUDIO start="146.28" end="148.42"/>
		<FORM kindOf="phono">nɤkinɯ, qaʑo kɯ-fse ci ɲɯ-ŋu.</FORM>
	</S>
	<S id="S048">
		<AUDIO start="149.08" end="153.45"/>
		<FORM kindOf="phono">ma nɯnɯ, nɯ ɣɯ ɯ-tshɯɣa nɯ, qaʑo ɯ-ʁrɯ cho ɯ-rme ra ɲɯ-fse,</FORM>
	</S>
	<S id="S049">
		<AUDIO start="153.84" end="156.79"/>
		<FORM kindOf="phono">xsar nɯ tɕe, tshɤt ɲɯ-fse.</FORM>
	</S>
	<S id="S050">
		<AUDIO start="156.97" end="160.93"/>
		<FORM kindOf="phono">tɕe chɤlɤnnɤ nɤ, yilei kɯ-fse ŋu ..., ŋu thaŋ,</FORM>
	</S>
	<S id="S051">
		<AUDIO start="161.75" end="164.1"/>
		<FORM kindOf="phono">tɯtɯphu ŋu-nɯ thaŋ ma </FORM>
	</S>
	<S id="S052">
		<AUDIO start="164.1" end="168.69"/>
		<FORM kindOf="phono">nɯnɯ xsar nɯnɯ tɕhɯkɤɣar praʁ ɯ-ŋgɯ zɯ ku-rɤʑi.</FORM>
	</S>
	<S id="S053">
		<AUDIO start="168.83" end="170.93"/>
		<FORM kindOf="phono">tɕe wuma ʑo ɲɯ-rkaŋ</FORM>
	</S>
	<S id="S054">
		<AUDIO start="170.93" end="172.65"/>
		<FORM kindOf="phono">tɕe nɯ xsar ɲɯ-ŋu.</FORM>
	</S>
</TEXT>