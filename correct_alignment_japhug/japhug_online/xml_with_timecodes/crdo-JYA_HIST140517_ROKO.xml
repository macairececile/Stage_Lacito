	<S id="S001">
		<AUDIO start="0.26" end="5.05"/>
		<FORM kindOf="phono">roko kɤ-ti ci tu tɕe, nɯ roko nɯnɯ, raʁ ɣɯ ɯ-ndɯr ɯ-mɤ-kɯ-ŋu-ci ma</FORM>
	</S>
	<S id="S002">
		<AUDIO start="5.54" end="7.58"/>
		<FORM kindOf="phono">mɯ-ɲɯ-ldʑɯz, kɯ-rko tsa ɲɯ-ŋu.</FORM>
	</S>
	<S id="S003">
		<AUDIO start="8.22" end="14.42"/>
		<FORM kindOf="phono">tɕe nɯnɯ, nɤkinɯ, tɕhoma kɯ-fse, sɤɕtɕɯɣ ɣɯ nɯnɯ ɯ-thathor nɯra pjɯ-βzu-nɯ. </FORM>
	</S>
	<S id="S004">
		<AUDIO start="15.43" end="19.52"/>
		<FORM kindOf="phono">ɯ-thathor tɕe tɕe nɤkinɯ iɕqha kɯki kɯ-fse tɕe ɯ-kɯ-mpɕɤr,</FORM>
	</S>
	<S id="S005">
		<AUDIO start="19.52" end="20.9"/>
		<FORM kindOf="phono">kutɕu ɯ-mdʑu kɯ-tu ku-βzu-nɯ</FORM>
	</S>
	<S id="S006">
		<AUDIO start="21.48" end="23.43"/>
		<FORM kindOf="phono">tɕe tɯ-ndʐi nɯ nɯtɕu kú-wɣ-lɤt kɯ-khɯ.</FORM>
	</S>
	<S id="S007">
		<AUDIO start="24.16" end="25.61"/>
		<FORM kindOf="phono">tɕe nɯnɯ roko tu-kɯ-ti ŋu.</FORM>
	</S>
	<S id="S008">
		<AUDIO start="26.51" end="28.89"/>
		<FORM kindOf="phono">tɕe nɯnɯ raʁ ɣɯ ɯ-ndɯr tsa ɲɯ-ŋu lo.</FORM>
	</S>
	<S id="S009">
		<AUDIO start="29.21" end="31.6"/>
		<FORM kindOf="phono">nɯnɯ jiagong tu-βzu-nɯ.</FORM>
	</S>
	<S id="S010">
		<AUDIO start="32.0" end="34.27"/>
