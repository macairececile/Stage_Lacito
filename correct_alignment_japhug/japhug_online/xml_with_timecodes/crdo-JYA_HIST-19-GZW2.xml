	<S id="S001">
		<AUDIO start="1.01" end="3.57"/>
		<FORM kindOf="phono">﻿japa iʑo ji-rɣa ci ɣɯ </FORM>
	</S>
	<S id="S002">
		<AUDIO start="3.9" end="7.78"/>
		<FORM kindOf="phono">nɯtɕu nɯ kɯ, nɤkinɯ, ɕ-pjɤ-ɣɤrʁaʁ tɕe,</FORM>
	</S>
	<S id="S003">
		<AUDIO start="7.78" end="10.68"/>
		<FORM kindOf="phono">βʑɯ ɯ-pɯ, maʁ, ɣzɯ ɣɯ ɯ-pɯ ci ko-ndo tɕe jo-ɣɯt.</FORM>
	</S>
	<S id="S004">
		<AUDIO start="11.04" end="13.49"/>
		<FORM kindOf="phono">tɕe nɯnɯ nɤkinɯ,</FORM>
	</S>
	<S id="S005">
		<AUDIO start="14.24" end="18.85"/>
		<FORM kindOf="phono">kɯ-mɤku ra tɕe, mɯ́j-khɯ tɕe, khro mɯ́j-rga tɕe nɤki ri,</FORM>
	</S>
	<S id="S006">
		<AUDIO start="19.38" end="21.38"/>
		<FORM kindOf="phono">(?) ɲɯ́-wɣ-mbi, tshitsuku ɲɯ́-wɣ-mbi,</FORM>
	</S>
	<S id="S007">
		<AUDIO start="21.38" end="23.44"/>
		<FORM kindOf="phono">ɲɯ́-wɣ-jtshi, tú-wɣ-raχtɕɤz tɕe,</FORM>
	</S>
	<S id="S008">
		<AUDIO start="23.44" end="25.85"/>
		<FORM kindOf="phono">ʑɯrɯʑɤri tɕe tɕendɤre ku-kɯ-nɯfse ɲɯ-ŋu.</FORM>
	</S>
	<S id="S009">
		<AUDIO start="26.47" end="27.94"/>
		<FORM kindOf="phono">tɕe ku-kɯ-nɯfse tɕe tɕe,</FORM>
	</S>
	<S id="S010">
		<AUDIO start="28.28" end="30.57"/>
		<FORM kindOf="phono">tɯ-zda nɯ ma kɯmaʁ tɯrme a-pɯ-me tɕe,</FORM>
	</S>
	<S id="S011">
		<AUDIO start="30.9" end="33.35"/>
		<FORM kindOf="phono">kha ra aʁɤndɯndɤt ɲɯ-ɤnɯɣro ɲɯ-ŋu ɲɯ-ti. </FORM>
	</S>
	<S id="S012">
		<AUDIO start="33.81" end="36.18"/>
		<FORM kindOf="phono">tɕe wuma ʑo ɲɯ-rga tɕe ɲɯ-ɤnɯɣro ɲɯ-ŋu.</FORM>
	</S>
	<S id="S013">
		<AUDIO start="36.71" end="39.07"/>
		<FORM kindOf="phono">tɯrme kɯ-ɣi a-pɯ-tu tɕe tu-ondzɯt ɲɯ-ŋu (ɲɯ-ti).</FORM>
	</S>
	<S id="S014">
		<AUDIO start="39.41" end="41.74"/>
		<FORM kindOf="phono">ɯ-mɤ-kɤ-nɯfse a-jɤ-ɣi-nɯ tɕe tu-ondzɯt tɕe </FORM>
	</S>
	<S id="S015">
		<AUDIO start="42.05" end="43.32"/>
		<FORM kindOf="phono">tu-nɯjʁo ɲɯ-ŋu ɲɯ-ti.</FORM>
	</S>
	<S id="S016">
		<AUDIO start="43.99" end="46.93"/>
		<FORM kindOf="phono">tɕe tɕhi tu-ti mɯ́j-sɤtso ri tu-ɣɤɕqali ɲɯ-ŋu ɲɯ-ti.</FORM>
	</S>
	<S id="S017">
		<AUDIO start="47.69" end="50.3"/>
		<FORM kindOf="phono">tɕe li nɯ tɯrme kɯnɤ ku-kɯ-nɯfse ɲɯ-ŋu, </FORM>
	</S>
	<S id="S018">
		<AUDIO start="50.36" end="53.99"/>
		<FORM kindOf="phono">tɯrme kɯmaʁ jɤ-kɯ-ɣe nɯra tɕe ɲɯ-sɯχsɤl tɕe,</FORM>
	</S>
	<S id="S019">
		<AUDIO start="54.47" end="61.74"/>
		<FORM kindOf="phono">nɯnɯ tu-nɯjʁo ɲɯ-ŋu ɲɯ-ti.</FORM>
	</S>
