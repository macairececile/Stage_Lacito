<TEXT id="crdo-JYA_HIST-21-QAPRAFTSA" xml:lang="jya">
	<HEADER>
		<TITLE>crdo-JYA_HIST-21-QAPRAFTSA</TITLE>
		<SOUNDFILE href="hist-21-qaprAftsa.wav"/>
	</HEADER>
	<S id="S001">
		<AUDIO start="0.17" end="2.97"/>
		<FORM kindOf="phono">qaprɤftsa kɤti ci tu tɕe, nɯ qajɯ ŋu</FORM>
	</S>
	<S id="S002">
		<AUDIO start="3.56" end="6.0"/>
		<FORM kindOf="phono">qajɯ ŋu tɕe, ɯ-thoʁ cho aʁɤndɯndɤt ʑo tu,</FORM>
	</S>
	<S id="S003">
		<AUDIO start="6.65" end="9.57"/>
		<FORM kindOf="phono">rcanɯ ɯ-me ɯ-jaʁ  rca kɯ-dɯ-dɤn ʑo ŋu tɕe,</FORM>
	</S>
	<S id="S004">
		<AUDIO start="9.57" end="11.87"/>
		<FORM kindOf="phono">tɯʑo kú-wɣ-rtoʁ tɕe ɯ-rme ʑo fse ma </FORM>
	</S>
	<S id="S005">
		<AUDIO start="12.34" end="14.14"/>
		<FORM kindOf="phono">ɯ-me ɯ-jaʁ ɲɯŋu kɤsɯso me ma </FORM>
	</S>
	<S id="S006">
		<AUDIO start="14.62" end="16.41"/>
		<FORM kindOf="phono">nɯnɯ kɯ-dɯ-dɤn kɯ-ndɯ-ndɯβ ʑo ŋu</FORM>
	</S>
	<S id="S007">
		<AUDIO start="16.99" end="19.19"/>
		<FORM kindOf="phono">tɕe ɯ-ku ɯ-taʁ ri li ɯ-ʁrɯ kɯ-fse ci ɣɤʑu.</FORM>
	</S>
	<S id="S008">
		<AUDIO start="20.26" end="21.75"/>
		<FORM kindOf="phono">tɕe nɯ jɤ-ari tɕe ju-mɤku ra, </FORM>
	</S>
	<S id="S009">
		<AUDIO start="21.82" end="27.99"/>
		<FORM kindOf="phono">qaprɤftsa nɯnɯ, ci ci jɤ-ari tɕe ɯ-ku ju-zmɤke, ci ci tɕe ɯ-jme ju-zmɤke ɲɯ-ɕti tɕe</FORM>
	</S>
	<S id="S010">
		<AUDIO start="28.6" end="31.13"/>
		<FORM kindOf="phono">ŋotɕu nɯ ɯ-ku ŋu, ŋotɕu ɯ-jme ŋu, mɯ́j-saχsɤl</FORM>
	</S>
	<S id="S011">
		<AUDIO start="32.03" end="34.54"/>
		<FORM kindOf="phono">ma nɯnɯ ɯ-mɤlɤjaʁ tɕe kɯ-dɯ-dɤn ʑo ɲɯ-ŋu.</FORM>
	</S>
	<S id="S012">
		<AUDIO start="34.87" end="38.41"/>
		<FORM kindOf="phono">tɕe qaprɤftsa nɯnɯ kɯ-xtɕi ɣɤʑu, kɯ-wxti ɣɤʑu</FORM>
	</S>
	<S id="S013">
		<AUDIO start="39.28" end="41.94"/>
		<FORM kindOf="phono">tɕe kɯ-xtɕi kɯ-qarŋe kɯ-fse ci tu tɕe, nɯnɯ</FORM>
	</S>
	<S id="S014">
		<AUDIO start="42.54" end="44.29"/>
		<FORM kindOf="phono">nɤki tɤjmɤɣ ɯ-ŋgɯ ri tu ŋgrɤl tɕe,</FORM>
	</S>
	<S id="S015">
		<AUDIO start="44.29" end="46.5"/>
		<FORM kindOf="phono">nɯnɯ tɤjmɤɣ nɯ ɯŋgɯ pɯkɯŋke nɯnɯ </FORM>
	</S>
	<S id="S016">
		<AUDIO start="46.96" end="49.43"/>
		<FORM kindOf="phono">pɯŋke tɕe, nɯnɯ tɤjmɤɣ nɯ ɣɯ-ndza mɤβdi ma</FORM>
	</S>
	<S id="S017">
		<AUDIO start="49.43" end="50.88"/>
		<FORM kindOf="phono">tɯxtu ɕɯmŋɤm tutinɯ ŋgrɤl</FORM>
	</S>
	<S id="S018">
		<AUDIO start="51.42" end="53.16"/>
		<FORM kindOf="phono">nɯnɯ ɯtɯɣ tu tutinɯ ŋgrɤl ma</FORM>
	</S>
	<S id="S019">
		<AUDIO start="53.65" end="54.92"/>
		<FORM kindOf="phono">nɯnɯ qaprɤftsa ŋu</FORM>
	</S>
	<S id="S020">
		<AUDIO start="54.92" end="56.99"/>
		<FORM kindOf="phono">qaprɤftsa kɯwxti ci tu tɕe nɯnɯ</FORM>
	</S>
	<S id="S021">
		<AUDIO start="57.46" end="58.69"/>
		<FORM kindOf="phono">tɕe ɯβri nɯ ɲaʁ</FORM>
	</S>
	<S id="S022">
		<AUDIO start="59.21" end="61.58"/>
		<FORM kindOf="phono">tɕe nɯnɯ li qaprɤftsa kɯnɤ</FORM>
	</S>
	<S id="S023">
		<AUDIO start="61.99" end="65.94"/>
		<FORM kindOf="phono">ɯ-mɤlɤjaʁ kɯ-dɯ-dɤn ʑo ɣɤʑu, ɯ-mɤlɤjaʁ maka mɤ-kɯ-saχsɤl, kɯ-me tɕe</FORM>
	</S>
	<S id="S024">
		<AUDIO start="66.27" end="67.72"/>
		<FORM kindOf="phono">qapri ʑo kɯ-fse ci ɣɤʑu</FORM>
	</S>
	<S id="S025">
		<AUDIO start="68.41" end="70.86"/>
		<FORM kindOf="phono">tɕe qaprɤftsa nɯ χsɯ-tɯphu jamar ɣɤʑu wo,</FORM>
	</S>
	<S id="S026">
		<AUDIO start="71.88" end="74.25"/>
		<FORM kindOf="phono">matɕi kɯ-wxti nɯ tɯ-tɯphu ɲɯ-ŋu ri,</FORM>
	</S>
	<S id="S027">
		<AUDIO start="74.54" end="78.15"/>
		<FORM kindOf="phono">ci, ɯ-rme, ɯ-mɤlɤjaʁ kɯ-tu ci ɣɤʑu, kɯ-me ci ɣɤʑu tɕe</FORM>
	</S>
	<S id="S028">
		<AUDIO start="78.92" end="80.11"/>
		<FORM kindOf="phono">tɕe nɯnɯ mɯ́j-naχtɕɯɣ</FORM>
	</S>
	<S id="S029">
		<AUDIO start="80.56" end="83.22"/>
		<FORM kindOf="phono">tɕe kɯ-xtɕi nɯ kɯ-qarŋe ʑo ɲɯ-ŋu</FORM>
	</S>
	<S id="S030">
		<AUDIO start="83.59" end="85.56"/>
		<FORM kindOf="phono">tɕe nɯnɯ li ɯ-mɤlɤjaʁ ɲɯ-dɤn </FORM>
	</S>
	<S id="S031">
		<AUDIO start="86.13" end="88.66"/>
		<FORM kindOf="phono">ma nɯnɯ li ɯ-βri nɯ ɯ-phaʁ ntsi nɯ</FORM>
	</S>
	<S id="S032">
		<AUDIO start="89.03" end="93.55"/>
		<FORM kindOf="phono">χchoʁe nɯnɯ ɯ-mɤlɤjaʁ lu-mtshɤt ʑo ɲɯ-ɕti </FORM>
	</S>
	<S id="S033">
		<AUDIO start="94.25" end="96.04"/>
		<FORM kindOf="phono">tɕe nɯnɯ ɲɯ-sɤndɤɣ</FORM>
	</S>
	<S id="S034">
		<AUDIO start="96.95" end="102.81"/>
		<FORM kindOf="phono">tɕe tɤjmɤɣ ɯtaʁ pɯkɯtu nɯra ma-tɤ́-wɣ-ndza ra ma kɯ-ɕɯngo tutinɯ ŋgrɤl </FORM>
	</S>
	<S id="S035">
		<AUDIO start="102.81" end="104.05"/>
		<FORM kindOf="phono">nɯnɯ qaprɤftsa ŋu.</FORM>
	</S>
	<S id="S036">
		<AUDIO start="104.7" end="107.42"/>
		<FORM kindOf="phono">tɕe qajɯ ci ɕti ɯthoʁ kurɤʑi ɕti</FORM>
	</S>
	<NOTE message="..." xmlns:lang="fr"/>
	<S id="S037">
		<AUDIO start="115.48" end="119.29"/>
		<FORM kindOf="phono">mɤkɯmtsɯɣ, ɲɯkɯmtsɯɣ kɤti me,</FORM>
	</S>
	<S id="S038">
		<AUDIO start="119.62" end="124.92"/>
		<FORM kindOf="phono">nɤki ɯʑo pɯnɯrtsɯ pɯtu ɣɯ ɯsta nɯnɯ túwɣndza tɕe</FORM>
	</S>
	<S id="S039">
		<AUDIO start="124.92" end="129.93"/>
		<FORM kindOf="phono">nɯnɯ kɯ-ɕɯngo ɲɯti, tɕe ɲɯ-kɯ-znɯtɯfɕɤl ɲɯŋu</FORM>
	</S>
