	<S id="S001">
		<AUDIO start="0.19" end="1.76"/>
		<FORM kindOf="phono">nɯra pjɯ-fɕat-a ɯ́-ŋu?</FORM>
	</S>
	<S id="S002">
		<AUDIO start="2.47" end="6.25"/>
		<FORM kindOf="phono">kɯrɯxtsa tɤ́-wɣ-βzu tɕe, nɤkinɯ,</FORM>
	</S>
	<S id="S003">
		<AUDIO start="6.75" end="9.9"/>
		<FORM kindOf="phono">kɯrɯxtsa nɯ a-pɯ-ŋu tɕe, ɯ-pa mŋulɤn ra</FORM>
	</S>
	<S id="S004">
		<AUDIO start="10.37" end="12.02"/>
		<FORM kindOf="phono">tɕe ɯ-rkɯ nɯtɕu xtsɤrkɯ ra</FORM>
	</S>
	<S id="S005">
		<AUDIO start="12.43" end="13.98"/>
		<FORM kindOf="phono">ɯ-ʁɤri nɯtɕu xtsɤɕna ra</FORM>
	</S>
	<S id="S006">
		<AUDIO start="14.65" end="16.38"/>
		<FORM kindOf="phono">tɕe nɯ xtsɤrkɯ ɣɯ ɯ-taʁ tɕe </FORM>
	</S>
	<S id="S007">
		<AUDIO start="16.38" end="17.88"/>
		<FORM kindOf="phono">tɕe li xtsɤku pjɯ́-wɣ-tshoʁ ra</FORM>
	</S>
	<S id="S008">
		<AUDIO start="18.56" end="21.37"/>
		<FORM kindOf="phono">tɕe nɯ xtsɤku nɯ pɯ́-wɣ-tshoʁ tɕe nɯnɯ ɯ-ŋgɯ nɯnɯ</FORM>
	</S>
	<S id="S009">
		<AUDIO start="21.64" end="25.22"/>
		<FORM kindOf="phono">tɕeki ɯ-rkɯ tu-ʑe tɕe tɕe //tɯ-ku// (ɯ-ku) mɯ-tɤ-nɯ-ɬoʁ mɤɕtʂa  ɯ-ŋgɯ nɯnɯ </FORM>
	</S>
	<S id="S010">
		<AUDIO start="25.67" end="28.2"/>
		<FORM kindOf="phono">nɤki tɯŋgar, tɕe tɤthu iɕqha,</FORM>
	</S>
	<S id="S011">
		<AUDIO start="28.69" end="32.03"/>
		<FORM kindOf="phono">smɤɣ lɤ-kɤ-pɣo tɕe thɯ-kɤ-taʁ nɯnɯ nɯtɕu ɲɯ-ɕe ra.</FORM>
	</S>
	<S id="S012">
		<AUDIO start="32.63" end="35.31"/>
		<FORM kindOf="phono">tɕe nɯnɯ chunmao ʁɟa ɕti tɕe mpja matɕi,</FORM>
	</S>
	<S id="S013">
		<AUDIO start="35.31" end="38.37"/>
		<FORM kindOf="phono">tɕe nɯ tú-wɣ-stu tɕe, ɯ-mdoʁ nɯ pjɯ-mpɕɤr mɤ-ra</FORM>
	</S>
	<S id="S014">
		<AUDIO start="38.37" end="42.21"/>
		<FORM kindOf="phono">tɕe nɤkinɯ, nɯ-smɤɣ nɯ-kɯ-ɤtʂoʁloʁ kɯ-fse, </FORM>
	</S>
	<S id="S015">
		<AUDIO start="42.21" end="46.61"/>
		<FORM kindOf="phono">ɯ-mdoʁ mɤ-kɯ-mpɕɤr kɯ-fse nɯnɯra xtsɤku chɯ-βzu-nɯ ŋgrɤl ma</FORM>
	</S>
	<S id="S016">
		<AUDIO start="46.61" end="49.92"/>
		<FORM kindOf="phono">nɯ xtsɤku ŋu tɕe, kɯrɯxtsa nɯnɯ, nɯra pjɯ-tu ra.</FORM>
	</S>
	<S id="S017">
		<AUDIO start="51.15" end="55.31"/>
		<FORM kindOf="phono">tɕendɤre iɕqha nɯ ɯ-srɯβzɤn tu-ti-nɯ tɕe,</FORM>
	</S>
	<S id="S018">
		<AUDIO start="55.61" end="59.02"/>
		<FORM kindOf="phono">ɯ-qhuchu, ɯ-xtsa nɯnɯ ɯ-qhu ɯ-ɲɯ-kɯ-sɤlɤɣɯt nɯnɯ li, </FORM>
	</S>
	<S id="S019">
		<AUDIO start="59.36" end="60.38"/>
		<FORM kindOf="phono">ɯ-chɯ kɯ-fse tɕe,</FORM>
	</S>
	<S id="S020">
		<AUDIO start="60.81" end="62.74"/>
		<FORM kindOf="phono">srɯβzɤn ci ʂɯlʂɯl tu-ɕe ra</FORM>
	</S>
	<S id="S021">
		<AUDIO start="63.33" end="65.34"/>
		<FORM kindOf="phono">tɕe nɯra tɯ-tɯ-tʂɯβ chɯ-βzu-nɯ tɕe tɕe</FORM>
	</S>
	<S id="S022">
		<AUDIO start="65.7" end="67.69"/>
		<FORM kindOf="phono">thɯ-kɤ-pɣaʁ kɯ-fse chɯ-tʂɯβ-nɯ tɕe tɕe </FORM>
	</S>
	<S id="S023">
		<AUDIO start="67.69" end="69.57"/>
		<FORM kindOf="phono">ɯ-tʂɯβ thɯ-jɤɣ tɕe tɕe chɯ-pɣaʁ-nɯ</FORM>
	</S>
	<S id="S024">
		<AUDIO start="70.22" end="74.69"/>
		<FORM kindOf="phono">tɕe tɯ-tʂɯβ nɯ ɯ-ŋgɯ kɯ-ru tɕe tɕe mpɕɤr, tɕe nɯ kɯ-fse tu-βzu-nɯ ŋu</FORM>
	</S>
	<S id="S025">
		<AUDIO start="75.78" end="78.15"/>
		<FORM kindOf="phono">tɯ-xtsa nɯ konaʁ xtsa pɯ-nɯ-ŋu,</FORM>
	</S>
	<S id="S026">
		<AUDIO start="78.53" end="82.75"/>
		<FORM kindOf="phono">kosca pɯ-nɯ-ŋu, qamdɯxtsa pɯ-nɯ-ŋu nɯ ʁɟa tu-fse ra.</FORM>
	</S>
	<S id="S027">
		<AUDIO start="83.33" end="84.24"/>
		<FORM kindOf="phono">tɕe pjɯ-tshoz ra </FORM>
	</S>
	<S id="S028">
		<AUDIO start="85.0" end="87.14"/>
		<FORM kindOf="phono">tɕe nɯ konaʁ-xtsa kɤ-ti tu,</FORM>
	</S>
	<S id="S029">
		<AUDIO start="87.7" end="90.1"/>
		<FORM kindOf="phono">tɕendɤre qamdɯ-xtsa kɤ-ti tu,</FORM>
	</S>
	<S id="S030">
		<AUDIO start="90.55" end="92.63"/>
		<FORM kindOf="phono">tɕe kosca, tɯ-xtsa nɯ tu</FORM>
	</S>
	<S id="S031">
		<AUDIO start="93.03" end="95.44"/>
		<FORM kindOf="phono">tɕendɤre palaxtsa kɤ-ti ci tu,</FORM>
	</S>
	<S id="S032">
		<AUDIO start="95.44" end="97.3"/>
		<FORM kindOf="phono">xtsɤrtɯm kɤ-ti ci tu,</FORM>
	</S>
	<S id="S033">
		<AUDIO start="97.7" end="99.12"/>
		<FORM kindOf="phono">tɤmbextsa kɤ-ti ci tu</FORM>
	</S>
	<S id="S034">
		<AUDIO start="99.44" end="100.86"/>
		<FORM kindOf="phono">tɕe kɯmŋu-tɯphu ɣɤʑu</FORM>
	</S>
	<S id="S035">
		<AUDIO start="101.53" end="102.51"/>
		<FORM kindOf="phono">nɯra tu-βzu-nɯ ŋgrɤl</FORM>
	</S>
	<S id="S036">
		<AUDIO start="102.51" end="103.97"/>
		<FORM kindOf="phono">alo mbroχpa kɯ-fse</FORM>
	</S>
	<S id="S037">
		<AUDIO start="104.32" end="107.0"/>
		<FORM kindOf="phono">bannong banmu nɯra kɯ tɕe, nɤkinɯ,</FORM>
	</S>
	<S id="S038">
		<AUDIO start="107.08" end="109.05"/>
		<FORM kindOf="phono">ɕɯrʁom kɯ tɕhɤrʁɯspa kɯ-fse nɯ</FORM>
	</S>
	<S id="S039">
		<AUDIO start="109.41" end="111.71"/>
		<FORM kindOf="phono">nɯ-xtsa, tɤmbe xta kɯ-fse chɯ-nɯ-βzu-nɯ ŋgrɤl</FORM>
	</S>
	<S id="S040">
		<AUDIO start="112.19" end="114.16"/>
		<FORM kindOf="phono">tɕe nɯ ɯ-ŋgɯmɤpɕi ʑo tɤthu ŋu </FORM>
	</S>
	<S id="S041">
		<AUDIO start="114.63" end="119.52"/>
		<FORM kindOf="phono">ɯ-pɕi nɯ smɤɣ iɕqha kɯ-ngɯt kɯ-fse nɯnɯ, kɯ-rʁom kɯ-fse nɯ chɯ-βzu-nɯ</FORM>
	</S>
	<S id="S042">
		<AUDIO start="119.78" end="121.27"/>
		<FORM kindOf="phono">tɕe nɯnɯ tɯ-ci mɤ-ɣi tɕe,</FORM>
	</S>
	<S id="S043">
		<AUDIO start="121.73" end="125.57"/>
		<FORM kindOf="phono">tɕe ɯ-ŋgɯ nɯnɯ //?// tɤthu kɯ-ndʑɤm kɯ-fse chɯ-βzu-nɯ </FORM>
	</S>
	<S id="S044">
		<AUDIO start="126.08" end="133.71"/>
		<FORM kindOf="phono">tɕe nɯ tɯ-xtsa tɕe kɯɕnɯ-tɯphu ɣɤʑu wo, kɯrɯxtsa</FORM>
