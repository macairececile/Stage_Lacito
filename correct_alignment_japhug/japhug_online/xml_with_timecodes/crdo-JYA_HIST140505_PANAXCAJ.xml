	<S id="S001">
		<AUDIO start="0.47" end="4.35"/>
		<FORM kindOf="phono">khɯna kɯ sɯjno kɤ-ndza ci, xɕaj kɤ-ndza ci tu tɕe,</FORM>
	</S>
	<S id="S002">
		<AUDIO start="4.35" end="6.41"/>
		<FORM kindOf="phono">nɯ pɤŋɤxɕaj rmi tɕe, </FORM>
	</S>
	<S id="S003">
		<AUDIO start="6.41" end="10.36"/>
		<FORM kindOf="phono">pɤŋɤxɕaj nɤki, khɯna kɯ tu-ndze tɕe lú-wɣ-sɯ-qioʁ ɲɯ-ŋu.</FORM>
	</S>
	<S id="S004">
		<AUDIO start="10.62" end="13.73"/>
		<FORM kindOf="phono">ma tɕe kɤ-ndza kɤ-ɣɤndɯβ mɯ́j-khɯ tɕe lú-wɣ-sɯ-qioʁ ɲɯ-ŋu tɕe,</FORM>
	</S>
	<S id="S005">
		<AUDIO start="14.37" end="19.22"/>
		<FORM kindOf="phono">nɯnɯ, nɤki, khɯna ɯ-kɯ-mŋɤm tɤ-tu tɕe, pɤŋɤxɕaj tu-ndze tɕe lú-wɣ-sɯ-qioʁ tɕe tɕe,</FORM>
	</S>
	<S id="S006">
		<AUDIO start="19.62" end="22.53"/>
		<FORM kindOf="phono">ɯ-kɯ-mŋɤm nɯ lu-nɯ-ɬoʁ ɲɯ-ŋu tu-ti-nɯ ŋgrɤl.</FORM>
	</S>
