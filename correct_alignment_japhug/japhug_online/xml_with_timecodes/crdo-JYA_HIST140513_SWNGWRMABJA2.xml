	<S id="S001">
		<AUDIO start="0.21" end="3.35"/>
		<FORM kindOf="phono">iɕqha sɯŋgɯrmɤβja nɯ, nɤkinɯ,</FORM>
	</S>
	<S id="S002">
		<AUDIO start="3.35" end="6.67"/>
		<FORM kindOf="phono">dɯxpa ma kɯkɯtɕu kɤntɕhaʁ jo-ɣi tɕe tɕendɤre </FORM>
	</S>
	<S id="S003">
		<AUDIO start="6.67" end="10.36"/>
		<FORM kindOf="phono">ɲɯ-mu tɕe nɤkinɯ, iɕqha nɯ,</FORM>
	</S>
	<S id="S004">
		<AUDIO start="10.36" end="11.46"/>
		<FORM kindOf="phono">a-me kɯ ko-ndo.</FORM>
	</S>
	<S id="S005">
		<AUDIO start="12.11" end="15.41"/>
		<FORM kindOf="phono">ko-ndo tɕe tɕe rca ɯ-tɯ-mu kɯ ɯ-mɲaʁ kɤ-cɯ koŋla mɯ́j-nɤz.</FORM>
	</S>
	<S id="S006">
		<AUDIO start="16.09" end="19.12"/>
		<FORM kindOf="phono">tɕendɤre ɯ-mɲaʁ nɯ ɲɯ-sɤre ma iʑora ji-mɲaʁ kɯ-fse</FORM>
	</S>
	<S id="S007">
		<AUDIO start="19.12" end="20.25"/>
		<FORM kindOf="phono">ku-sɤwij kɯ-fse nɯ, </FORM>
	</S>
	<S id="S008">
		<AUDIO start="20.62" end="22.47"/>
		<FORM kindOf="phono">koŋla ɯ-ndʐi kɯ tu-luj ɲɯ-maʁ</FORM>
	</S>
	<S id="S009">
		<AUDIO start="23.2" end="27.47"/>
		<FORM kindOf="phono">tɕe ɯ-rqhu kɯ-fse ci ɣɤʑu tɕe, ɯnɯnɯ kɯ ɯ-rdu nɯ tu-ɕɯfkaβ kɯ-fse ɲɯ-ŋu.</FORM>
	</S>
	<S id="S010">
		<AUDIO start="28.15" end="29.93"/>
		<FORM kindOf="phono">tɕe nɯnɯ ɯ-mɲaʁ ku-sɤwij</FORM>
	</S>
	<S id="S011">
		<AUDIO start="30.16" end="34.6"/>
		<FORM kindOf="phono">tɕe ta-cɯ qhe ɯ-rqhu nɯnɯ /ji jisɤ/ ju-sɤrtɕhoʁ kɯ-fse qhe </FORM>
	</S>
	<S id="S012">
		<AUDIO start="34.6" end="36.92"/>
		<FORM kindOf="phono">tɕe ɯ-mɲaʁ nɯ, ɯ-mɲaʁrdu nɯ tu-saχsɤl ɲɯ-ŋu.</FORM>
	</S>
	<S id="S013">
		<AUDIO start="37.66" end="41.69"/>
		<FORM kindOf="phono">nɯ sɯŋgɯrmɤβja nɯ dɯxpa ma tɕe tɕe ɯ-tɯ-mu ɲɯ-sɤre ʑo qhe,</FORM>
	</S>
	<S id="S014">
		<AUDIO start="41.69" end="44.3"/>
		<FORM kindOf="phono">ɯ-mɲaʁ ci ci ɲɯ-ci, ci ci ku-sɤwij ɲɯ-ŋu ma</FORM>
	</S>
	<S id="S015">
		<AUDIO start="44.56" end="47.25"/>
		<FORM kindOf="phono">koŋla ɯ-mɲaʁ kɤ-nɤrɯra mɯ́j-nɤz matɕi </FORM>
	</S>
	<S id="S016">
		<AUDIO start="47.36" end="48.95"/>
		<FORM kindOf="phono">"tha ɣɯ-sat-a-nɯ" ɲɯ-sɯsɤm.</FORM>
	</S>
	<S id="S017">
		<AUDIO start="49.23" end="52.78"/>
