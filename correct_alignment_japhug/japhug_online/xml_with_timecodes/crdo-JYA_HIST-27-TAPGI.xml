	<S id="S001">
		<AUDIO start="0.39" end="3.67"/>
		<FORM kindOf="phono">tɕe nɤki, tɯ-mɲaʁ ɣɯ ɯ-ŋgo tɕe tɤpɣi kɤ-ti ci tu</FORM>
	</S>
