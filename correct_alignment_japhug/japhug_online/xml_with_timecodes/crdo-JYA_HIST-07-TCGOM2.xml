	<S id="S001">
		<AUDIO start="0.04" end="4.78"/>
		<FORM kindOf="phono">tɕɣom nɯ ɯ-dɯχɯn wuma ʑo mɯm tɕe, nɤkinɯ, iɕqha nɯ,</FORM>
	</S>
	<S id="S002">
		<AUDIO start="5.52" end="7.45"/>
		<FORM kindOf="phono">ri ɣɤzɯβzɯβ.</FORM>
	</S>
	<S id="S003">
		<AUDIO start="8.21" end="11.17"/>
		<FORM kindOf="phono">tɕe nɯ, tsuku kɯ kɯ-mɤrtsaβ tu-ti-nɯ ŋu,</FORM>
	</S>
	<S id="S004">
		<AUDIO start="11.17" end="13.0"/>
		<FORM kindOf="phono">tsuku kɯ mɯ́j-mɤrtsaβ tu-ti-nɯ ŋu ri,</FORM>
	</S>
	<S id="S005">
		<AUDIO start="13.45" end="17.05"/>
		<FORM kindOf="phono">kɯ-mɤrtsaβ ʑo sthɯci me ri, tɕe tɯ-mdʑu ɯ-taʁ ɲɯ-sɤzɯβzɯβ qhe,</FORM>
	</S>
	<S id="S006">
		<AUDIO start="17.51" end="19.76"/>
		<FORM kindOf="phono">tɯ-kɯr ɯ-ŋgɯ ʑo xoŋnɤxoŋ ʑo ɲɯ-ste ŋu ma </FORM>
	</S>
	<S id="S007">
		<AUDIO start="20.28" end="24.42"/>
		<FORM kindOf="phono">ɯnɯnɯ tɕɣom ŋu tɕeri, tsuku kɯ-rga tu, tsuku mɤ-kɯ-rga tu.</FORM>
	</S>
	<S id="S008">
		<AUDIO start="25.34" end="26.94"/>
		<FORM kindOf="phono">aʑo ndɤre rga-a.</FORM>
	</S>
	<S id="S009">
		<AUDIO start="28.11" end="33.55"/>
		<FORM kindOf="phono">ɯ-dɯχɯn wuma ʑo mɯm.</FORM>
	</S>
