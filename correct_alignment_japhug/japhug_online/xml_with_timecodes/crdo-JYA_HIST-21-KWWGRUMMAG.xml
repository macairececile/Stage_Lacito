	<S id="S001">
		<AUDIO start="0.37" end="3.57"/>
		<FORM kindOf="phono">﻿kuɣrummɤɣ kɤ-ti ci tu tɕe, nɯnɯ ndɤre wuma ʑo mɯm</FORM>
	</S>
	<S id="S002">
		<AUDIO start="4.19" end="8.17"/>
		<FORM kindOf="phono">tɕe nɯ ɯ-βri ɯ-taʁ nɯra qandʐi kɯ-fse, aɣrɤɣrum kɯ-fse </FORM>
	</S>
	<S id="S003">
		<AUDIO start="8.77" end="10.47"/>
		<FORM kindOf="phono">ma kɯ-wɣrɯ-wɣrum ʑo sɯŋsɯŋ maʁ. (ideoph)</FORM>
	</S>
	<S id="S004">
		<AUDIO start="11.24" end="13.57"/>
		<FORM kindOf="phono">ɯkɯki, ɯkɯki nɯ ɯ-ŋga ki ɯ-mdoʁ asɯ-ndo</FORM>
	</S>
	<S id="S005">
		<AUDIO start="13.92" end="15.12"/>
		<FORM kindOf="phono">kuɣrummɤɣ tu-ti-nɯ</FORM>
	</S>
	<S id="S006">
		<AUDIO start="15.7" end="17.88"/>
		<FORM kindOf="phono">kuɣrummɤɣ tu-kɯ-ti nɯ, pjɯ-wɣrum ɲɯ-ra ri,</FORM>
	</S>
	<S id="S007">
		<AUDIO start="18.33" end="20.67"/>
		<FORM kindOf="phono">kɯ-wɣrɯ-wɣrum ʑo tu ri, kɯki kɯ-fse nɯ dɤn. (kɯ-ɤɣrɤɣrumkɯ-fse nɯ dɤn)</FORM>
	</S>
	<S id="S008">
		<AUDIO start="21.67" end="24.59"/>
		<FORM kindOf="phono">tɕe mɤ-naχtɕɯɣ ri ʁnaʁna kuɣrummɤɣ ŋu.</FORM>
	</S>
	<S id="S009">
		<AUDIO start="25.27" end="28.46"/>
		<FORM kindOf="phono">kuɣrummɤɣ rca, tɯ-khɤl ʑo, tu-ormbɯrmbɯ ʑo tu-ɬoʁ ŋu </FORM>
	</S>
	<S id="S010">
		<AUDIO start="29.42" end="32.15"/>
		<FORM kindOf="phono">tɕe nɯ tsuku tɕe tɯ-rdoʁ tɯ-rdoʁ ma kɯ-me tu ri,</FORM>
	</S>
	<S id="S011">
		<AUDIO start="32.15" end="34.83"/>
		<FORM kindOf="phono">tsuku tɕe nɯ kɯ-fse tu-ormbɯ-rmbɯ ʑo tu-ɬoʁ ŋu.</FORM>
	</S>
	<S id="S012">
		<AUDIO start="35.58" end="37.77"/>
		<FORM kindOf="phono">tɕe nɯnɯ kuɣrummɤɣ nɯ wuma ʑo mɯm.</FORM>
	</S>
	<S id="S013">
		<AUDIO start="38.39" end="40.55"/>
		<FORM kindOf="phono">tɕe kɯ-sɤndɤɣ tɕi me, wuma ʑo mɯm.</FORM>
	</S>
	<S id="S014">
		<AUDIO start="41.19" end="44.8"/>
		<FORM kindOf="phono">tɕe nɯnɯ ɯ-tshɯɣa nɯra, ɯ-fkaβ chondɤre ɯ-ru nɯnɯra </FORM>
	</S>
	<S id="S015">
		<AUDIO start="45.23" end="47.72"/>
		<FORM kindOf="phono">li kɯmaʁ tɤjmɤɣ ra fse qhe, ɯ-rʑɯɣ ra tu.</FORM>
	</S>
	<S id="S016">
		<AUDIO start="48.35" end="49.85"/>
		<FORM kindOf="phono">tɕeri tu-ormbɯrmbɯ ʑo tu-ɬoʁ ŋu,</FORM>
	</S>
	<S id="S017">
		<AUDIO start="49.85" end="51.55"/>
		<FORM kindOf="phono">ɕkrɤz ɯ-ŋgɯ tu-ɬoʁ ŋu.</FORM>
	</S>
	<S id="S018">
		<AUDIO start="52.33" end="54.5"/>
		<FORM kindOf="phono">tɕe kupa kɯ qinggangjun tu-ti ŋu.</FORM>
	</S>
	<S id="S019">
		<AUDIO start="55.12" end="58.11"/>
		<FORM kindOf="phono">ma ɕkrɤz ɯ-ŋgɯ ʁɟa ʑo tu-ɬoʁ ŋu tɕe,</FORM>
	</S>
	<S id="S020">
		<AUDIO start="58.11" end="66.72"/>
		<FORM kindOf="phono">nɯnɯ tɤjmɤɣ nɯ grɯβgrɯβ staʁ ʑo mɯm.</FORM>
	</S>
	<S id="S021">
		<AUDIO start="67.23" end="70.91"/>
		<FORM kindOf="phono">ma kuɣrummɤɣ ŋu</FORM>
	</S>
	<S id="S022">
		<AUDIO start="70.91" end="76.67"/>
		<FORM kindOf="phono">kuɣrummɤɣ nɯ kɯnɤ wuma ʑo arɤkhɯmkhɤl.</FORM>
	</S>
	<NOTE message="===================" xmlns:lang="fr"/>
	<S id="S023">
		<AUDIO start="77.21" end="79.04"/>
		<FORM kindOf="phono">ʑmbɯlɯm kɤ-ti ci tu tɕe, nɯnɯ </FORM>
	</S>
	<S id="S024">
		<AUDIO start="79.55" end="83.37"/>
		<FORM kindOf="phono">ʑmbɯlɯm nɯnɯ, kɯki ɯ-fkaβ nɯra nɤmbju ʑo qhe,</FORM>
	</S>
	<S id="S025">
		<AUDIO start="83.37" end="85.84"/>
		<FORM kindOf="phono">ɲɯ́-wɣ-nɤmɤle tɯ-jaʁ ku-ndɤm ʑo ɕti ma,</FORM>
	</S>
	<S id="S026">
		<AUDIO start="86.25" end="88.22"/>
		<FORM kindOf="phono">ɯ-taʁ, ɯ-ndʑɯɣ ʑo kɯ-fse tu.</FORM>
	</S>
	<S id="S027">
		<AUDIO start="89.04" end="91.66"/>
		<FORM kindOf="phono">ma nɯnɯ ʑmbɯlɯm nɯ, tɤ-ɬoʁ ɕɯmɯma tɕe,</FORM>
	</S>
	<S id="S028">
		<AUDIO start="92.05" end="93.42"/>
		<FORM kindOf="phono">χploʁχploʁ ʑo kɯ-pa ŋu. (ideoph)</FORM>
	</S>
	<S id="S029">
		<AUDIO start="93.83" end="95.8"/>
		<FORM kindOf="phono">tɕe nɯ ɯ-ru nɯ kɯ-wɣrɯ-wɣrum ʑo ŋu.</FORM>
	</S>
	<S id="S030">
		<AUDIO start="96.28" end="100.36"/>
		<FORM kindOf="phono">tɕe ɯ-χpoʁ ɣɯ ɯ-pa pɕoʁ nɯ li kɯ-wɣrɯ-wɣrum ʑo ŋu, </FORM>
	</S>
	<S id="S031">
		<AUDIO start="100.36" end="101.55"/>
		<FORM kindOf="phono">ɯ-ru nɯ kɯ-wɣrɯ-wɣrum ʑo ŋu.</FORM>
	</S>
	<S id="S032">
		<AUDIO start="102.07" end="105.72"/>
		<FORM kindOf="phono">tɕe nɯnɯ ʑmbɯlɯm nɯnɯ kɯ-ɤrɤkhɯkhɤl tu,</FORM>
	</S>
	<S id="S033">
		<AUDIO start="105.72" end="107.31"/>
		<FORM kindOf="phono">tɯ-rdoʁ tɯ-rdoʁ tu-kɯ-ɬoʁ tu.</FORM>
	</S>
	<S id="S034">
		<AUDIO start="107.97" end="111.16"/>
		<FORM kindOf="phono">tɕe nɯnɯ mbraj cho sɤjku ɯ-ŋgɯ ri tu-ɬoʁ ŋu.</FORM>
	</S>
	<S id="S035">
		<AUDIO start="111.69" end="113.32"/>
		<FORM kindOf="phono">sɯŋgɯ tu-ɬoʁ ɕti tɕe, nɯnɯ </FORM>
	</S>
	<S id="S036">
		<AUDIO start="113.69" end="118.41"/>
		<FORM kindOf="phono">mbraj kɯ-dɤn chondɤre sɤjku kɯ-dɤn ɯ-ŋgɯ ʁɟa ʑo ʑmbɯlɯm nɯ tu-ɬoʁ ŋu</FORM>
	</S>
	<S id="S037">
		<AUDIO start="119.17" end="120.69"/>
		<FORM kindOf="phono">ɕkrɤz ɯ-ŋgɯ kɯnɤ tu-kɯ-ɬoʁ tu.</FORM>
	</S>
	<S id="S038">
		<AUDIO start="121.23" end="122.68"/>
		<FORM kindOf="phono">ri ɕkrɤz ɯ-ŋgɯ nɯ mɤ-dɤn.</FORM>
	</S>
	<S id="S039">
		<AUDIO start="123.28" end="128.77"/>
		<FORM kindOf="phono">nɤki, mbraj cho sɤjku ɯ-si nɯ ɣɯ ɯ-ŋgɯ nɯ tɕu, wuma ʑo dɤn.</FORM>
	</S>
	<S id="S040">
		<AUDIO start="129.6" end="132.53"/>
		<FORM kindOf="phono">tɕe nɯnɯ kɯ-ɤrɤkhɯmkhɤl tu, mɤ-kɯ-ɤrɤkhɯmkhɤl tu.</FORM>
	</S>
	<S id="S041">
		<AUDIO start="133.48" end="135.48"/>
		<FORM kindOf="phono">tɕe nɯnɯ ʑmbɯlɯm ŋu.</FORM>
	</S>
	<S id="S042">
		<AUDIO start="135.9" end="137.44"/>
		<FORM kindOf="phono">tɕe tú-wɣ-ndza tɕe qiaβ.</FORM>
	</S>
	<S id="S043">
		<AUDIO start="138.27" end="140.57"/>
		<FORM kindOf="phono">tɕe kupa kɯ youlaku tu-ti ŋu.</FORM>
	</S>
	<S id="S044">
		<AUDIO start="141.44" end="145.41"/>
		<FORM kindOf="phono">ma nɯnɯ, nɤkinɯ, ɯ-rmi li tu ma iɕqha nɯ,</FORM>
	</S>
	<S id="S045">
		<AUDIO start="145.66" end="150.0"/>
		<FORM kindOf="phono">aʁɤndɯndɤt ʑo tu ɕti.</FORM>
	</S>
	<S id="S046">
		<AUDIO start="150.84" end="154.5"/>
		<FORM kindOf="phono">tɕe tham tɕe, kɯki, tangniaobing ɲɯ-phɤn tu-ti-nɯ ŋu tɕe,</FORM>
	</S>
	<S id="S047">
		<AUDIO start="154.96" end="158.02"/>
		<FORM kindOf="phono">tangniaobing nɯ-kɯ-mŋɤm kɯ-tu ra kɯ rca, </FORM>
	</S>
	<S id="S048">
		<AUDIO start="158.02" end="160.32"/>
		<FORM kindOf="phono">kɯ-dɯ-dɤn tɯ-kuxtɕo jamar tu-nɯ-χtu-nɯ tɕe,</FORM>
	</S>
	<S id="S049">
		<AUDIO start="160.73" end="162.02"/>
		<FORM kindOf="phono">tɕe ɲɯ-sɯɣrom-nɯ ɲɯ-ŋu.</FORM>
	</S>
	<S id="S050">
		<AUDIO start="162.71" end="164.37"/>
		<FORM kindOf="phono">tɕe ɲɯ-sɯɣrom-nɯ tɕe tɕe,</FORM>
	</S>
	<S id="S051">
		<AUDIO start="164.37" end="167.25"/>
		<FORM kindOf="phono">na-sɯɣrom-nɯ tɕe tɕendɤre, nɤkinɯ,</FORM>
	</S>
	<S id="S052">
		<AUDIO start="167.25" end="169.15"/>
		<FORM kindOf="phono">ku-sqa-nɯ tɕe ɯ-ci ku-tshi-nɯ ɲɯ-ŋu.</FORM>
	</S>
	<S id="S053">
		<AUDIO start="169.59" end="171.79"/>
		<FORM kindOf="phono">tɕe nɤki, ɯʑo kɯnɤ tu-ndza-nɯ ɲɯ-ŋu.</FORM>
	</S>
	<S id="S054">
		<AUDIO start="172.71" end="175.1"/>
		<FORM kindOf="phono">tɕe ɯʑo kɯnɤ tú-wɣ-nɯ-xsɯr tɕe ɲɯ-sna</FORM>
	</S>
	<S id="S055">
		<AUDIO start="175.1" end="176.89"/>
		<FORM kindOf="phono">liangban tú-wɣ-nɯ-βzu tɕe ɲɯ-mɯm.</FORM>
	</S>
	<S id="S056">
		<AUDIO start="178.07" end="179.57"/>
		<FORM kindOf="phono">tɕe nɯnɯ mɤ-sɤndɤɣ.</FORM>
	</S>
	<S id="S057">
		<AUDIO start="180.07" end="183.01"/>
		<FORM kindOf="phono">tɕe youlaku tu-ti-nɯ ŋu tɕe, nɯnɯ ŋu.</FORM>
	</S>
	<S id="S058">
		<AUDIO start="183.84" end="186.55"/>
		<FORM kindOf="phono">tɕe nɯnɯ ʑmbɯlɯm ŋu.</FORM>
	</S>
	<S id="S059">
		<AUDIO start="186.78" end="187.89"/>
		<FORM kindOf="phono">iʑora i-sɤtɕha tu.</FORM>
	</S>
	<S id="S060">
		<AUDIO start="189.43" end="191.37"/>
		<FORM kindOf="phono">aʑo pɯ-xtɕi-a ɕɯ-kɤ-ɕar pɯ-rɲo-t-a.</FORM>
	</S>
	<S id="S061">
		<AUDIO start="192.07" end="194.49"/>
		<FORM kindOf="phono">tham tɕe mɯ́j-cha-a, a-mi ɲɯ-mŋɤm.</FORM>
	</S>
	<S id="S062">
		<AUDIO start="194.87" end="199.07"/>
		<FORM kindOf="phono">tɕe ŋotɕu kɯ-tu nɯra sɯz-a laʁma, ju-ɕe-a mɯ́j-cha-a.</FORM>
	</S>
	<S id="S063">
		<AUDIO start="201.68" end="202.64"/>
		<FORM kindOf="phono">nɯnɯ ʑmbɯlɯm ŋu.</FORM>
	</S>
